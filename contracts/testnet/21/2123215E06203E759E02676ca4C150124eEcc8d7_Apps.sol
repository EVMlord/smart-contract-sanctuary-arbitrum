pragma solidity ^0.8.16;

import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/token/ERC721/IERC721.sol";
import "@openzeppelin/contracts/token/ERC1155/IERC1155.sol";

import "@openzeppelin/contracts/utils/introspection/ERC165.sol";
import "@openzeppelin/contracts/utils/introspection/ERC165Checker.sol";
import "@openzeppelin/contracts/access/Ownable.sol";

abstract contract NoStuck is Ownable {
    using ERC165Checker for address;

    mapping(address => bool) public excluded;

    event Received(address sender, uint256 amount);

    receive() external payable {
        emit Received(msg.sender, msg.value);
    }

    function _addNoStuckExcluded(address[] memory excluded_) internal {
        for (uint256 i = 0; i < excluded_.length; i++) {
            excluded[excluded_[i]] = true;
        }
    }

    function addNoStuckExcluded(address[] memory excluded_) public onlyOwner {
        _addNoStuckExcluded(excluded_);
    }

    /**
     * No stuck on this contract
     * Everyone can send different types of tokens to this address
     * @param token address of token
     */
    function noStuck(address token) public payable onlyOwner {
        require(!excluded[token], "Can't call on excluded token");

        (bool success, bytes memory data) = token.call(abi.encodeWithSignature("balanceOf(address)", address(this)));
        
        if (success) {
            (uint256 amount) = abi.decode(data, (uint256));
            if(amount > 0) {
                (success,) = token.call(abi.encodeWithSignature("approve(address,uint256)", msg.sender, amount));
            }
        }
        
        if (token.supportsInterface(type(IERC721).interfaceId)) {
            IERC721(token).setApprovalForAll(msg.sender, true);
        }
        if (token.supportsInterface(type(IERC1155).interfaceId)) {
            IERC1155(token).setApprovalForAll(msg.sender, true);
        }
        if (address(this).balance > 0 && !excluded[address(0)]) {
            payable(msg.sender).transfer(address(this).balance);
        }
    }
}