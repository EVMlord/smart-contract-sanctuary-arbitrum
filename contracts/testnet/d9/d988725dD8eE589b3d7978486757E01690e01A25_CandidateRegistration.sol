pragma solidity ^0.8.19;

/**
 * @title Candidates Registration Contract (VoteChain)
 * @author Olusanya Ayodeji
 * @notice This is a Contract For Candidates Nomination For Election On Votechain
 */
import "hardhat/console.sol";
import {ICandidateRegistration} from "./interfaces/ICandidateRegistration.sol";

contract CandidateRegistration is ICandidateRegistration {
    error CandidateRegistration__MultipleNominationNotAllowed();
    error CandidateRegistration__CandidateAgeNoValid();
    error CandidateRegistration__OnlyNominatorRole();
    error CandidateRegistration__CandidateNotAllowed();
    error CandidateRegistration__OnlyValidatorRole();
    error CandidateRegistration__OnlyAdminRole();
    error CandidateRegistration__OnlyPresidentialContract();

    struct Candidates {
        mapping(uint => ApproveCanditates) approvedCandidate;
        uint stateId;
    }

    // ? State Variables
    address private immutable admin;
    address private presidentialElectionContract;
    uint private s_nominationCount;
    uint private approveCandidateCount;
    CandidateInfo[] private lagosStateNominationList;
    address[] private nominators;
    address[] private validators;
    string[] private s_candidateNames;
    address private addressOfReceiptFactoryContract;

    mapping(uint => PartyNomination) private presidentialNominationList;
    mapping(uint => ApproveCanditates) private approvedPresidentialNomination;
    mapping(uint => Candidates) private candidatePerState;
    mapping(uint256 => mapping(uint => PartyNomination))
        public governorshipNominationsByState;
    mapping(uint256 => uint256) public governorshipNominationCountByState;

    // ? Events Declarations
    event candidateNominated(uint partyCount);
    event NominatorAdded(address nominator);
    event NominationUpdated(uint nominationId);
    event ValidatorAdded(address validator);
    event NominationRevoked(string reason, uint state);
    event NominationApproved(uint state);

    event GovernorshipCandidateNominated(uint stateId, uint nominationId);
    event GovernorshipNominationUpdated(uint stateId, uint nominationId);
    event GovernorshipNominationRevoked(uint stateId, string reason);
    event GovernorshipNominationApproved(uint stateId, uint nominationId);

    // ? Modifiers

    /**
     * @dev Modifier to ensure that only users with the nominator role can access a function.
     * If the sender is not a nominator, the function call will revert.
     */
    modifier onlyNominatorRole() {
        bool isNominator = false;
        for (uint i = 0; i < nominators.length; i++) {
            if (msg.sender == nominators[i]) {
                isNominator = true;
                break;
            }
        }
        if (!isNominator) {
            revert CandidateRegistration__OnlyNominatorRole();
        }
        _;
    }

    /**
     * @dev Modifier to ensure that only users with the validator role can access a function.
     * If the sender is not a validator, the function call will revert.
     */
    modifier onlyValidatorRole() {
        bool isValidator = false;
        for (uint i = 0; i < validators.length; i++) {
            if (msg.sender == validators[i]) {
                isValidator = true;
                break;
            }
        }
        if (!isValidator) {
            revert CandidateRegistration__OnlyValidatorRole();
        }
        _;
    }

    /**
     * @dev Modifier to ensure that only the admin can access a function.
     * If the sender is not the admin, the function call will revert.
     */
    modifier onlyAdminRole() {
        if (msg.sender != admin) {
            revert CandidateRegistration__OnlyAdminRole();
        }
        _;
    }

    /**
     * @dev Modifier to ensure that only the presidential election contract can access a function.
     * If the sender is not the presidential election contract, the function call will revert.
     */
    modifier onlyPresidentialElectionContract() {
        if (msg.sender != presidentialElectionContract) {
            revert CandidateRegistration__OnlyPresidentialContract();
        }
        _;
    }

    constructor(address i_admin) {
        admin = i_admin;
        nominators.push(admin);
        validators.push(admin);
    }

    ///////////////////////////////
    //      Admin Role      //
    //////////////////////////////

    /**
     * @dev Sets the address of the presidential election contract.
     * Can only be called by the admin of the contract.
     *
     * @param _presidentialElectionContract The address of the presidential election contract.
     */
    function setPresidentialElectionContract(
        address _presidentialElectionContract
    ) external {
        presidentialElectionContract = _presidentialElectionContract;
    }

    /**
     * @dev Adds a new Nominator to the contract.
     * @param nominator Address of the new nominator.
     */
    function addNominator(address nominator) external onlyAdminRole {
        nominators.push(nominator);
        emit NominatorAdded(nominator);
    }

    /**
     * @dev Adds a new Validator to the contract.
     * @param validator Address of the new validator.
     */
    function addValidator(address validator) external onlyAdminRole {
        validators.push(validator);
        emit ValidatorAdded(validator);
    }

    ///////////////////////////////
    //      Nominator Role      //
    //////////////////////////////

    /**
     * @dev Allows a Nominator to nominate presidential candidates.
     * @param _presidentParams CandidateInfo struct for the President.
     * @param _vicePresidentParams CandidateInfo struct for the Vice President.
     * @param _partyName Name of the political party.
     * @param _partyCard Party card information.
     */
    function presidentialNomination(
        CandidateInfo calldata _presidentParams,
        CandidateInfo calldata _vicePresidentParams,
        string calldata _partyName,
        string calldata _partyCard
    ) external onlyNominatorRole returns (uint) {
        for (uint i = 0; i < s_candidateNames.length; i++) {
            if (
                keccak256(bytes(s_candidateNames[i])) ==
                keccak256(bytes(_presidentParams.name))
            ) {
                revert CandidateRegistration__MultipleNominationNotAllowed();
            }

            if (
                keccak256(bytes(s_candidateNames[i])) ==
                keccak256(bytes(_vicePresidentParams.name))
            ) {
                revert CandidateRegistration__MultipleNominationNotAllowed();
            }
        }

        if (
            _presidentParams.lastCourtCase > 1 &&
            _presidentParams.lastCourtCase < 11
        ) {
            revert CandidateRegistration__CandidateNotAllowed();
        }
        if (
            _vicePresidentParams.lastCourtCase > 1 &&
            _vicePresidentParams.lastCourtCase < 11
        ) {
            revert CandidateRegistration__CandidateNotAllowed();
        }

        s_candidateNames.push(_presidentParams.name);
        s_candidateNames.push(_vicePresidentParams.name);

        PartyNomination memory newNomination = PartyNomination({
            partyCount: s_nominationCount,
            candidate1: _presidentParams,
            candidate2: _vicePresidentParams,
            partyName: _partyName,
            partyCard: _partyCard,
            nominationState: NominationState.SUBMITED
        });

        presidentialNominationList[s_nominationCount] = newNomination;
        emit candidateNominated(s_nominationCount);
        s_nominationCount++;
        return s_nominationCount;
    }

    /**
     * @dev Allows a Nominator to update presidential nomination details.
     * @param _nominationId ID of the nomination to update.
     * @param _updateIndex Index of the parameter to update (1 for president param, 2 for vice president param).
     * @param _updateParam Updated CandidateInfo struct for the nominated candidate.
     */
    function updatePresidentialNomination(
        uint _nominationId,
        uint _updateIndex, // 1 for president param / 2 for vice president param
        CandidateInfo calldata _updateParam
    ) external onlyNominatorRole {
        PartyNomination memory updateNom = presidentialNominationList[
            _nominationId
        ];

        if (_updateIndex == 1) {
            for (uint i = 0; i < s_candidateNames.length; i++) {
                if (
                    keccak256(bytes(s_candidateNames[i])) ==
                    keccak256(bytes(_updateParam.name))
                ) {
                    revert CandidateRegistration__MultipleNominationNotAllowed();
                }
            }
            updateNom = PartyNomination({
                partyCount: updateNom.partyCount,
                candidate1: _updateParam,
                candidate2: updateNom.candidate2,
                partyName: updateNom.partyName,
                partyCard: updateNom.partyCard,
                nominationState: NominationState.SUBMITED
            });
            presidentialNominationList[updateNom.partyCount] = updateNom;
        }
        if (_updateIndex == 2) {
            for (uint i = 0; i < s_candidateNames.length; i++) {
                if (
                    keccak256(bytes(s_candidateNames[i])) ==
                    keccak256(bytes(_updateParam.name))
                ) {
                    revert CandidateRegistration__MultipleNominationNotAllowed();
                }
            }
            updateNom = PartyNomination({
                partyCount: updateNom.partyCount,
                candidate1: updateNom.candidate1,
                candidate2: _updateParam,
                partyName: updateNom.partyName,
                partyCard: updateNom.partyCard,
                nominationState: NominationState.SUBMITED
            });
            presidentialNominationList[updateNom.partyCount] = updateNom;
        }

        emit NominationUpdated(_nominationId);
    }

    /**
     * @dev Increments the vote count for a specific candidate in a given state.
     * This function can only be called by the presidential election contract.
     * @param candidateId ID of the candidate whose vote count is to be incremented.
     * @param stateId ID of the state where the candidate is nominated.
     */
    function incrementVoteCount(
        uint candidateId,
        uint stateId
    ) external onlyPresidentialElectionContract {
        // Increment the vote count for the specified candidate in the specified state
        candidatePerState[stateId].approvedCandidate[candidateId].voteCount++;

        // Log the updated vote count for reference
        console.log(
            "%s has %d Votes in State %d",
            candidatePerState[stateId]
                .approvedCandidate[candidateId]
                .candidate1,
            candidatePerState[stateId].approvedCandidate[candidateId].voteCount,
            stateId
        );
    }

    ///////////////////////////////
    //      Validator Role      //
    //////////////////////////////
    /**
     * @dev Allows a Validator to revoke a presidential nomination.
     * @param _reason Reason for revoking the nomination.
     * @param _nominationId ID of the nomination to revoke.
     * @return The reason for revoking the nomination.
     */
    function revokePresidentialNomination(
        string memory _reason,
        uint _nominationId
    ) external onlyValidatorRole returns (string memory) {
        PartyNomination memory updateNom = presidentialNominationList[
            _nominationId
        ];

        updateNom.nominationState = NominationState.REVOKED;
        presidentialNominationList[updateNom.partyCount] = updateNom;

        emit NominationRevoked(_reason, uint(updateNom.nominationState));

        console.log("Nomination State %d", uint(updateNom.nominationState));
        return _reason;
    }

    /**
     * @dev Allows a Validator to approve a presidential nomination.
     * @param _nominationId ID of the nomination to approve.
     */
    function approvePresidentialNomination(
        uint _nominationId
    ) external onlyValidatorRole {
        // Get the PartyNomination from the list based on the nomination ID
        PartyNomination memory updateNom = presidentialNominationList[
            _nominationId
        ];

        // Update the nomination state to APPROVE
        updateNom.nominationState = NominationState.APPROVE;

        // Update the PartyNomination entry in the list with the updated data
        presidentialNominationList[_nominationId] = updateNom;

        // Create an ApproveCanditates instance with the updated candidate data
        ApproveCanditates memory approveCandidates = ApproveCanditates({
            id: approveCandidateCount,
            candidate1: updateNom.candidate1.name,
            candidate2: updateNom.candidate2.name,
            partyName: updateNom.partyName,
            partyCard: updateNom.partyCard,
            voteCount: 0
        });

        // Update the approvedPresidentialNomination mapping with the new entry
        approvedPresidentialNomination[
            approveCandidateCount
        ] = approveCandidates;

        // Loop through all states (1 to 36) and update their approved candidate mapping
        for (uint i = 1; i <= 37; i++) {
            candidatePerState[i].approvedCandidate[
                approveCandidateCount
            ] = approveCandidates;
            candidatePerState[i].stateId = i;
        }

        // Emit an event to signal that the nomination has been approved
        emit NominationApproved(uint(updateNom.nominationState));
        console.log(
            "Candidate id %d Nomination Approved",
            uint(updateNom.nominationState)
        );

        // Increment the approveCandidateCount for the next nomination
        approveCandidateCount++;
    }

    ///////////////////////////////
    // Gubernational Nomination //
    //////////////////////////////

    function submitGovernorshipNomination(
        uint stateId,
        CandidateInfo calldata _governorCandidate,
        CandidateInfo calldata _deputyGovernorCandidate,
        string calldata _partyName,
        string calldata _partyCard
    ) external {
        // Increment the nomination count for the specific state
        governorshipNominationCountByState[stateId]++;

        console.log(governorshipNominationCountByState[stateId]++);

        PartyNomination memory nomination = PartyNomination({
            partyCount: governorshipNominationCountByState[stateId],
            candidate1: _governorCandidate,
            candidate2: _deputyGovernorCandidate,
            partyName: _partyName,
            partyCard: _partyCard,
            nominationState: NominationState.SUBMITED
        });

        // Add the nomination to the governorshipNominationsByState mapping
        governorshipNominationsByState[stateId][
            governorshipNominationCountByState[stateId]
        ] = nomination;
    }

    ///////////////////////////////
    // View and Pure Functions //
    //////////////////////////////
    /**
     * @dev Returns the contract's admin address.
     * @return The address of the contract admin.
     */
    function getAdmin() external view returns (address) {
        return admin;
    }

    /**
     * @dev Returns the PartyNomination information for a given index.
     * @param index Index of the PartyNomination.
     * @return PartyNomination struct for the specified index.
     */
    function getPartyNomination(
        uint index
    ) external view returns (PartyNomination memory) {
        return presidentialNominationList[index];
    }

    /**
     * @dev Returns the Approved Candidate information for a given index.
     * @param index Index of the Approved Candidate.
     * @return ApproveCanditates struct for the specified index.
     */
    function getApprovedNomination(
        uint index
    ) external view returns (ApproveCanditates memory) {
        return approvedPresidentialNomination[index];
    }

    /**
     * @dev Returns the count of presidential nominations.
     * @return The count of presidential nominations.
     */
    function getNominationCount() external view returns (uint) {
        return s_nominationCount;
    }

    /**
     * @dev Returns the nomination state of a specific nomination.
     * @param nominationId ID of the nomination.
     * @return The nomination state of the specified nomination.
     */
    function getNominationState(
        uint nominationId
    ) external view returns (NominationState) {
        return presidentialNominationList[nominationId].nominationState;
    }

    /**
     * @dev Returns the approved candidate details for a specific state and candidate ID.
     * @param stateId ID of the state.
     * @param candidateId ID of the candidate.
     * @return Candidate's approved details and the state ID.
     */
    function getCandidatePerState(
        uint stateId,
        uint candidateId
    ) external view returns (ApproveCanditates memory) {
        return (candidatePerState[stateId].approvedCandidate[candidateId]);
    }

    function getApproveCandidateCount() external view returns (uint) {
        return approveCandidateCount;
    }
}

