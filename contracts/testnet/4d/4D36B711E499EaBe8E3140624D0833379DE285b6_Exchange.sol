pragma solidity ^0.8.18;

import {Errors} from "../../common/Errors.sol";
import {OrderType, Order} from "../../interfaces/IExchange.sol";

contract PriceValidator {
    /// trigger price short <= indexPrice <= trigger price long
    function _validateCondition1(
        bool isLong,
        uint256 indexPrice,
        uint256 triggerPrice
    ) internal pure {
        if (isLong) {
            require(indexPrice <= triggerPrice, Errors.TRIGGER_PRICE_NOT_PASS);
        } else {
            require(indexPrice >= triggerPrice, Errors.TRIGGER_PRICE_NOT_PASS);
        }
    }

    // trigger price long  <= indexPrice <= trigger price short
    function _validateCondition2(
        bool isLong,
        uint256 indexPrice,
        uint256 triggerPrice
    ) internal pure {
        if (isLong) {
            require(indexPrice >= triggerPrice, Errors.TRIGGER_PRICE_NOT_PASS);
        } else {
            require(indexPrice <= triggerPrice, Errors.TRIGGER_PRICE_NOT_PASS);
        }
    }

    function _validateLimitExecution(
        bool open,
        bool isLong,
        uint256 indexPrice,
        uint256 triggerPrice
    ) internal pure {
        if (open) {
            _validateCondition1(isLong, indexPrice, triggerPrice);
        } else {
            _validateCondition2(isLong, indexPrice, triggerPrice);
        }
    }

    function _validateStopMarketExecution(
        bool open,
        bool isLong,
        uint256 indexPrice,
        uint256 triggerPrice
    ) internal pure {
        // top level order
        if (open) {
            _validateCondition2(isLong, indexPrice, triggerPrice);
        } else {
            // when this is a close order
            // stop market become a stop loss order
            _validateCondition1(isLong, indexPrice, triggerPrice);
        }
    }

    function _validateTpSlPrice(bool isLong, uint96 tpPrice, uint96 slPrice) internal pure {
        // only validate if both tpPrice and slPrice are greater than 0
        if (!(tpPrice > 0 && slPrice > 0)) {
            return;
        }

        if (isLong) {
            require(tpPrice > slPrice, Errors.INVALID_TP_SL_PRICE);
        } else {
            require(tpPrice < slPrice, Errors.INVALID_TP_SL_PRICE);
        }
    }

    function _validateTpSlExecution(
        bool isLong,
        uint256 indexPrice,
        uint96 tpPrice,
        uint96 slPrice
    ) internal pure {
        require(tpPrice > 0 || slPrice > 0, "Invalid tp/sl price");
        if (isLong) {
            require(
                (tpPrice > 0 && indexPrice >= tpPrice) || indexPrice <= slPrice,
                Errors.TRIGGER_PRICE_NOT_PASS
            );
        } else {
            require(
                indexPrice <= tpPrice || (slPrice > 0 && indexPrice >= slPrice),
                Errors.TRIGGER_PRICE_NOT_PASS
            );
        }
    }
}

