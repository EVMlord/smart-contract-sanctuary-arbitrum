pragma solidity 0.8.13;

import "@openzeppelin/contracts-upgradeable/security/PausableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/access/AccessControlUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "@openzeppelin/contracts/utils/math/Math.sol";
import "@openzeppelin/contracts/utils/cryptography/MerkleProof.sol";

import "./base/BaseCitiNFT.sol";
import "./base/Versioned.sol";


struct AuctionInfo {
    // inclusive
    uint256 startTimestamp;
    // inclusive
    uint256 endTimestamp;
    uint256 priceDropIntervalSeconds;
    uint256 dropPerInterval;
    
    uint256 totalSupply;
    uint256 startPrice;
    uint256 endPrice;
    uint256 allowance;

    bytes32 merkleRoot;
}

contract DutchAuction is Initializable, PausableUpgradeable, AccessControlUpgradeable, Versioned {
    
    using SafeMath for uint256;
    
    bytes32 public constant PAUSER_ROLE = keccak256("PAUSER_ROLE");
    uint256 private constant MAX_UINT = 2**256 - 1;
    
    BaseCitiNFT private _nftContract;
    
    // array of multiple stages of the auction
    AuctionInfo[] private _stages;

    // stage index => total minted
    mapping(uint256 => uint256) private _totalMinted;
    
    // stage index => (account => mints)
    mapping(uint256 => mapping(address => uint256)) private _mints;

    event StageAdded(uint256 indexed stage, AuctionInfo info);
    event NFTMinted(uint256 indexed stage, uint256 indexed tokenId, address indexed recipient, uint256 soldPrice, uint256 refund);
    event Withdraw(address indexed recipient, uint256 amount);

    /// @custom:oz-upgrades-unsafe-allow constructor
    constructor() initializer {}
    
    function initialize(BaseCitiNFT nftContract) initializer public {
        require(address(nftContract) != address(0), "Invalid NFT contract");
        __Pausable_init();
        __AccessControl_init();

        _grantRole(DEFAULT_ADMIN_ROLE, msg.sender);
        _grantRole(PAUSER_ROLE, msg.sender);

        _nftContract = nftContract;
    }

    function pause() public onlyRole(PAUSER_ROLE) {
        _pause();
    }

    function unpause() public onlyRole(PAUSER_ROLE) {
        _unpause();
    }

    function adminAddStage(AuctionInfo calldata info) public onlyRole(DEFAULT_ADMIN_ROLE) whenNotPaused {
        require(info.totalSupply > 0, "adminConfigureAuction: invalid supply");
        require(info.endTimestamp > info.startTimestamp, "adminConfigureAuction: invalid duration");
        require(info.startPrice > 0, "adminConfigureAuction: invalid start price");
        require(info.endPrice > 0, "adminConfigureAuction: invalid end price");
        require(info.startPrice >= info.endPrice, "adminConfigureAuction: end price higher than start price");
        require(info.priceDropIntervalSeconds > 0, "adminConfigureAuction: invalid drop interval");
        require(info.dropPerInterval > 0, "adminConfigureAuction: invalid drop price");
        require(info.startTimestamp < MAX_UINT, "adminConfigureAuction: invalid start time");

        if (_stages.length > 0) {
            require(info.startTimestamp > _stages[_stages.length - 1].endTimestamp, "adminConfigureAuction: stages overlap");
        }

        _stages.push(info);
        emit StageAdded(_stages.length - 1, info);
    }

    function currentStage(uint256 timestamp) public view returns (bool, uint256) {
        for (uint256 index = 0; index < _stages.length; ++index) {
            if (timestamp >= _stages[index].startTimestamp && timestamp <= _stages[index].endTimestamp) {
                return (true, index);
            }
        }
        return (false, MAX_UINT);
    }

    function withdraw(address payable recipient, uint256 amount) public onlyRole(DEFAULT_ADMIN_ROLE) whenNotPaused {
        require(recipient != address(0), "Invalid recipient");
        recipient.transfer(amount);
        emit Withdraw(recipient, amount);
    }

    function withdrawAll(address payable recipient) public onlyRole(DEFAULT_ADMIN_ROLE) whenNotPaused {
        withdraw(recipient, address(this).balance);
    }

    modifier validStage(uint256 stage) {
        require(stage < _stages.length, "invalid stage");
        _;
    }

    function getPrice(uint256 stage, uint256 timestamp) public view validStage(stage) returns (uint256) {
        if (timestamp <= _stages[stage].startTimestamp) {
            return _stages[stage].startPrice;
        } else {
            uint256 intervals = timestamp.sub(_stages[stage].startTimestamp).div(_stages[stage].priceDropIntervalSeconds);
            uint256 drop = intervals.mul(_stages[stage].dropPerInterval);
            if (_stages[stage].startPrice > drop) {
                return Math.max(_stages[stage].startPrice.sub(drop), _stages[stage].endPrice);
            } else {
                return _stages[stage].endPrice;
            }
        }
    }
    
    function mintNFT(uint256 stage, bytes32[] calldata whitelistProof) public payable 
        validStage(stage) 
        whenInStage(stage) 
        whenHasSupply(stage) 
        whenHasAllowance(stage) 
        whenNotPaused 
        returns (uint256) {
        bytes32 merkleRoot = _stages[stage].merkleRoot;
        if (merkleRoot != 0) {
            bytes32 leaf = keccak256(abi.encodePacked(msg.sender));
            require(MerkleProof.verify(whitelistProof, merkleRoot, leaf), "mintNFT: not in whitelist");
        }
        uint256 currentPrice = getPrice(stage, block.timestamp);
        require(msg.value >= currentPrice, "mintNFT: price is too low");
        _totalMinted[stage] += 1;
        if (_stages[stage].allowance > 0) {
            _mints[stage][msg.sender] += 1;
        }
        uint256 tokenId = _nftContract.safeMint(msg.sender, "");
        uint256 refund = msg.value.sub(currentPrice);
        if (refund > 0) {
            payable(msg.sender).transfer(refund);
        }
        emit NFTMinted(stage, tokenId, msg.sender, currentPrice, refund);
        return tokenId;
    }

    function configured() public view returns (bool) {
        return _stages.length != 0;
    }

    function totalMinted(uint256 stage) public view validStage(stage) returns (uint256) {
        return _totalMinted[stage];
    }

    function allowance(uint256 stage, address account) public view validStage(stage) returns (uint256) {
        if (_stages[stage].allowance > 0) {
            return _stages[stage].allowance.sub(_mints[stage][account]);
        } else {
            return MAX_UINT;
        }
    }

    function stageInfo(uint256 stage) public view validStage(stage) returns (AuctionInfo memory) {
        return _stages[stage];
    }

    function numStages() public view returns (uint256) {
        return _stages.length;
    }

    function stages() public view returns (AuctionInfo[] memory) {
        return _stages;
    }

    modifier whenInStage(uint256 stage) {
        require(block.timestamp >= _stages[stage].startTimestamp, "Auction not started");
        require(block.timestamp <= _stages[stage].endTimestamp, "Auction is over");
        _;
    }

    modifier whenHasSupply(uint256 stage) {
        require(_totalMinted[stage] < _stages[stage].totalSupply, "Sold out");
        _;
    }

    modifier whenHasAllowance(uint256 stage) {
        require(allowance(stage, msg.sender) > 0, "No allowance");
        _;
    }
}

