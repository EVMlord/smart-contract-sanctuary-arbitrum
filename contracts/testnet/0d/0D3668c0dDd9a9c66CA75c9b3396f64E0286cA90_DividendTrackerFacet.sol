// SPDX-License-Identifier: MIT
pragma solidity ^0.8.13;

import "@openzeppelin/contracts/utils/math/SafeCast.sol";
import {ERC20} from "@openzeppelin/contracts/token/ERC20/ERC20.sol";
import {ERC20Burnable} from "@openzeppelin/contracts/token/ERC20/extensions/ERC20Burnable.sol";
import {IUniswapV2Router02} from "../interfaces/IUniswapV2Router02.sol";
import {IDividendPayingToken} from "../interfaces/IDividendPayingToken.sol";
import "../libraries/LibDiamond.sol";
import {BaseFacet} from "./BaseFacet.sol";
import {IERC20} from "@openzeppelin/contracts/interfaces/IERC20.sol";

contract DividendTrackerFacet is
    BaseFacet,
    ERC20,
    ERC20Burnable,
    IDividendPayingToken
{
    event Claim(
        address indexed account,
        uint256 amount,
        bool indexed automatic
    );

    constructor() ERC20("Meme Dividend", "DIVIDEND") {
        _grantRole(DEFAULT_ADMIN_ROLE, msg.sender);

        _grantRole(LibDiamond.TOKEN_ADMIN_ROLE, msg.sender);
        _grantRole(LibDiamond.EXCLUDED_FROM_DIVIDEND_ROLE, address(this));
        _grantRole(
            LibDiamond.EXCLUDED_FROM_DIVIDEND_ROLE,
            LibDiamond.BURN_ADDRESS
        );
    }

    /// Received ETH will be handled as dividend.
    receive() external payable {
        accrueDividend();
    }

    // ==================== ERC20 ==================== //

    function _transfer(
        address,
        address,
        uint256
    ) internal pure virtual override {
        require(false, "No transfers allowed");
    }

    // ==================== IDividendPayingToken ==================== //

    /// @return dividends The amount of dividend in wei that `_owner` can withdraw.
    function dividendOf(address _owner)
        public
        view
        returns (uint256 dividends)
    {
        return withdrawableDividendOf(_owner);
    }

    /// @return dividends The amount of dividends that `_owner` has withdrawn
    function withdrawnDividendOf(address _owner)
        public
        view
        returns (uint256 dividends)
    {
        LibDiamond.DiamondStorage storage _ds = LibDiamond.diamondStorage();

        return _ds.withdrawnDividend[_owner];
    }

    /// The total accumulated dividends for a address
    function accumulativeDividendOf(address _owner)
        public
        view
        returns (uint256 accumulated)
    {
        LibDiamond.DiamondStorage storage _ds = LibDiamond.diamondStorage();

        return
            (_ds.magnifiedDividendPerShare *
                balanceOf(_owner) +
                _ds.magnifiedDividend[_owner]) / LibDiamond.MAGNITUDE;
    }

    /// The total withdrawable dividends for a address
    function withdrawableDividendOf(address _owner)
        public
        view
        returns (uint256 withdrawable)
    {
        LibDiamond.DiamondStorage storage _ds = LibDiamond.diamondStorage();

        return accumulativeDividendOf(_owner) - _ds.withdrawnDividend[_owner];
    }

    // ==================== Views ==================== //

    function isExcludedFromDividend(address account)
        public
        view
        returns (bool)
    {
        return hasRole(LibDiamond.EXCLUDED_FROM_DIVIDEND_ROLE, account);
    }

    /// Gets the index of the last processed wallet
    /// @return index The index of the last wallet that was paid dividends
    function getLastProcessedIndex() external view returns (uint256 index) {
        LibDiamond.DiamondStorage storage _ds = LibDiamond.diamondStorage();

        return _ds.lastProcessedIndex;
    }

    /// @return numHolders The number of dividend tracking token holders
    function getTokenHolders() external view returns (uint256 numHolders) {
        LibDiamond.DiamondStorage storage _ds = LibDiamond.diamondStorage();

        return _ds.dividendHolders.keys.length;
    }

    /// Gets account information by address
    function getAccount(address _account)
        public
        view
        returns (
            address account,
            int256 index,
            int256 iterationsUntilProcessed,
            uint256 withdrawableDividends,
            uint256 totalDividends,
            uint256 lastClaimTime,
            uint256 nextClaimTime,
            uint256 secondsUntilAutoClaimAvailable
        )
    {
        LibDiamond.DiamondStorage storage _ds = LibDiamond.diamondStorage();
        LibDiamond.Map storage holders = LibDiamond
            .diamondStorage()
            .dividendHolders;
        account = _account;

        index = getIndexOfKey(account);

        iterationsUntilProcessed = 0;
        if (index >= 0) {
            if (SafeCast.toUint256(index) > _ds.lastProcessedIndex) {
                iterationsUntilProcessed =
                    index -
                    SafeCast.toInt256(_ds.lastProcessedIndex);
            } else {
                uint256 processesUntilEndOfArray = holders.keys.length >
                    _ds.lastProcessedIndex
                    ? holders.keys.length - _ds.lastProcessedIndex
                    : 0;
                iterationsUntilProcessed =
                    index +
                    SafeCast.toInt256(processesUntilEndOfArray);
            }
        }
        withdrawableDividends = withdrawableDividendOf(account);
        totalDividends = accumulativeDividendOf(account);
        lastClaimTime = _ds.claimTimes[account];
        nextClaimTime = lastClaimTime > 0
            ? lastClaimTime + _ds.claimTimeout
            : 0;
        secondsUntilAutoClaimAvailable = nextClaimTime > block.timestamp
            ? nextClaimTime - block.timestamp
            : 0;
    }

    /// Gets account information by index
    /// @param _index The index to get information for
    /// @return account The account retrieved
    /// @return index The index of the account in the iterable mapping
    /// @return iterationsUntilProcessed The number of wallets left to process before this wallet
    /// @return withdrawableDividends The amount of dividends this account can withdraw
    /// @return totalDividends The total dividends this account has earned
    /// @return lastClaimTime The last time the account claimed dividends
    /// @return nextClaimTime The next time this account is eligible to claim dividends
    /// @return secondsUntilAutoClaimAvailable The number of seconds until this account is eligible for dividend claims
    function getAccountAtIndex(uint256 _index)
        public
        view
        returns (
            address account,
            int256 index,
            int256 iterationsUntilProcessed,
            uint256 withdrawableDividends,
            uint256 totalDividends,
            uint256 lastClaimTime,
            uint256 nextClaimTime,
            uint256 secondsUntilAutoClaimAvailable
        )
    {
        LibDiamond.Map storage holders = LibDiamond
            .diamondStorage()
            .dividendHolders;

        if (_index >= holders.keys.length) {
            return (
                0x0000000000000000000000000000000000000000,
                0,
                0,
                0,
                0,
                0,
                0,
                0
            );
        }
        return getAccount(holders.keys[_index]);
    }

    function canAutoClaim(uint256 lastClaimTime) private view returns (bool) {
        LibDiamond.DiamondStorage storage _ds = LibDiamond.diamondStorage();

        if (lastClaimTime > block.timestamp) {
            return false;
        }
        return block.timestamp - lastClaimTime >= _ds.claimTimeout;
    }

    // ==================== Management ==================== //

    /// @notice Adds incoming funds to the dividends per share
    function accrueDividend()
        public
        payable
        onlyRole(LibDiamond.TOKEN_ADMIN_ROLE)
    {
        LibDiamond.DiamondStorage storage _ds = LibDiamond.diamondStorage();
        require(totalSupply() > 0, "No supply");
        if (msg.value > 0) {
            _ds.magnifiedDividendPerShare +=
                (msg.value * LibDiamond.MAGNITUDE) /
                totalSupply();
            _ds.totalAccruedDividend += msg.value;

            emit DividendsDistributed(msg.sender, msg.value);
        }
    }

    /// @param user The user to give the token administrator role to
    function setTokenAdminRole(address user)
        external
        onlyRole(LibDiamond.TOKEN_ADMIN_ROLE)
    {
        _grantRole(LibDiamond.TOKEN_ADMIN_ROLE, user);
    }

    /// @param token The token address of the reward
    function setRewardToken(
        address token,
        LibDiamond.SwapRouter router,
        address[] calldata path
    ) external onlyRole(LibDiamond.TOKEN_ADMIN_ROLE) {
        require(token != address(0), "cannot be null address");
        LibDiamond.DiamondStorage storage _ds = LibDiamond.diamondStorage();

        _ds.rewardToken.token = token;
        _ds.rewardToken.router = router;
        _ds.rewardToken.path = path;
    }

    function excludeFromDividend(address _account)
        external
        onlyRole(LibDiamond.TOKEN_ADMIN_ROLE)
    {
        _grantRole(LibDiamond.EXCLUDED_FROM_DIVIDEND_ROLE, _account);
        _setBalance(_account, 0);
        remove(_account);
    }

    function setMinBalanceForDividend(uint256 newValue)
        external
        onlyRole(LibDiamond.TOKEN_ADMIN_ROLE)
    {
        LibDiamond.DiamondStorage storage _ds = LibDiamond.diamondStorage();

        _ds.minDividendBalance = newValue;
    }

    /// @param _new The new time (in seconds) needed between claims
    /// @dev Must be between 3600 and 86400 seconds
    function updateClaimTimeout(uint256 _new)
        external
        onlyRole(LibDiamond.TOKEN_ADMIN_ROLE)
    {
        require(
            _new >= 3600 && _new <= 86400,
            "Claim wait too short or too long"
        );
        LibDiamond.DiamondStorage storage _ds = LibDiamond.diamondStorage();

        _ds.claimTimeout = _new;
    }

    /// @return success true if the transfer succeeded, false otherwise
    function rescueToken(address tokenAddress)
        external
        onlyRole(LibDiamond.TOKEN_ADMIN_ROLE)
        returns (bool success)
    {
        return
            IERC20(tokenAddress).transfer(
                msg.sender,
                IERC20(tokenAddress).balanceOf(address(this))
            );
    }

    /// @param newBalance The new balance to set for the account.
    function setBalance(address payable account, uint256 newBalance)
        public
        onlyRole(LibDiamond.TOKEN_ADMIN_ROLE)
    {
        LibDiamond.DiamondStorage storage _ds = LibDiamond.diamondStorage();

        if (isExcludedFromDividend(account)) {
            return;
        }

        if (newBalance >= _ds.minDividendBalance) {
            _setBalance(account, newBalance);
            set(account, newBalance);
        } else {
            _setBalance(account, 0);
            remove(account);
        }
        _processAccount(account, true);
    }

    /// This function uses a set amount of gas to process dividends for as many wallets as it can
    /// @param gas The amount of gas to use for processing dividends
    /// @return numProcessed The number of wallets processed
    /// @return numClaims The number of actual claims sent
    /// @return lastIndex The index of the last wallet processed
    function process(uint256 gas)
        public
        onlyRole(LibDiamond.TOKEN_ADMIN_ROLE)
        returns (
            uint256 numProcessed,
            uint256 numClaims,
            uint256 lastIndex
        )
    {
        LibDiamond.DiamondStorage storage _ds = LibDiamond.diamondStorage();
        LibDiamond.Map storage holders = LibDiamond
            .diamondStorage()
            .dividendHolders;

        uint256 numHolders = holders.keys.length;
        if (numHolders == 0) {
            return (0, 0, _ds.lastProcessedIndex);
        }

        uint256 _lastProcessedIndex = _ds.lastProcessedIndex;
        uint256 gasUsed = 0;
        uint256 gasLeft = gasleft();
        uint256 iterations = 0;
        uint256 claims = 0;

        while (gasUsed < gas && iterations < numHolders) {
            _lastProcessedIndex++;
            if (_lastProcessedIndex >= holders.keys.length) {
                _lastProcessedIndex = 0;
            }
            address account = holders.keys[_lastProcessedIndex];
            if (
                canAutoClaim(_ds.claimTimes[account]) &&
                _processAccount(payable(account), true)
            ) {
                claims++;
            }

            iterations++;
            uint256 newGasLeft = gasleft();
            if (gasLeft > newGasLeft) {
                gasUsed += gasLeft - newGasLeft;
            }
            gasLeft = newGasLeft;
        }
        _ds.lastProcessedIndex = _lastProcessedIndex;
        return (iterations, claims, _lastProcessedIndex);
    }

    function swapETHAndWithdrawDividend(address _owner, uint256 _value)
        private
        returns (uint256)
    {
        LibDiamond.DiamondStorage storage _ds = LibDiamond.diamondStorage();

        address router = _fetchTokenRouter(_ds.rewardToken.router);
        try
            IUniswapV2Router02(router)
                .swapExactETHForTokensSupportingFeeOnTransferTokens{
                value: _value
            }(
                0, // accept any amount of tokens
                _ds.rewardToken.path,
                _owner,
                block.timestamp
            )
        {
            emit DividendWithdrawn(_owner, _value);
            return _value;
        } catch {
            _ds.withdrawnDividend[_owner] -= _value;
        }
        return 0;
    }

    // ==================== Internal ==================== //

    function set(address key, uint256 val) public {
        LibDiamond.Map storage holders = LibDiamond
            .diamondStorage()
            .dividendHolders;

        if (holders.inserted[key]) {
            holders.values[key] = val;
        } else {
            holders.inserted[key] = true;
            holders.values[key] = val;
            holders.indexOf[key] = holders.keys.length;
            holders.keys.push(key);
        }
    }

    function remove(address key) public {
        LibDiamond.DiamondStorage storage _ds = LibDiamond.diamondStorage();
        LibDiamond.Map storage holders = _ds.dividendHolders;

        if (!holders.inserted[key]) {
            return;
        }

        delete holders.inserted[key];
        delete holders.values[key];

        uint256 index = holders.indexOf[key];
        uint256 lastIndex = holders.keys.length - 1;
        address lastKey = holders.keys[lastIndex];

        holders.indexOf[lastKey] = index;
        delete holders.indexOf[key];

        holders.keys[index] = lastKey;
        holders.keys.pop();
    }

    function getIndexOfKey(address key) public view returns (int256 index) {
        LibDiamond.Map storage holders = LibDiamond
            .diamondStorage()
            .dividendHolders;

        if (!holders.inserted[key]) {
            return -1;
        }
        return int256(holders.indexOf[key]);
    }

    function _processAccount(address payable account, bool automatic)
        internal
        returns (bool)
    {
        LibDiamond.DiamondStorage storage _ds = LibDiamond.diamondStorage();

        uint256 amount = _withdrawDividendOfUser(account);
        if (amount <= 0) {
            return false;
        }

        _ds.claimTimes[account] = block.timestamp;
        emit Claim(account, amount, automatic);
        return true;
    }

    function _withdrawDividendOfUser(address _owner)
        internal
        returns (uint256)
    {
        LibDiamond.DiamondStorage storage _ds = LibDiamond.diamondStorage();

        uint256 _withdrawableDividend = withdrawableDividendOf(_owner);
        if (_withdrawableDividend > 0) {
            _ds.withdrawnDividend[_owner] += _withdrawableDividend;
            return swapETHAndWithdrawDividend(_owner, _withdrawableDividend);
        }

        return _withdrawableDividend;
    }

    function _setBalance(address _owner, uint256 _newBalance) internal {
        uint256 currentBalance = balanceOf(_owner);
        if (_newBalance > currentBalance) {
            _mint(_owner, _newBalance - currentBalance);
        } else if (_newBalance < currentBalance) {
            _burn(_owner, currentBalance - _newBalance);
        }
    }

    function _fetchTokenRouter(LibDiamond.SwapRouter _router)
        internal
        view
        returns (address)
    {
        LibDiamond.DiamondStorage storage _ds = LibDiamond.diamondStorage();

        if (_router == LibDiamond.SwapRouter.UNISWAP) {
            return _ds.uniswapRouter;
        }
        if (_router == LibDiamond.SwapRouter.SUSHISWAP) {
            return _ds.sushiRouter;
        }
        if (_router == LibDiamond.SwapRouter.MAGICSWAP) {
            return _ds.magicswapRouter;
        }
        return _ds.uniswapRouter;
    }

    function _mint(address _owner, uint256 value) internal override {
        LibDiamond.DiamondStorage storage _ds = LibDiamond.diamondStorage();

        _ds.magnifiedDividend[_owner] -= _ds.magnifiedDividendPerShare * value;
        super._mint(_owner, value);
    }

    function _burn(address _owner, uint256 value) internal override {
        LibDiamond.DiamondStorage storage _ds = LibDiamond.diamondStorage();

        _ds.magnifiedDividend[_owner] += _ds.magnifiedDividendPerShare * value;
        super._burn(_owner, value);
    }
}

