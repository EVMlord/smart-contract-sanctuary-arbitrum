import {IUniswapV2Router02} from "spookyswap/contracts/interfaces/IUniswapV2Router02.sol";
import {IPancakeRouter02} from "pancake/projects/exchange-protocol/contracts/interfaces/IPancakeRouter02.sol";
import {IJoeRouter02} from "traderjoe/contracts/traderjoe/interfaces/IJoeRouter02.sol";
import {ISushiRouter02} from "sushiswap/protocols/sushiswap/contracts/interfaces/IUniswapV2Router02.sol";
import {Ownable} from "openzeppelin/contracts/access/Ownable.sol";
import "./adapters/UniswapAdapter.sol";
import "./adapters/StargateAdapter.sol";

contract CrossChainV3Swaps is StargateAdapter, UniswapAdapter, Ownable {
    using SafeERC20 for IERC20;
    IPancakeRouter02 public immutable pancakeRouter;
    IUniswapV2Router02 public immutable spookyRouter;
    IJoeRouter02 public immutable joeRouter;
    ISushiRouter02 public immutable sushiRouter;

    IWETH9 public immutable weth;

    //Mapping to make sure swaps can only be done with the exchanges on the proper chain.
    mapping(uint8 => mapping(uint16 => bool)) public chainIDs;

    event ReceivedOnDestination(address _token, uint256 _amountLD);
    event Swap(
        address _token,
        uint256 amountLD,
        address _finalToken,
        bool _failed
    );

    constructor(
        address _stargateRouter,
        address _swapRouter,
        address _pancakeRouter,
        address _spookyRouter,
        address _joeRouter,
        address _sushiRouter,
        address _weth
    ) {
        stargateRouter = IStargateRouter(_stargateRouter);
        swapRouter = ISwapRouter(_swapRouter);
        pancakeRouter = IPancakeRouter02(_pancakeRouter);
        spookyRouter = IUniswapV2Router02(_spookyRouter);
        joeRouter = IJoeRouter02(_joeRouter);
        sushiRouter = ISushiRouter02(_sushiRouter);
        weth = IWETH9(_weth);
        feeWallet = msg.sender;
    }

    uint8 internal constant DEPOSIT = 1;
    uint8 internal constant WETH_DEPOSIT = 2;
    uint8 internal constant WETH_WITHDRAW = 3;
    uint8 internal constant UNISWAP_SINGLE_HOP = 4;
    uint8 internal constant UNISWAP_MULTI_HOP = 5;
    uint8 internal constant PANCAKE_SWAP = 6;
    uint8 internal constant SPOOKY_SWAP = 7;
    uint8 internal constant JOE_SWAP = 8;
    uint8 internal constant SUSHI_SWAP = 9;
    uint8 internal constant STARGATE_CALL = 10;

    ///Src logic

    function getChainID() internal view returns (uint16) {
        uint16 id;
        assembly {
            id := chainid()
        }
        return id;
    }

    function mappingAdditions(uint16 _chainId, uint8 _step) external onlyOwner {
        chainIDs[_step][_chainId] = true;
    }

    function crossChainSwap(uint8[] memory steps, bytes[] memory datas)
        external
        payable
    {
        require(
            msg.value > 0,
            "stargate requires a msg.value to pay crosschain message"
        );
        uint256 stepsLength = steps.length;
        for (uint256 i; i < stepsLength; i++) {
            uint8 step = steps[i];
            // update for total amounts in contract?
            if (step == DEPOSIT) {
                (address token, uint256 amount) = abi.decode(
                    datas[i],
                    (address, uint256)
                );
                IERC20(token).safeTransferFrom(
                    msg.sender,
                    address(this),
                    amount
                );
            } else if (step == WETH_DEPOSIT) {
                uint256 amount = abi.decode(datas[i], (uint256));
                weth.deposit{value: amount}();
            } else if (step == WETH_WITHDRAW) {
                (address to, uint256 amount) = abi.decode(
                    datas[i],
                    (address, uint256)
                );
                weth.withdrawTo(to, amount);
            } else if (step == UNISWAP_SINGLE_HOP) {
                SingleHopInfo memory singleHop = abi.decode(
                    datas[i],
                    (SingleHopInfo)
                );
                require(chainIDs[step][getChainID()], "Incorrect Chain");
                IERC20(singleHop.tokenIn).safeIncreaseAllowance(
                    address(swapRouter),
                    singleHop.qty
                );
                swapExactInputSingle(singleHop);
            } else if (step == UNISWAP_MULTI_HOP) {
                MultiHopInfo memory multiHop = abi.decode(
                    datas[i],
                    (MultiHopInfo)
                );
                require(chainIDs[step][getChainID()], "Incorrect Chain");
                IERC20(multiHop.tokenIn).safeIncreaseAllowance(
                    address(swapRouter),
                    multiHop.qty
                );
                swapExactInputMultihop(multiHop);
            } else if (step == PANCAKE_SWAP) {
                (
                    uint256 amountIn,
                    uint256 amountOutMin,
                    address[] memory path,
                    address to,
                    uint256 deadline
                ) = abi.decode(
                        datas[i],
                        (uint256, uint256, address[], address, uint256)
                    );
                require(chainIDs[step][getChainID()], "Incorrect Chain");
                IERC20(path[0]).safeIncreaseAllowance(
                    address(pancakeRouter),
                    amountIn
                );
                IPancakeRouter02(pancakeRouter).swapExactTokensForTokens(
                    amountIn,
                    amountOutMin,
                    path,
                    to,
                    deadline
                );
            } else if (step == SPOOKY_SWAP) {
                (
                    uint256 amountIn,
                    uint256 amountOutMin,
                    address[] memory path,
                    address to,
                    uint256 deadline
                ) = abi.decode(
                        datas[i],
                        (uint256, uint256, address[], address, uint256)
                    );
                require(chainIDs[step][getChainID()], "Incorrect Chain");
                IERC20(path[0]).safeIncreaseAllowance(
                    address(spookyRouter),
                    amountIn
                );
                spookyRouter.swapExactTokensForTokens(
                    amountIn,
                    amountOutMin,
                    path,
                    to,
                    deadline
                );
            } else if (step == JOE_SWAP) {
                (
                    uint256 amountIn,
                    uint256 amountOutMin,
                    address[] memory path,
                    address to,
                    uint256 deadline
                ) = abi.decode(
                        datas[i],
                        (uint256, uint256, address[], address, uint256)
                    );
                require(chainIDs[step][getChainID()], "Incorrect Chain");
                IERC20(path[0]).safeIncreaseAllowance(
                    address(joeRouter),
                    amountIn
                );
                joeRouter.swapExactTokensForTokens(
                    amountIn,
                    amountOutMin,
                    path,
                    to,
                    deadline
                );
            } else if (step == SUSHI_SWAP) {
                (
                    uint256 amountIn,
                    uint256 amountOutMin,
                    address[] memory path,
                    address to,
                    uint256 deadline
                ) = abi.decode(
                        datas[i],
                        (uint256, uint256, address[], address, uint256)
                    );
                IERC20(path[0]).safeIncreaseAllowance(
                    address(sushiRouter),
                    amountIn
                );
                sushiRouter.swapExactTokensForTokens(
                    amountIn,
                    amountOutMin,
                    path,
                    to,
                    deadline
                );
            } else if (step == STARGATE_CALL) {
                (
                    StargateParams memory params,
                    bytes[] memory data,
                    uint8[] memory steperonies
                ) = abi.decode(datas[i], (StargateParams, bytes[], uint8[]));
                IERC20(params.token).safeIncreaseAllowance(
                    address(stargateRouter),
                    params.amount
                );
                stargateCall(steperonies, data, params);
            }
        }
    }

    receive() external payable {}
}

