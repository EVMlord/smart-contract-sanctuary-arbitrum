            require(!isRebatePaid(msg.sender), "rebate must not be paid");

            Rebate storage rebate = globalRebateAddresses[msg.sender];
            rebate.eligible = true;
            rebate.paid = false;
        }

        for (uint256 i = 0; i < _numberOfTokens; i++) {
            globalTokenIds++;

            uint256 tokenId = globalTokenIds;

            globalTokenCodes[tokenId] = _unicodes;

            _safeMint(msg.sender, tokenId);
        }
    }

