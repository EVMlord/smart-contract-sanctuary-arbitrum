struct Skeleton {
  uint256 head;
  uint256 torso;
  uint256 lArm;
  uint256 rArm;
  uint256 lLeg;
  uint256 rLeg;
  uint256 mouth;
  uint256 eyes;
  uint256 color;
  uint256 marking;
  uint256 crown;
}
struct Outfit {
  uint256 head;
  uint256 torso;
  uint256 lArm;
  uint256 rArm;
  uint256 lLeg;
  uint256 rLeg;
  uint256 floating;
}

library CharacterLibrary {
  uint256 public constant MAX_INT = 2**256 - 1;

  function getSkeletonSlot(uint256 slotID, Skeleton memory skeleton)
    public
    pure
    returns (uint256)
  {
    if (slotID == 0) {
      return skeleton.head;
    } else if (slotID == 1) {
      return skeleton.torso;
    } else if (slotID == 2) {
      return skeleton.lArm;
    } else if (slotID == 3) {
      return skeleton.rArm;
    } else if (slotID == 4) {
      return skeleton.lLeg;
    } else if (slotID == 5) {
      return skeleton.rLeg;
    } else if (slotID == 6) {
      return skeleton.mouth;
    } else if (slotID == 7) {
      return skeleton.eyes;
    } else if (slotID == 8) {
      return skeleton.color;
    } else if (slotID == 9) {
      return skeleton.marking;
    } else if (slotID == 10) {
      return skeleton.crown;
    }
    return MAX_INT;
  }

  function getOutfitSlot(uint256 slotID, Outfit memory outfit)
    public
    pure
    returns (uint256)
  {
    if (slotID == 0) {
      return outfit.head;
    } else if (slotID == 1) {
      return outfit.torso;
    } else if (slotID == 2) {
      return outfit.lArm;
    } else if (slotID == 3) {
      return outfit.rArm;
    } else if (slotID == 4) {
      return outfit.lLeg;
    } else if (slotID == 5) {
      return outfit.rLeg;
    } else if (slotID == 6) {
      return outfit.floating;
    }
    return MAX_INT;
  }
}

