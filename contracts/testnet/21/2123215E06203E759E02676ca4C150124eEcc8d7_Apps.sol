import "@openzeppelin/contracts/proxy/Clones.sol";
import "./DataService.sol";
import "./interfaces/IApp.sol";
import "./security/NoStuck.sol";

contract Apps is NoStuck {
    address payable public dataService;
    uint256 private _incSalt;

    event SetDataService(address indexed dataService);
    event NewApp(address indexed author, address indexed proxy, bytes initializeData);

    constructor() {}

    function setDataService(address payable dataService_) public onlyOwner {
        dataService = dataService_;
        emit SetDataService(dataService_);
    }

    /**
     * @dev Create the app implement logic function from logicContract as proxy and initialize it with initializeData
     * @param logicContract_ address of the logical contract for implementation
     * @param initializeData initialization parameter = abi.encode(arg1, arg2, ...);
     */
    function createApp(address logicContract_, AppLib.Subscribe memory subscribe_, bytes memory initializeData) public returns (address) {
        //create proxy implement logicContract_ and return the proxy address
        address proxy = Clones.cloneDeterministic(
            logicContract_,
            bytes32(_incSalt)
        );
