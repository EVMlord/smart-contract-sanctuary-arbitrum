pragma solidity ^0.8.0;

import "../Utility/SerialBuffer.sol";
import "hardhat/console.sol";

struct FandomFixedData {
    uint64  invitorId; // invitor fans Fandom token id, use uint64 instead of uint256
    uint64  producerId; // belong to which producer Fandom token id, use uint64 instead of uint256

    uint8   grade; //
    uint8   fanType; // 1: gensis producer, 2: super fan, 3: fan
    uint32  starRole; // card role
}

struct FandomAttr {
    uint64  maxSP; // max support power
    uint64  sp; // support power

    uint16  level; // fandom level
    uint32  expireTimestamp; // expire time unix stamp
}

contract Fandom721Codec
{
    using SerialBuffer for SerialBuffer.Buffer;

    constructor() {
    }

    function encodeFandomFixedData(FandomFixedData memory data) public pure returns(bytes memory bydata) {
        
        uint32 size = 64; // sizeof(FandomFixedData) align 256
        bydata = new bytes(size);

        SerialBuffer.Buffer memory coder = SerialBuffer.Buffer({
            index: size,
            buffer: bydata
        });

        coder.writeUint64(data.invitorId);
        coder.writeUint64(data.producerId);
        coder.writeUint8(data.grade);
        coder.writeUint8(data.fanType);
        coder.writeUint32(data.starRole);
    } 

    function decodeFandomFixedData(bytes memory bydata) public pure returns(FandomFixedData memory data) {
        
        SerialBuffer.Buffer memory coder = SerialBuffer.Buffer({
            index: bydata.length,
            buffer: bydata
        });

        data.invitorId = coder.readUint64();
        data.producerId = coder.readUint64();
        data.grade = coder.readUint8();
        data.fanType = coder.readUint8();
        data.starRole = coder.readUint32();
    }


    function encodeFandomAttr(FandomAttr memory data) public pure returns(bytes memory bydata) {
        
        uint32 size = 64; // sizeof(FandomAttr) align 256
        bydata = new bytes(size);

        SerialBuffer.Buffer memory coder = SerialBuffer.Buffer({
            index: size,
            buffer: bydata
        });

        coder.writeUint64(data.maxSP);
        coder.writeUint64(data.sp);
        coder.writeUint16(data.level);
        coder.writeUint32(data.expireTimestamp);

        console.log("[sol]encodeFandomAttr buffer.length=",bydata.length);
    } 

    function decodeFandomAttr(bytes memory bydata) public pure returns(FandomAttr memory data) {
        
        console.log("[sol]decodeFandomAttr buffer.length=",bydata.length);

        SerialBuffer.Buffer memory coder = SerialBuffer.Buffer({
            index: bydata.length,
            buffer: bydata
        });

        data.maxSP = coder.readUint64();
        data.sp = coder.readUint64();
        data.level = coder.readUint16();
        data.expireTimestamp = coder.readUint32();
    }
}

