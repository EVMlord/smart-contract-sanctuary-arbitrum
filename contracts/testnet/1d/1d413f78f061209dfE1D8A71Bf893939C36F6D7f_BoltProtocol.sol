pragma solidity ^0.8.9;

import "../interfaces/IBoltProtocol.sol";
import "../utils/Parameters.sol";

import "@uniswap/v3-periphery/contracts/libraries/TransferHelper.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "@openzeppelin/contracts/utils/Counters.sol";
import "@openzeppelin/contracts/utils/cryptography/ECDSA.sol";
// import "@openzeppelin/contracts/utils/Multicall.sol";
import "@openzeppelin/contracts/utils/structs/EnumerableSet.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";

import "hardhat/console.sol";

interface IMessageExecutor {
    function execute(address fns, bytes memory data)
        external
        payable
        returns (bytes memory);
}

/// @title BoltProtocol logic contract for Bolt Protocol
contract BoltProtocol is IBoltProtocol, BoltStructs, Initializable, Parameters {
    /// @notice ID of the network where this contract is, as described in Networks enum. 
	Networks public CURRENT_NETWORK; // deprecated // TODO: remove
    using Counters for Counters.Counter;
    using EnumerableSet for EnumerableSet.AddressSet;

    mapping(address => BoltRelayer) public boltRelayerStakes;
    EnumerableSet.AddressSet private boltRelayerAddresses;
    Counters.Counter private swapCounter;
    mapping(bytes32 => SwapState) public swaps;
    bytes32[] public activeSwaps;

    IMessageExecutor public msgexe;
    EnumerableSet.AddressSet private stakedTokens;

    function initialize(
        address _ex,
        address _feeRecipient,
        uint64[] calldata _params
    ) public initializer {
        msgexe = IMessageExecutor(_ex);
        setParams(_feeRecipient, _params);
    }

    function afterUpgrade() external {
    }

    /// @notice Any relayer can add stake to take swap requests & earn fees;
    /// @notice the stake is bound to one route, which consist of source & destination networks (stake amounts must be identical)
    /// @param routes Routes to stake funds to e.g. [(ETH, PLG), (ETH, AVAX)]
    /// @param amounts The amount to stake for each route
    /// @param token The token to stake (zero for native coin)
    function stake(
        Route[] memory routes,
        uint[] memory amounts,
        address token
    ) external payable validRoutes(routes) {
    	address relayerAddr = msg.sender;
        uint totalAmount = 0;
        BoltRelayer storage r = boltRelayerStakes[relayerAddr];
        uint[] memory routeIds = new uint[](routes.length);
        for (uint i = 0; i < routes.length; i++) {
            routeIds[i] = getRouteID(routes[i]);
            boltRelayerStakes[relayerAddr].stakes[routeIds[i]][token].stakedAmount += amounts[i];
            totalAmount += amounts[i];
        }

        _checkReceive(token, relayerAddr, totalAmount);

        if (r.unstakeEnableBlock == 0) {
            // new relayer
            r.unstakeEnableBlock =
                getBlockNumber() +
                durationToFirstUnstake;
        }

        boltRelayerAddresses.add(relayerAddr); // add() method already checks for duplicates
        stakedTokens.add(token);
        emit Stake(relayerAddr, routeIds, amounts, token);
    }

    /// @notice This is called by a relayer to withdraw their stake after a delay
    /// @notice If this succeeds, fire an event for the pending unstake (with amount & waiting time)
    /// @notice After the waiting period, the relayer needs to call unstake() again to get their funds
    /// @notice Additionally, for new relayers, there is a separate locking period where unstaking is not yet enabled
    /// @param route The route with staked funds to withdraw e.g. (ETH, PLG)
    /// @param token Token to withdraw (zero for native coin)
    /// @param amount Amount to withdraw
    function unstake(
        Route memory route,
        address token,
        uint amount
    ) external {
        BoltRelayer storage r = boltRelayerStakes[msg.sender];
        StakedFunds storage sf = r.stakes[getRouteID(route)][token];

        uint uamt = sf.unstakingAmount;
        if (uamt > 0) {
            uint t = sf.unstakeTime;
            // unstake is already pending => try finalize it instead
            require(
                getBlockNumber() >= t,
                errorToString(Errors.UNSTAKE_TIME_NOT_MET)
            );
            emit Unstake(msg.sender, getRouteID(route), token, uamt, t, true);
            (sf.unstakeTime, sf.unstakingAmount) = (0, 0);
            if (sf.stakedAmount == 0 && sf.lockedAmount == 0) {
                boltRelayerAddresses.remove(msg.sender);
            }
            _transfer(token, msg.sender, uamt);
        } else {
            require(
                getBlockNumber() >= r.unstakeEnableBlock,
                errorToString(Errors.FIRST_UNSTAKE_TIME_NOT_MET)
            );
            require(
                amount > 0 && sf.stakedAmount > sf.lockedAmount + amount,
                errorToString(Errors.UNSTAKE_ZERO)
            );
            sf.stakedAmount -= amount;

            uint unstakeTime = getBlockNumber() + durationToUnstake;
            (sf.unstakeTime, sf.unstakingAmount) = (unstakeTime, amount);
            emit Unstake(
                msg.sender,
                getRouteID(route),
                token,
                amount,
                unstakeTime,
                false
            );
        }
    }

    /// @notice Encodes the source & destination networks into a single uint256
    /// @param r The route object, which contains src & dst (Networks enum)
    /// @return routeID
    function getRouteID(Route memory r) public pure returns (uint) {
        return uint(r.src) * (2 ** 32) + uint(r.dst);
    }

    /// @notice This modifier checks Route objects and reverts upon any route that doesn't contain the uint32(block.chainid) of the contract
    modifier validRoutes(Route[] memory rs) {
        for (uint i = 0; i < rs.length; i++) {
            require(
                rs[i].src != rs[i].dst,
                errorToString(Errors.ROUTE_INVALID)
            );
            require(
                rs[i].src == uint32(block.chainid) || rs[i].dst == uint32(block.chainid),
                errorToString(Errors.ROUTE_INVALID)
            );
        }
        _;
    }

    /// @notice This modifier looks for "blank" ExecutableMessage & checks that its data is without conflict
    modifier validMessages(
        ExecutableMessage memory s,
        ExecutableMessage memory d
    ) {
        if (s.callData.length == 0) {
            require(
                s.tokenIn == s.tokenOut,
                errorToString(Errors.BLANK_MSG_INVALID)
            );
        }
        if (d.callData.length == 0) {
            require(
                d.tokenIn == d.tokenOut,
                errorToString(Errors.BLANK_MSG_INVALID)
            );
        }
        _;
    }

    /// @notice Called by any address to start a swap from source to destination network (defined by `route`)
    /// @notice This requires a relayer(s) with enough funds staked. Relayers join permissionlessly, operate trustlessly & collect fixed %fees
    /// @notice The `message`s determine the token to swap & receive
    /// @notice The requester can split their swap among multiple relayers
    /// @param amount Amount to swap
    /// @param splitPercents The weights to split `amount` among relayers (base 10.000) e.g. [3000, 7000]
    /// @param rs A list of relayers
    /// @param route The route to swap on e.g. (ETH, PLG) (swapping from Ethereum to Polygon
    /// @param srcMsg message to execute in sourc network before crossing
    /// @param dstMsg message to execute in destination network after crossing
    function swap(
        uint amount,
        uint[] memory splitPercents,
        address[] memory rs,
        Route memory route,
        ExecutableMessage memory srcMsg,
        ExecutableMessage memory dstMsg
    ) external payable validMessages(srcMsg, dstMsg) {

        require(splitPercents.length == rs.length, errorToString(Errors.STAKE_AMOUNT_INVALID));
        require(
            route.src == uint32(block.chainid),
            errorToString(Errors.ROUTE_INVALID)
        );
        (, uint returnAmt) = executeSrc(amount, srcMsg);

        uint[] memory amounts = splitAmounts(returnAmt, splitPercents, BASE_PRECISION);
        for (uint i = 0; i < amounts.length; i++) {
            SwapInfo memory s;
            {
                (, s.crossAmount) = getSwapFees(returnAmt);
                s.totalAmount = amounts[i];

                swapCounter.increment();
                // new swap data
                s.nonce = swapCounter.current();
                s.boltRelayerAddr = rs[i];
                s.route = route;

                if (srcMsg.requester == address(0)) srcMsg.requester = msg.sender;
                if (dstMsg.requester == address(0)) dstMsg.requester = msg.sender;
                s.srcMsg = srcMsg;
                s.dstMsg = dstMsg;
            }

            bytes32 h = computeSwapID(s);
            {
                (uint availableStake,) = getAvailableStake(
                    s.boltRelayerAddr,
                    s.route,
                    s.srcMsg.tokenOut
                );
                uint lockAmount = getLockAmount(s.crossAmount);
                require(
                    availableStake >= lockAmount,
                    errorToString(Errors.INSUFFICIENT_STAKE)
                );
                _lockStakeForSwap(s, address(0));
                (swaps[h].startBlock, swaps[h].expiringBlock, swaps[h].status) = (
                    uint64(getBlockNumber()),
                    uint64(getBlockNumber() + lockDuration),
                    Status.S_WAITING
                );
            }
            activeSwaps.push(h);
            emit Swap(h, s);
        }
    }

    /// @notice Byte-encode & hash the SwapInfo and use the output as the swap identifier
    /// @param s The SwapInfo, which contains all data that describes the swap request (token, amount, route, messages to execute etc.)
    /// @return swapID The hash output
    function computeSwapID(SwapInfo memory s) public pure returns (bytes32) {
        (bytes32 temp1, bytes32 temp2) = (
            getMsgHash(s.srcMsg),
            getMsgHash(s.dstMsg)
        );
        temp1 = keccak256(abi.encodePacked(temp1, temp2));
        bytes memory content = abi.encodePacked(
            s.nonce,
            s.boltRelayerAddr,
            getRouteID(s.route),
            s.crossAmount,
            s.totalAmount,
            temp1
        );
        return keccak256(content);
    }

    function getMsgHash(ExecutableMessage memory m)
        internal
        pure
        returns (bytes32)
    {
        return
            keccak256(
                abi.encodePacked(
                    m.requester,
                    m.tokenIn,
                    m.tokenOut,
                    m.callAddress,
                    m.callData
                )
            );
    }

    function _lockStakeForSwap(SwapInfo memory s, address boltOperator)
        internal
    {
        (address token, address unlockSigner, uint amount) = _getLockData(
            s,
            boltOperator
        );
        
        boltRelayerStakes[s.boltRelayerAddr].stakes[getRouteID(s.route)][token].lockedAmount += amount;
        require(
            boltRelayerStakes[s.boltRelayerAddr].stakes[getRouteID(s.route)][token].stakedAmount > boltRelayerStakes[s.boltRelayerAddr].stakes[getRouteID(s.route)][token].lockedAmount,
            errorToString(Errors.INSUFFICIENT_STAKE)
        );

        bytes32 h = computeSwapID(s);
        emit LockStake(h, token, unlockSigner, s.boltRelayerAddr, amount);
    }

    function _getLockData(SwapInfo memory s, address boltOperator)
        internal
        view
        returns (
            address token,
            address unlockSigner,
            uint amount
        )
    {
        if (s.route.src == uint32(block.chainid)) {
            token = s.srcMsg.tokenOut;
            unlockSigner = s.srcMsg.requester;
        } else {
            require(
                boltOperator != address(0),
                errorToString(Errors.ADDRESS_ZERO)
            );
            token = s.dstMsg.tokenIn;
            unlockSigner = boltOperator;
        }
        amount = getLockAmount(s.crossAmount);
    }

    /// @notice Unlock a completed swap, releasing the relayer's locked funds (swap requests always lock the equivalent amount in relayer's stake until it is finished)
    /// @notice `unlock` can only be called after a fixed `lockDuration` OR with a signature from requester
    /// @param s The SwapInfo, which contains all data that describes the swap request (token, amount, route, messages to execute etc.)
    /// @param signature (optional) The signature from requester
    function unlock(SwapInfo memory s, bytes memory signature) external {
        bytes32 h = computeSwapID(s);
        SwapState storage sd = swaps[h];
        (address token, address unlockRewardReceiver, uint amount) = _getLockData(
            s,
            sd.boltOperator
        );
        require(
            sd.status ==
                (
                    s.route.src == uint32(block.chainid)
                        ? Status.S_FULFILLED
                        : Status.F_PROCESSED
                ),
            errorToString(Errors.SWAP_INVALID_STATUS)
        );

        if (signature.length > 0) {
            // unlock by unlockSigner signature
            bytes32 signedContent = keccak256(
                abi.encodePacked(UNLOCK_MESSAGE_PREFIX, h)
            );
            address recoveredAddress = ECDSA.recover(signedContent, signature);
            require(
                recoveredAddress == unlockRewardReceiver,
                errorToString(Errors.RELAY_SIGNATURE_INVALID)
            );
        } else {
            require(
                getBlockNumber() >= sd.expiringBlock,
                errorToString(Errors.UNLOCK_TIME_NOT_MET)
            );
            unlockRewardReceiver = PROTOCOL_FEE_RECIPIENT; // "early unlock reward" is not claimed
        }

        _unlock(s, sd, token, amount, unlockRewardReceiver);
        (bool success, ) = _removeActiveSwap(h);
        require(success, errorToString(Errors.MODIFY_ACTIVE_SWAP_ERROR));
        emit Unlock(h);
    }

    function _unlock(
        SwapInfo memory s,
        SwapState storage sd,
        address token,
        uint amount,
        address unlockRewardReceiver
    ) internal {
        boltRelayerStakes[s.boltRelayerAddr].stakes[getRouteID(s.route)][token].lockedAmount -= amount;

        if (s.route.src == uint32(block.chainid)) {
            sd.status = Status.S_COMPLETED;
            (uint[] memory fees, uint crossAmount) = getSwapFees(
                s.totalAmount
            );
            require(
                crossAmount == s.crossAmount,
                errorToString(Errors.RELAY_DATA_INCORRECT)
            );
            _transfer(
                token,
                sd.boltOperator,
                crossAmount + fees[uint(BoltActors.OPERATOR)]
            );
            _transfer(token, s.boltRelayerAddr, fees[uint(BoltActors.RELAYER)]);
            _transfer(
                token,
                PROTOCOL_FEE_RECIPIENT,
                fees[uint(BoltActors.PROTOCOL)]
            );
            _transfer(token, unlockRewardReceiver, fees[uint(BoltActors.UNLOCK_SIGNER)]);
        } else {
            sd.status = Status.F_COMPLETED;
        }
    }

    /// @notice Cancel an expired swap, releasing the relayer's locked funds & sending the swap amount back to requester
    /// @notice `withdrawRefund` can only be called after a fixed `lockDuration` AND the swap is still not fulfilled
    /// @param s The SwapInfo to be refunded
    function withdrawRefund(SwapInfo memory s) external {
        bytes32 h = computeSwapID(s);
        SwapState storage sd = swaps[h];
        (address token, , uint lockAmount) = _getLockData(s, sd.boltOperator);
        require(
            sd.status ==
                (
                    s.route.src == uint32(block.chainid)
                        ? Status.S_WAITING
                        : Status.F_WAITING
                ),
            errorToString(Errors.SWAP_INVALID_STATUS)
        );
        require(
            getBlockNumber() >= sd.expiringBlock,
            errorToString(Errors.UNLOCK_TIME_NOT_MET)
        );
        uint refundAmount;
        if (s.route.src == uint32(block.chainid)) {
            boltRelayerStakes[s.boltRelayerAddr]
                .stakes[getRouteID(s.route)][token].lockedAmount -= lockAmount;
            refundAmount = s.totalAmount;
            _transfer(token, s.srcMsg.requester, refundAmount);
            sd.status = Status.S_REFUNDED;
        } else {
            refundAmount = s.crossAmount;
            _transfer(token, sd.boltOperator, refundAmount);
            sd.status = Status.F_EXPIRED;
        }
        _removeActiveSwap(h);
        emit Refund(h, token, refundAmount);
    }

    function executeSrc(uint amount, ExecutableMessage memory srcMsg)
        internal
        returns (address, uint)
    {
        _checkReceive(srcMsg.tokenIn, msg.sender, amount);

        // SRC network: revert when msg execution reverts
        return this._executeMessage(amount, srcMsg);
    }

    function executeDst(uint crossAmount, ExecutableMessage memory dstMsg)
        internal
    {
        address resultToken;
        uint resultAmount;
        try this._executeMessage(crossAmount, dstMsg) returns (
            address rt,
            uint amt
        ) {
            (resultToken, resultAmount) = (rt, amt);
        } catch (bytes memory lowLevelData) {
            // DST network: when msg execution reverts, continue using tokenIn
            emit MessageExecuteLog(
                dstMsg.callAddress,
                dstMsg.callData,
                lowLevelData
            );
            (resultToken, resultAmount) = (dstMsg.tokenIn, crossAmount);
        }

        _transfer(resultToken, dstMsg.requester, resultAmount);
    }

    modifier onlySelf() {
        require(
            address(this) == msg.sender,
            errorToString(Errors.ONLY_SELF_CALL)
        );
        _;
    }

    /// @notice execute a call to external contract, specified in `message`; only called by self
    function _executeMessage(uint amount, ExecutableMessage memory _em)
        external
        onlySelf
        returns (address, uint)
    {
        if (_em.callData.length == 0) {
            // blank msg
            return (_em.tokenIn, amount);
        } else {
            uint balanceBefore = _selfBalanceOf(_em.tokenOut);
            uint256 msgval = 0;
            if (_em.tokenIn == NATIVE_TOKEN_ADDRESS) {
                msgval = amount;
            } else {
                TransferHelper.safeTransfer(
                    _em.tokenIn,
                    address(msgexe),
                    amount
                );
            }

            bytes memory result = msgexe.execute{value: msgval}(
                _em.callAddress,
                _em.callData
            );
            require(
                result.length == 64,
                errorToString(Errors.MSGEXE_INVALID_RETURN_DATA)
            );
            (address resultToken, uint resultAmount) = abi.decode(
                result,
                (address, uint)
            );
            require(
                resultToken == _em.tokenOut &&
                    _selfBalanceOf(_em.tokenOut) - balanceBefore ==
                    resultAmount,
                errorToString(Errors.MSGEXE_INVALID_RETURN_DATA)
            );
            return (resultToken, resultAmount);
        }
    }

    /// @return availableStake The available stake amount (total stake minus locked) of a relayer that can be used for swaps
    /// @return sf StakedFunds object that contains detailed info
    /// @param relayerAddr The relayer address
    /// @param route Route that was staked
    /// @param token Token that was staked
    function getAvailableStake(
        address relayerAddr,
        Route memory route,
        address token
    ) public view returns (uint, StakedFunds memory) {
        StakedFunds memory sf = boltRelayerStakes[relayerAddr].stakes[getRouteID(route)][token];
        return (sf.stakedAmount - sf.lockedAmount, sf);
    }

    /// @notice Fulfill a swap from another network to this one using the sender's included funds, which will be used when that swap's relayer calls `relay`
    /// @param s The SwapInfo
    /// @param boltOperator The address to receive your fee for fulfilling
    function fulfill(SwapInfo memory s, address boltOperator) public payable {
        bytes32 h = computeSwapID(s);

        require(
            swaps[h].boltOperator == address(0) &&
                swaps[h].status == Status.EMPTY,
            errorToString(Errors.FULFILL_EXISTS)
        );

        require(
            s.route.dst == uint32(block.chainid),
            errorToString(Errors.ROUTE_INVALID)
        );
        uint crossAmount = s.crossAmount;
        address crossToken = s.dstMsg.tokenIn;

        _checkReceive(crossToken, msg.sender, crossAmount);

        (swaps[h].startBlock, swaps[h].expiringBlock, swaps[h].status, swaps[h].boltOperator) = (
            uint64(getBlockNumber()),
            uint64(getBlockNumber() + lockDuration),
            Status.F_WAITING,
            boltOperator
        );
        activeSwaps.push(h);
        emit Fulfill(boltOperator, s.boltRelayerAddr, h, s);
    }

    /// @notice Called by relayer to relay the swap request. This will execute their dstMsg & transfer the final amount to the requester
    /// @param s The SwapInfo; its hash must match the swapID in storage; relayers cannot sign a SwapInfo that doesn't already exist in SRC network or they risk their locked stake being slashed
    /// @param boltOperator The operator's address (operator is whoever called `fulfill()`)
    /// @param signature The relayer's signature on the content of the swap request
    function relay(
        SwapInfo memory s,
        address boltOperator,
        bytes calldata signature
    ) public {
        bytes32 swapID = computeSwapID(s);
        // check fulfill & the coresponding swap exist, and operator matches
        require(
            boltOperator != address(0) &&
                swaps[swapID].boltOperator == boltOperator,
            errorToString(Errors.RELAY_DATA_INCORRECT)
        );
        require(
            swaps[swapID].status == Status.F_WAITING,
            errorToString(Errors.FULFILL_INVALID_STATUS)
        );
        require(
            getBlockNumber() < swaps[swapID].expiringBlock,
            errorToString(Errors.FULFILL_EXPIRED)
        );
        // verify signature
        bytes32 signedContent = keccak256(
            abi.encodePacked(RELAY_MESSAGE_PREFIX, swapID, boltOperator)
        );
        address recoveredAddress = ECDSA.recover(signedContent, signature);
        require(
            recoveredAddress == s.boltRelayerAddr,
            errorToString(Errors.RELAY_SIGNATURE_INVALID)
        );
        executeDst(s.crossAmount, s.dstMsg);
        _lockStakeForSwap(s, boltOperator);
        emit Relay(swapID, s.boltRelayerAddr, boltOperator, false);
        (swaps[swapID].expiringBlock, swaps[swapID].status) = (
            uint64(getBlockNumber() + lockDuration),
            Status.F_PROCESSED
        );
    }

    /// @notice Convenience function for a relayer to fulfill a swap with their own funds (separate from their locked stake)
    /// @param s The SwapInfo; relayers cannot sign a SwapInfo that doesn't already exist in SRC network or they risk their locked stake being slashed
    /// @param boltOperator The operator's address (operator is whoever called `fulfill()`)
    /// @param signature The relayer's signature on the content of the swap request
    function fulfillAndRelay(
        SwapInfo calldata s,
        address boltOperator,
        bytes calldata signature
    ) external payable {
        fulfill(s, boltOperator);
        relay(s, boltOperator, signature);
    }

    /// @notice Called by a relayer to SRC network after the swap is finished in DST. This will move the swapped amount to operator's address and distribute fees
    /// @param s The SwapInfo
    /// @param boltOperator The operator's address (operator is whoever called `fulfill()`)
    /// @param signature The relayer's signature on the content of the swap request
    function relayReturn(
        SwapInfo memory s,
        address boltOperator,
        bytes calldata signature
    ) external {
        bytes32 swapID = computeSwapID(s);
        {
            // check s exists & has not expired
            require(
                swaps[swapID].status == Status.S_WAITING,
                errorToString(Errors.SWAP_INVALID_STATUS)
            );
            require(
                s.boltRelayerAddr != address(0),
                errorToString(Errors.RELAY_DATA_INCORRECT)
            );
            require(
                getBlockNumber() < swaps[swapID].expiringBlock,
                errorToString(Errors.SWAP_EXPIRED)
            );

            require(
                boltOperator != address(0),
                errorToString(Errors.RELAY_DATA_INCORRECT)
            );

            bytes32 signedContent = keccak256(
                abi.encodePacked(RELAY_MESSAGE_PREFIX, swapID, boltOperator)
            );
            address recoveredAddress = ECDSA.recover(signedContent, signature);
            require(
                recoveredAddress == s.boltRelayerAddr,
                errorToString(Errors.RELAY_SIGNATURE_INVALID)
            );
            // store fulfill data
            swaps[swapID].boltOperator = boltOperator;
            
            // swaps[swapID].expiringBlock = getBlockNumber() + lockDuration;
        }

        
        // update timeout & status
        (swaps[swapID].expiringBlock, swaps[swapID].status) = (
            uint64(getBlockNumber() + lockDuration),
            Status.S_FULFILLED
        );
        emit Relay(swapID, s.boltRelayerAddr, boltOperator, true);
    }

    /// @notice `Slash` can be called by anyone who notices bad behaviors by a relayer, which can be proved using the signatures created by that relayer
    /// @notice This will then use their locked stake to restore potentially lost funds & burn the over-collateral
    /// @notice The watcher also receives reward
    /// @param rule Rule to slash with
    /// @param s The SwapInfo; relayers cannot sign a SwapInfo that doesn't already exist in SRC network or they risk their locked stake being slashed
    /// @param boltOperator The operator's address (operator is whoever called `fulfill()`)
    /// @param signedSwapID The actual swapID signed by relayer
    /// @param signature The relayer's signature
    function slash(
        SlashRules rule,
        SwapInfo calldata s,
        address boltOperator,
        bytes32 signedSwapID,
        bytes calldata signature
    ) external {
        address boltRelayerAddr = s.boltRelayerAddr;
        // verify signature
        bytes32 signedContent = keccak256(
            abi.encodePacked(RELAY_MESSAGE_PREFIX, signedSwapID, boltOperator)
        );
        address recoveredAddress = ECDSA.recover(signedContent, signature);
        require(
            recoveredAddress == boltRelayerAddr,
            errorToString(Errors.RELAY_SIGNATURE_INVALID)
        );

        uint amt;
        address token;
        bytes32 h = computeSwapID(s);
        if (rule == SlashRules.RULE1) {
            require(
                signedSwapID == h,
                errorToString(Errors.SLASH_DATA_INCORRECT)
            );
            (amt, token) = _slashRule1(h, s, boltRelayerAddr);
        } else if (rule == SlashRules.RULE2) {
            require(
                signedSwapID == h,
                errorToString(Errors.SLASH_DATA_INCORRECT)
            );
            (amt, token) = _slashRule2(h, s, boltOperator);
        } else if (rule == SlashRules.RULE3) {
            (amt, token) = _slashRule3(h, signedSwapID, s);
        } else {
            revert(errorToString(Errors.SLASH_DATA_INCORRECT));
        }

        emit Slash(rule, amt, token, boltRelayerAddr, h);
    }

    function _slashRule1(
        bytes32 swapID,
        SwapInfo calldata s,
        address boltRelayerAddr
    ) internal returns (uint, address) {
        require(
            s.route.dst == uint32(block.chainid),
            errorToString(Errors.ROUTE_INVALID)
        );
        require(
            computeSwapID(s) == swapID,
            errorToString(Errors.SLASH_DATA_INCORRECT)
        );
        require(
            swaps[swapID].status == Status.F_WAITING || swaps[swapID].status == Status.EMPTY,
            errorToString(Errors.FULFILL_MISSING)
        );

        (uint[] memory fees, ) = getSwapFees(s.totalAmount);
        uint watcherReward = fees[uint(BoltActors.RELAYER)] * watcherRewardPercent[uint(SlashRules.RULE1)] / BASE_PRECISION;
        uint lostStake = s.crossAmount + watcherReward;
        StakedFunds storage sf = boltRelayerStakes[boltRelayerAddr].stakes[getRouteID(s.route)][s.dstMsg.tokenIn];
        sf.stakedAmount -= lostStake;
        executeDst(s.crossAmount, s.dstMsg);
        _transfer(s.dstMsg.tokenIn, msg.sender, watcherReward);

        swaps[swapID].status = Status.F_SLASHED;
        _removeActiveSwap(swapID);
        return (lostStake, s.dstMsg.tokenIn);
    }

    function _slashRule2(
        bytes32 swapID,
        SwapInfo calldata s,
        address signedOperatorAddress
    ) internal returns (uint, address) {
        require(
            s.route.dst == uint32(block.chainid),
            errorToString(Errors.ROUTE_INVALID)
        );
        require(
            swaps[swapID].status == Status.F_PROCESSED,
            errorToString(Errors.FULFILL_INVALID_STATUS)
        );
        require(
            signedOperatorAddress != swaps[swapID].boltOperator,
            errorToString(Errors.SLASH_DATA_INCORRECT)
        );

        (uint[] memory fees, ) = getSwapFees(s.totalAmount);
        uint lockAmt = getLockAmount(s.crossAmount);
        uint amt1 = s.crossAmount + fees[uint(BoltActors.OPERATOR)];
        uint[] memory rem = splitWatcherReward(
            lockAmt - amt1,
            watcherRewardPercent[uint(SlashRules.RULE2)],
            BASE_PRECISION
        );

        _transfer(s.dstMsg.tokenOut, swaps[swapID].boltOperator, amt1);
        _transfer(s.dstMsg.tokenOut, msg.sender, rem[0]);
        _transfer(s.dstMsg.tokenOut, PROTOCOL_FEE_RECIPIENT, rem[1]);

        swaps[swapID].status = Status.F_SLASHED;
        _removeActiveSwap(swapID);
        return (lockAmt, s.dstMsg.tokenOut);
    }

    function _slashRule3(
        bytes32 swapID,
        bytes32 slashedID,
        SwapInfo calldata s
    ) internal returns (uint, address) {
        require(
            swaps[slashedID].status == Status.EMPTY,
            errorToString(Errors.SLASH_DATA_INCORRECT)
        );
        require(
            s.route.src == uint32(block.chainid),
            errorToString(Errors.ROUTE_INVALID)
        );
        require(
            swaps[swapID].status == Status.S_WAITING,
            errorToString(Errors.SWAP_MISSING)
        );

        uint lockAmt = getLockAmount(s.crossAmount);
        uint amt1 = s.crossAmount;
        uint[] memory rem = splitWatcherReward(
            lockAmt - amt1,
            watcherRewardPercent[uint(SlashRules.RULE3)],
            BASE_PRECISION
        );

        _transfer(s.srcMsg.tokenOut, s.srcMsg.requester, amt1);
        _transfer(s.srcMsg.tokenOut, msg.sender, rem[0]);
        _transfer(s.srcMsg.tokenOut, PROTOCOL_FEE_RECIPIENT, rem[1]);

        swaps[swapID].status = Status.S_SLASHED;
        _removeActiveSwap(swapID);
        return (getLockAmount(s.crossAmount), s.srcMsg.tokenOut);
    }

    /// @return The number of active swap requests (active: any that has not been finalized)
    function getActiveSwapCount() external view returns (uint) {
        return activeSwaps.length;
    }

    /// @return The list of all active swap requests
    /// @return The list of corresponding SwapState objects (containing status , operator address etc.)
    function getActiveSwaps() external view returns (bytes32[] memory, SwapState[] memory) {
        SwapState[] memory stateLst = new SwapState[](activeSwaps.length);
        for (uint i = 0; i < activeSwaps.length; i++) {
            stateLst[i] = swaps[activeSwaps[i]];
        }

        return (activeSwaps, stateLst);
    }

    /// @return The list of all relayers that has stake in this contract
    function getRelayerAddresses() external view returns (address[] memory) {
        return boltRelayerAddresses.values();
    }

    function stakedTokenList() external view returns (address[] memory) {
        return stakedTokens.values();
    }

    function _removeActiveSwap(bytes32 h) internal returns (bool, uint) {
        uint len = activeSwaps.length;
        for (uint i = 0; i < len; i++) {
            if (activeSwaps[i] == h) {
                bytes32 temp = activeSwaps[len - 1];
                activeSwaps[i] = temp;
                activeSwaps.pop();
                return (true, i);
            }
        }
        return (false, 0);
    }

    function _selfBalanceOf(address token) internal view returns (uint) {
        if (token == NATIVE_TOKEN_ADDRESS) {
            return address(this).balance;
        } else {
            return IERC20(token).balanceOf(address(this));
        }
    }

    function _transfer(
        address token,
        address to,
        uint amount
    ) internal {
        if (token == NATIVE_TOKEN_ADDRESS) {
            TransferHelper.safeTransferETH(to, amount);
        } else {
            TransferHelper.safeTransfer(token, to, amount);
        }
    }

    function _checkReceive(
        address token,
        address from,
        uint amount
    ) internal {
        if (token == NATIVE_TOKEN_ADDRESS) {
            require(
                amount == msg.value,
                errorToString(Errors.TX_NATIVE_AMOUNT_INVALID)
            );
        } else {
            TransferHelper.safeTransferFrom(token, from, address(this), amount);
        }
    }
}

// SPDX-License-Identifier: MIT
pragma solidity ^0.8.9;

abstract contract BoltStructs {
    enum Networks {
        ETH,
        ARB,
        OP,
        PLG,
        FTM
    }

    enum BoltActors {
        RELAYER,
        OPERATOR,
        PROTOCOL,
        UNLOCK_SIGNER
