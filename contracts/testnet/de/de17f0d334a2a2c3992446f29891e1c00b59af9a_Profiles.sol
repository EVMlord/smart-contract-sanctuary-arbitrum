pragma solidity ^0.8.20;

import {LibString} from "../lib/solmate/src/utils/LibString.sol";
import {ECDSA} from "@openzeppelin/contracts/utils/cryptography/ECDSA.sol";
import {OwnableUpgradeable} from "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";
import {Initializable} from "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";

import {IProfiles} from "./interfaces/IProfiles.sol";
import {Operatable} from "./abstracts/Operatable.sol";
import {SoulBound721Upgradeable} from "./abstracts/SoulBound721Upgradeable.sol";

contract Profiles is IProfiles, Initializable, SoulBound721Upgradeable, OwnableUpgradeable, Operatable {
    using ECDSA for bytes32;

    string public constant NAME = "ggQuest";
    string public constant SYMBOL = "ggQ";
    string public contractURI;

    /// @notice registered addresses
    address[] private registeredAddresses;

    /// @notice Supported thirdParties (Twitch, Discord, Steam...)
    string[] private thirdParties;

    /// @notice Associate an address with profile data
    mapping(address => ProfileData) private profiles;

    /// @notice Pseudonymes to avoid two players to have the same one
    mapping(string => bool) private takenPseudonymes;

    function initialize(address _owner) public initializer {
        __SoulBound721_init(NAME, SYMBOL);
        __Ownable_init();
        operators[_owner] = true;
    }

    /// @notice Mint a new ggQuest NFT
    /// @param _userData desc
    function mint(UpdatableByUserData calldata _userData, bytes calldata _signature) external {
        address player = msg.sender;
        if (profiles[player].isRegistered) {
            revert ProfileAlreadyRegistered(player);
        }

        _setUserData(player, _userData);
        registeredAddresses.push(player);

        bytes32 hash = keccak256(abi.encodePacked(address(this), player));
        bytes32 message = ECDSA.toEthSignedMessageHash(hash);

        (address recovered, ECDSA.RecoverError error) = ECDSA.tryRecover(message, _signature);
        if (error != ECDSA.RecoverError.NoError || recovered != player) {
            revert WrongSignature();
        }

        uint256 _tokenId = uint256(uint160(player));
        profiles[player].isRegistered = true;
        _mint(player, _tokenId);
        emit Mint(player, _userData.pseudo);
    }

    /**
     * @notice Update sender's profile data
     * @param _userData updatable data to be affected to the sender
     *
     */
    function update(UpdatableByUserData calldata _userData) external {
        if (!profiles[msg.sender].isRegistered) {
            revert ProfileNotRegistered(msg.sender);
        }
        _setUserData(msg.sender, _userData);
        emit Update(msg.sender, _userData.pseudo);
    }

    /**
     * @notice Increase reputation of the user
     * @param _userAddress profile address
     * @param _amount amount to add to the reputation
     *
     */
    function increaseReputation(address _userAddress, uint256 _amount) external onlyOperator {
        if (!profiles[_userAddress].isRegistered) {
            revert ProfileNotRegistered(_userAddress);
        }

        profiles[_userAddress].gainedReputation += _amount;

        emit IncreaseReputation(_userAddress, _amount);
    }

    /**
     * @notice Decrease reputation of the user
     * @param _userAddress profile address
     * @param _amount amount to add to the reputation
     *
     */
    function decreaseReputation(address _userAddress, uint256 _amount) external onlyOperator {
        if (!profiles[_userAddress].isRegistered) {
            revert ProfileNotRegistered(_userAddress);
        }

        profiles[_userAddress].lostReputation += _amount;
        emit DecreaseReputation(_userAddress, _amount);
    }

    /**
     * @notice Add a supported third party - operators only
     * @param _thirdPartyName NAME of the thirdParty (e.g. "TWITCH", "DISCORD", "STEAM"...)
     *
     */
    function addThirdParty(string calldata _thirdPartyName) external onlyOperator {
        if (bytes(_thirdPartyName).length == 0) {
            revert ThirdPartyNameCannotBeEmpty();
        }
        thirdParties.push(_thirdPartyName);
        emit AddSupportedThirdParty(_thirdPartyName);
    }

    /**
     * @notice Link a third party account to a profile
     * @param _profileAddress address of the profile
     * @param _thirdPartyId id of the supported third party (index of the third party in thirdParties)
     * @param _thirdPartyUserID id of the user in the third party database (with which we get info from third party api)
     */
    function linkThirdPartyToProfile(
        address _profileAddress,
        uint256 _thirdPartyId,
        uint256 _thirdPartyUserID
    ) external onlyOperator {
        if (!profiles[_profileAddress].isRegistered) {
            revert ProfileNotRegistered(_profileAddress);
        }

        ThirdParty[] memory linkedThirdParties = profiles[_profileAddress].linkedThirdParties;
        uint256 length = linkedThirdParties.length;

        for (uint256 i = 0; i < length; ) {
            if (linkedThirdParties[i].thirdPartyId == _thirdPartyId) {
                revert ProfileAlreadyLinked(_profileAddress, _thirdPartyId);
            }

            unchecked {
                ++i;
            }
        }

        ThirdParty memory newThirdPartyLink;
        newThirdPartyLink.thirdPartyId = _thirdPartyId;
        newThirdPartyLink.userId = _thirdPartyUserID;

        profiles[_profileAddress].linkedThirdParties.push(newThirdPartyLink);

        emit LinkThirdPartyToProfile(_profileAddress, _thirdPartyId, _thirdPartyUserID);
    }

    /**
     * @notice Unlink a third party account from a profile
     * @param _profileAddress address of the profile
     * @param _thirdPartyId id of the supported third party (index of the third party in thirdParties)
     *
     */
    function unlinkThirdPartyFromProfile(address _profileAddress, uint256 _thirdPartyId) external onlyOperator {
        if (!profiles[_profileAddress].isRegistered) {
            revert ProfileNotRegistered(_profileAddress);
        }

        ThirdParty[] memory linkedThirdParties = profiles[_profileAddress].linkedThirdParties;
        uint256 length = linkedThirdParties.length;
        if (length == 0) return;

        bool removed = false;
        uint256 removedIndex;
        for (uint256 i = 0; i < length; ) {
            if (linkedThirdParties[i].thirdPartyId == _thirdPartyId) {
                delete profiles[_profileAddress].linkedThirdParties[i];
                removed = true;
                removedIndex = i;
            }

            unchecked {
                ++i;
            }
        }

        if (removed) {
            // Remove gap from array by putting last item to removed index
            profiles[_profileAddress].linkedThirdParties[removedIndex] = profiles[_profileAddress].linkedThirdParties[
                length - 1
            ];

            profiles[_profileAddress].linkedThirdParties.pop();
        }

        emit UnlinkThirdPartyToProfile(_profileAddress, _thirdPartyId);
    }

    function setContractURI(string calldata _contractURI) external onlyOwner {
        contractURI = _contractURI;
    }

    /**
     * @notice Get the registered addresses
     * @return array of registered addresses
     *
     */
    function getRegisteredAddresses() external view returns (address[] memory) {
        return registeredAddresses;
    }

    /**
     * @notice Get all the supported third parties. Can be used to associate the id with each one of them
     * @return array of thirdParties
     */
    function getThirdParties() external view returns (string[] memory) {
        return thirdParties;
    }

    /**
     * @notice Get profile data of a registered address
     * @param _userAddress address of the user
     * @return profile data associated with the given user address
     */
    function getProfileData(address _userAddress) external view returns (ProfileData memory) {
        return profiles[_userAddress];
    }

    function getProfilePictures(
        address _user
    ) external view returns (string memory profilePictureURL, string memory coverPictureURL) {
        profilePictureURL = profiles[_user].profilePictureURL;
        coverPictureURL = profiles[_user].coverPictureURL;
    }

    /**
     * @notice Get the reputation score of an address
     * @dev You have to calculate the total score (as SafeMath doesn't support int for negative scores)
     * @param _userAddress profile address
     * @return gained reputation, amount of reputation to add to the total score
     * @return lost reputation, amount of reputation to substract to the total score
     *
     */
    function getReputation(address _userAddress) external view returns (uint256, uint256) {
        return (profiles[_userAddress].gainedReputation, profiles[_userAddress].lostReputation);
    }

    /**
     * @notice Check if an address is registered
     * @param _userAddress address of the user
     * @return true if user address is registered
     *
     */
    function hasProfileData(address _userAddress) external view returns (bool) {
        return profiles[_userAddress].isRegistered;
    }

    /**
     * @notice Check if a pseudo is available (not taken)
     * @param _pseudo pseudo to challenge
     * @return true if the address is available
     */
    function isAvailable(string calldata _pseudo) external view returns (bool) {
        return !takenPseudonymes[_pseudo];
    }

    function generateHash(UpdatableByUserData calldata _userData) external pure returns (bytes32) {
        return keccak256(abi.encodePacked(_userData.pseudo, _userData.profilePictureURL, _userData.coverPictureURL));
    }

    /**
     * @dev Returns an URI for a given token ID
     */
    function tokenURI(uint256 _tokenId) public view override returns (string memory) {
        if (_ownerOf(_tokenId) == address(0)) {
            revert TokenIdNotFound();
        }
        string memory baseURI = contractURI;
        return bytes(baseURI).length != 0 ? string(abi.encodePacked(baseURI, LibString.toString(_tokenId))) : "";
    }

    /**
     * @notice Update a profile fields
     * @param _user address of the user profile
     * @param _userData data to update
     */
    function _setUserData(address _user, UpdatableByUserData calldata _userData) private {
        string memory currentPseudo = profiles[_user].pseudo;
        string memory newPseudo = _userData.pseudo;

        // Check if the new pseudonym is not an empty string
        if (bytes(_userData.pseudo).length == 0) {
            revert PseudoCannotBeEmpty();
        }

        // Check if the new pseudonym is already taken
        bool isNewPseudoAlreadyTaken = takenPseudonymes[newPseudo];
        // Check if the new pseudonym is equal to the current one
        bool isNewPseudoEqualCurrent = keccak256(bytes(newPseudo)) == keccak256(bytes(currentPseudo));
        if (isNewPseudoAlreadyTaken || isNewPseudoEqualCurrent) {
            revert PseudoNotAvailable();
        }

        // Mark the pseudo as taken if modified
        takenPseudonymes[currentPseudo] = false;
        takenPseudonymes[newPseudo] = true;

        // Update Profile data
        profiles[_user].pseudo = newPseudo;
        profiles[_user].profilePictureURL = _userData.profilePictureURL;
        profiles[_user].coverPictureURL = _userData.coverPictureURL;
    }
}

