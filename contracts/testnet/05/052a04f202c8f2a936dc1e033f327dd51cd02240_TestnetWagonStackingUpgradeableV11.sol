        uint256 lockedPeriod = getCurrentLockedPeriod();
        if(lockedPeriod == 0) return;
        
        userLatestLock storage new_user_latest_lock = userLatestLocks[poolId][msg.sender];
        if(lockedPeriod == new_user_latest_lock.lockedPeriod) return;

        if(new_user_latest_lock.totalLocked == 0) {
            new_user_latest_lock.totalLocked = _totalStake;
        } else {
            new_user_latest_lock.totalLocked = getCurrentTotalUserLocked(poolId, msg.sender);
        }
        
        new_user_latest_lock.lockedPeriod = lockedPeriod;
    }

    // updateUserLatestLockRemoveAutocompound.
    // Update user latest locked status which are:
    // - total wagon locked
    // - locked period
    function updateUserLatestLockRemoveAutocompound(uint256 poolId, uint256 _totalLocked) private {
        uint256 lockedPeriod = getCurrentLockedPeriod();
        if(lockedPeriod == 0) return;
        
        userLatestLock storage new_user_latest_lock = userLatestLocks[poolId][msg.sender];
        if(lockedPeriod >= new_user_latest_lock.lockedPeriod) return;

        new_user_latest_lock.totalLocked = _totalLocked;
        new_user_latest_lock.lockedPeriod = lockedPeriod;
    }

    // updateTotalLatestLocked.
    // Update total latest locked
    function updateTotalLatestLocked(uint256 poolId) private {
        uint256 lockedPeriod = getCurrentLockedPeriod();
        if(lockedPeriod == 0) return;
        if(totalPeriodLocked[poolId][lockedPeriod] > 0) return;

        totalPeriodLocked[poolId][lockedPeriod] = getTotalStaked(poolId, lockedPeriod);
    }

    // getCurrentPeriod.
    // Get the staking period time to be locked
    function getCurrentPeriod() public view returns (uint256) {
        if(firstLockTime == 0) return 1;

        uint256 timeNow = block.timestamp;
        if(timeNow <= firstLockTime) return 1;

        //  return different days between now and the first lock timestamp
        uint256 diffDays = BokkyPooBahsDateTimeLibrary.diffSeconds(firstLockTime, timeNow);
        uint256 period = diffDays / lockDuration;

        return period + 2;
    }

    // getCurrentLockedPeriod.
    // Get the locked period
    function getCurrentLockedPeriod() public view returns(uint256) {
        return getCurrentPeriod() - 1;
    }

    // getCurrentTotalUserStake.
    // Get total user current locked WAG.
    function getCurrentTotalUserStake(uint256 poolId, address _address) public isPoolAvailable(poolId) view returns(uint256) {
        uint256 currentPeriod = getCurrentPeriod();
        uint256 totalUserStaked = stakings[poolId][currentPeriod][_address];

        return totalUserStaked;
    }

    // getCurrentTotalUserLocked.
    // Get total user current locked WAG.
    function getCurrentTotalUserLocked(uint256 poolId, address _address) public isPoolAvailable(poolId) view returns(uint256) {
        uint256 lockedPeriod = getCurrentLockedPeriod();
        uint256 totalUserLocked = getTotalUserPeriodLocked(poolId, lockedPeriod, _address);

        return totalUserLocked;
    }

    // getTotalUserPeriodLocked.
    // Get total user locked WAG at a certain locked period
    // @param _lockedPeriod locked period
    function getTotalUserPeriodLocked(uint256 poolId, uint256 _lockedPeriod, address _address) public isPoolAvailable(poolId) view returns(uint256) {
        if(_lockedPeriod == 0) return 0;
        if(_lockedPeriod == 1) return stakings[poolId][1][_address];

        userLatestLock memory userLock = userLatestLocks[poolId][_address];
        uint256 totalUserLocked = userLock.totalLocked;
        uint256 latestLockedPeriod = userLock.lockedPeriod;

        if(latestLockedPeriod == _lockedPeriod) return totalUserLocked;

        if(latestLockedPeriod > 0 && latestLockedPeriod < _lockedPeriod) {
            totalUserLocked += calculateReward(poolId, latestLockedPeriod, totalUserLocked);
            totalUserLocked -= withdraws[poolId][latestLockedPeriod][_address];
        }

        uint256 i = latestLockedPeriod + 1;
        for(i; i <= _lockedPeriod; i++) {
            totalUserLocked += stakings[poolId][i][_address];
            if (i == _lockedPeriod) break;
            totalUserLocked += calculateReward(poolId, i, totalUserLocked);
            totalUserLocked -= withdraws[poolId][i][_address];
        }

        return totalUserLocked;
    }

    // getTotalUserReward.
    // Get total user reward
    // @param _address sender Address
    function getTotalUserReward(uint256 poolId, address _address) public isPoolAvailable(poolId) view returns(uint256) {
        uint256 currentLockedPeriod = getCurrentLockedPeriod();

        if(currentLockedPeriod == 1) return 0;

        userLatestLock memory userLock = userLatestLocks[poolId][_address];
        if(userLock.lockedPeriod == 0) return 0;
        
        uint256 totalUserLocked = 0;
        uint256 totalReward = 0;
        uint256 reward = 0;

        uint256 i = 1;
        for(i; i <= currentLockedPeriod; i++) {
            totalUserLocked += stakings[poolId][i][_address];
            if (i == currentLockedPeriod) continue;
            reward = calculateReward(poolId, i, totalUserLocked);
            totalUserLocked += reward;
            totalReward += reward;
            totalUserLocked -= withdraws[poolId][i][_address];
        }

        return totalReward;
    }

    // calculateReward.
    // Calculate reward with by specified user staked at a certain period
    // @param _period period to calculate the reward
    // @param _totalUserStaked total of user staked for the reward
    function calculateReward(uint256 poolId, uint256 _period, uint256 _totalUserStaked) internal isPoolAvailable(poolId) view returns(uint256) {
        if(_totalUserStaked == 0) return 0;

        uint256 totalProfit = totalProfitShares[poolId][_period];
        if(totalProfit == 0) return 0;

        uint256 totalStaked = getTotalStaked(poolId, _period);
        if(totalStaked == 0) return 0;

        return _totalUserStaked * totalProfit / totalStaked;
    }

    // getTotalStaked.
    // Get total staked in certain period
    // @param _period period to calculate the reward
    function getTotalStaked(uint256 poolId, uint256 _period) public isPoolAvailable(poolId) view returns(uint256) {
        if(_period == 0) return 0;
        if(_period == 1) return totalPeriodStakes[poolId][1];

        if(totalPeriodLocked[poolId][_period] > 0) return totalPeriodLocked[poolId][_period];

        uint256 totalStaked = 0;

        uint256 previousLockedPeriod = totalPeriodLocked[poolId][_period-1];
        if(previousLockedPeriod > 0) {
            totalStaked = previousLockedPeriod;
            totalStaked += totalProfitShares[poolId][_period-1];
            totalStaked -= totalPeriodWithdraws[poolId][_period-1];
            totalStaked += totalPeriodStakes[poolId][_period];
            return totalStaked;
        }
        
        uint i = 1;
        for(i; i <= _period; i++) {
            totalStaked += totalPeriodStakes[poolId][i];
            if (i == _period) continue;
            totalStaked += totalProfitShares[poolId][i];
            totalStaked -= totalPeriodWithdraws[poolId][i];
        }
        return totalStaked;
    }

    // getUserReward.
    // Get user reward from current locked period.
    function getUserReward(uint256 poolId, address _address) public isPoolAvailable(poolId) view returns(uint256) {
        uint256 period = getCurrentLockedPeriod();
        uint256 userReward = getUserPeriodReward(poolId, period, _address);
        
        return userReward;
    }

    // getUserPeriodReward.
    // Get user reward from certain locked period.
    // @param _period period to get the reward
    function getUserPeriodReward(uint256 poolId, uint256 _period, address _address) public isPoolAvailable(poolId) view returns(uint256) {
        if(_period == 0) return 0;

        uint256 totalUserLocked = getTotalUserPeriodLocked(poolId, _period, _address);
        uint256 totalStaked = getTotalStaked(poolId, _period);
        return totalUserLocked * totalProfitShares[poolId][_period] / totalStaked;
    }

    // stakeWagon.
    // Stake WAG to be lock.
    // @param _amountWagon Amount of wagon to be stake
    function stakeWagon(uint256 poolId, uint256 _amountWagon) public isPoolAvailable(poolId) whenNotPaused nonReentrant{
        require(wagon.balanceOf(msg.sender) >= _amountWagon, "Balance not enough.");
        wagon.transferFrom(msg.sender, address(this), _amountWagon);
        
        uint256 period = getCurrentPeriod();

        stakings[poolId][period][msg.sender] += _amountWagon;
        totalPeriodStakes[poolId][period] += _amountWagon;

        if(userLatestStakePeriod[poolId][msg.sender] == 0) {
            updateUserLatestLockStake(poolId, _amountWagon);
        } else {
            updateUserLatestLock(poolId);
        }

        userLatestStakePeriod[poolId][msg.sender] = period;

        emit Stake(poolId, period, msg.sender, _amountWagon);
    }

    // unstakeWagon.
    // Unstake WAG before it locked.
    // @param _amountWagon Amount of wagon to be stake
    function unstakeWagon(uint256 poolId, uint256 _amountWagon) public isPoolAvailable(poolId) whenNotPaused nonReentrant{
        uint256 period = getCurrentPeriod();

        uint256 staked = stakings[poolId][period][msg.sender];
        require(staked >= _amountWagon, "Too much amount to unstake.");

        stakings[poolId][period][msg.sender] -= _amountWagon;
        totalPeriodStakes[poolId][period] -= _amountWagon;
        wagon.transfer(msg.sender, _amountWagon);

        updateUserLatestLock(poolId);
        emit Unstake(poolId, period, msg.sender, _amountWagon);
    }

    // distributeProfitSharing.
    // Add Wagon to be distribute for rewards on current locked period.
    // @param _amountWagon Amount of wagon to be stake
    function distributeProfitSharing(uint256 poolId, uint256 _amountWagon) public isPoolAvailable(poolId) onlyRole(PROFIT_SHARE_ROLE){
        uint256 lockedPeriod = getCurrentLockedPeriod();
        require(lockedPeriod > 0, "Staking period not yet started");

        require(wagon.balanceOf(msg.sender) >= _amountWagon, "Balance not enough.");

        uint256 rewards = _amountWagon;
        uint256 fee = 0;

        if(distributionFee > 0) {
            fee = rewards * distributionFee / 100;
            rewards = rewards - fee;
            wagon.transferFrom(msg.sender, treasuryAddress, fee);
        }

        wagon.transferFrom(msg.sender, address(this), rewards);
        
        totalProfitShares[poolId][lockedPeriod] += rewards;
        updateTotalLatestLocked(poolId);
        emit DistributeProfitSharing(poolId, lockedPeriod, rewards);
    }

    // getTotalUserWithdrawable.
    // Get how many wagon user can uncompound from this locked period.
    function getTotalUserWithdrawable(uint256 poolId, address _address) public isPoolAvailable(poolId) view returns(uint256 totalUserLocked, uint256 totalUserWithdrawable) {
        uint256 lockedPeriod = getCurrentLockedPeriod();
        if(lockedPeriod == 0) return (0, 0);

        totalUserLocked = getCurrentTotalUserLocked(poolId, _address);
        totalUserWithdrawable = totalUserLocked + calculateReward(poolId, lockedPeriod, totalUserLocked);
        totalUserWithdrawable -= withdraws[poolId][lockedPeriod][_address];
    }
    
    // removeAutoCompound.
    // Remove user's locked WAG and reward from auto compound.
    // @params _amount Amount of wagon to be uncompound
    function removeAutoCompound(uint256 poolId, uint256 _amount) public isPoolAvailable(poolId) isAbleToUncompound nonReentrant{
        require(_amount > 0, "Cannot withdraw 0");
        
        uint256 totalUserLocked = 0;
        uint256 totalUserWithdrawable = 0;
        
        (totalUserLocked, totalUserWithdrawable) = getTotalUserWithdrawable(poolId, msg.sender);
        require(_amount <= totalUserWithdrawable, "Not enough locked withdrawable");

        uint256 lockedPeriod = getCurrentLockedPeriod();
        withdraws[poolId][lockedPeriod][msg.sender] += _amount;
        totalPeriodWithdraws[poolId][lockedPeriod] += _amount;

        pendingWithdraws[poolId][msg.sender].amount += _amount;
        pendingWithdraws[poolId][msg.sender].claimableTimestamp = getLockTimePeriod(lockedPeriod + 1);

        updateUserLatestLockRemoveAutocompound(poolId, totalUserLocked);
        emit RemoveAutoCompound(poolId, msg.sender, lockedPeriod, _amount);
    }

    // getLockTimePeriod.
    // Get timestamp when a period will be locked
    // @params period
    function getLockTimePeriod(uint256 period) public view returns (uint256) {
        if(firstLockTime == 0) return 0;
        if(period < 1) return 0;

        return BokkyPooBahsDateTimeLibrary.addSeconds(firstLockTime , (period - 1) * lockDuration);
    }

    // withdraw.
    // Withdraw user's uncompounded WAG
    // @params _amount amount of WAG to be withdraw
    function withdraw(uint256 poolId, uint256 _amount) public isPoolAvailable(poolId) nonReentrant {
        require(_amount > 0, "Cannot withdraw 0");
        
        pendingWithdraw memory userPendingWithdrawable = pendingWithdraws[poolId][msg.sender];
        require(_amount <= userPendingWithdrawable.amount, "Not enough pending withdrawable");

        require(block.timestamp >= userPendingWithdrawable.claimableTimestamp, "Not yet time to withdraw");
        
        if (userPendingWithdrawable.amount - _amount == 0) {
            pendingWithdraws[poolId][msg.sender].claimableTimestamp = 0;
        }
        
        pendingWithdraws[poolId][msg.sender].amount -= _amount;

        wagon.transfer(msg.sender, _amount);
        updateUserLatestLock(poolId);
        emit Withdraw(poolId, msg.sender, _amount);
    }

}