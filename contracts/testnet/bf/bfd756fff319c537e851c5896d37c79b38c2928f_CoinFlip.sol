pragma solidity ^0.8.17;

import {SafeERC20, IERC20} from 'openzeppelin/token/ERC20/utils/SafeERC20.sol';
import 'openzeppelin/token/ERC20/ERC20.sol';
import {Ownable} from 'openzeppelin/access/Ownable.sol';
import '../sources/interfaces/IUniswapV2Factory.sol';
import '../sources/interfaces/IUniswapV2Router.sol';

interface IFlush is IERC20 {
    function mint(address to, uint256 amount) external;

    function burn(uint256 amount) external;
}

contract Flush is ERC20, Ownable {
    using SafeERC20 for IERC20;

    uint256 public constant INITIAL_SUPPLY = 100_000_000e18;

    address public bankroll;

    IUniswapV2Router immutable router;
    address public uniswapV2Pair;

    constructor(address _router) ERC20('Flush', 'FLUSH') {
        router = IUniswapV2Router(_router);
        uniswapV2Pair = IUniswapV2Factory(router.factory()).createPair(address(this), router.WETH());
        _mint(msg.sender, INITIAL_SUPPLY);
    }

    function setBankroll(address _bankroll) public onlyOwner {
        require(_bankroll != address(0), 'Flush: bankroll is the zero address');
        bankroll = _bankroll;
    }

    function mint(address to, uint256 amount) external payable {
        if (msg.sender != bankroll) {
            require(msg.value >= 1e15, 'Flush: mint fee is 0.001 ETH');
            _mint(to, 10000 ether);
        } else {
            _mint(to, amount);
        }
    }

    function burn(uint256 amount) external {
        _burn(msg.sender, amount);
    }

    function safeFlushTransfer(address _to, uint256 _amount) public onlyOwner {
        uint256 flushBalance = balanceOf(address(this));
        if (_amount > flushBalance) {
            _transfer(address(this), _to, flushBalance);
        } else {
            _transfer(address(this), _to, _amount);
        }
    }
}

