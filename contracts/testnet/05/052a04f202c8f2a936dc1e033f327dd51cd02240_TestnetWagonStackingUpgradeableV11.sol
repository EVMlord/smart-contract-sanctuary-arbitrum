    event DistributeProfitSharing(uint256 indexed pool, uint256 indexed period, uint256 amount);
    event CreatePool(uint256 indexed id, string name);
    event UpdatePool(uint256 indexed id, string name);
    event UpdateFirstLockTime(uint timestamp);
    event UpdateLockDuration(uint timestamp);
    event UpdateUncompoundDuration(uint timestamp);
    event RemoveAutoCompound(uint256 indexed pool, address indexed staker, uint indexed period, uint256 amount);
