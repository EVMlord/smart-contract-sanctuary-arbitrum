    }

    function getCostAmount(uint8 eSeries_) view external returns (uint256){
        return _costAmount[eSeries_];
    }

    /**
     * @notice 获取孵化随机数
     */
    function getHatchRandomNumber(uint8 eSeries_, bytes32 seed_, uint256 index_) public view returns (uint256){
        if(eSeries_ == 0){
            return uint256(seed_) % IL2UnicornRule(l2UnicornRule).modNumber();
        }else{
            bytes32 newSeed_ = keccak256(abi.encodePacked(seed_, bytes32(index_)));
            return uint256(newSeed_) % IL2UnicornRule(l2UnicornRule).modNumber();
        }
    }

    /**
     * @notice 查看孵化结果
     */
    function viewHatchResults(uint256 requestId_, uint8 startIndex_, uint8 endIndex_) external view returns (HatchResult[] memory hatchResults){
        uint8 hatchCount_ = _requestIdHatchCount[requestId_];
        uint8 eSeries_ = _requestIdESeries[requestId_];
        GameInfo memory gameInfo_ = _requestIdGameInfo[requestId_];
        //
        if (startIndex_ >= 0 && endIndex_ >= startIndex_) {
            uint256 len = endIndex_ + 1 - startIndex_;
            uint256 arrayLen = len > hatchCount_ ? hatchCount_ : len;
            hatchResults = new HatchResult[](arrayLen);
            uint256 arrayIndex_ = 0;
            for (uint8 index_ = startIndex_; index_ < ((endIndex_ > hatchCount_) ? hatchCount_ : endIndex_);) {
                hatchResults[arrayIndex_] = HatchResult(
                    requestId_,
                    gameInfo_.user,
                    gameInfo_.seed,
                    eSeries_,
                    index_,
                    getHatchRandomNumber(eSeries_, gameInfo_.seed, index_)
                );
                unchecked{++index_; ++arrayIndex_;}
            }
        }
        return hatchResults;
    }

}

