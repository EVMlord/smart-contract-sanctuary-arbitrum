        EMPTY,
        S_WAITING,
        S_FULFILLED,
        S_COMPLETED,
        S_REFUNDED,
        S_SLASHED,
        F_WAITING,
        F_PROCESSED,
        F_COMPLETED,
        F_EXPIRED,
        F_SLASHED
    }

    enum LockTypes {
        SWAP_LOCK,
        PENDING_REWARD
    }

    struct Route {
        uint32 src;
        uint32 dst;
    }

    struct ExecutableMessage {
        address requester;
        address tokenIn;
        address tokenOut;
        address callAddress;
        bytes callData; // includes amount, receiver etc.
    }

    struct SwapInfo {
        uint nonce;
        address boltRelayerAddr;
        Route route;
        uint crossAmount;
        uint totalAmount;
        ExecutableMessage srcMsg;
        ExecutableMessage dstMsg;
    }

    struct SwapState {
        uint64 startBlock;
        uint64 expiringBlock;
        Status status;
        address boltOperator;
    }

    struct StakedFunds {
        uint stakedAmount;
        uint lockedAmount;
        uint unstakingAmount;
        uint unstakeTime;
    }

    struct BoltRelayer {
        mapping(uint => mapping(address => StakedFunds)) stakes;
        uint unstakeEnableBlock;
        // bytes[] extraAddresses; // placeholder
    }
}

