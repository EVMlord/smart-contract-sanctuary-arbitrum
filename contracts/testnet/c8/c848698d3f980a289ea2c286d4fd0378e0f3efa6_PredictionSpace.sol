import "@chainlink/contracts/src/v0.8/interfaces/AggregatorV3Interface.sol";
import "@openzeppelin/contracts-upgradeable/security/PausableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "./IPredictionSpace.sol";
import "./Prediction.sol";
import "./DateTime.sol";

import "hardhat/console.sol";


    struct PredictPair {
        address upper;
        address lower;
        uint256 value;
    }

    struct PredictionResponse {
        Prediction prediction;
        PredictPair[] pairs;
    }

contract PredictionSpace is IPredictionSpace, Initializable, PausableUpgradeable, OwnableUpgradeable {

    uint public makerFee;
    uint public platformFee;
    address public cronjobAddress;

    // List prediction
    Prediction[] public predictions;
    // Store price by timestamp (timestamp => (pair address => price))
    mapping(uint => mapping(address => uint256)) pricesByTime;
    // Mapping prediction list by timestamp (timestamp => array of prediction index
    mapping(uint => uint[]) predictionsByTime;
    // Mapping predictors (prediction index => array of predict pairs
    mapping(uint => PredictPair[]) public predictors;

    /**
     * Network: Arb Goerli
     * Aggregator: BTC/USD
     * Address: 0x6550bc2301936011c1334555e62A87705A81C12C
     */
    /// @custom:oz-upgrades-unsafe-allow constructor
    constructor() {
        _disableInitializers();
    }

    function initialize() public initializer {
        __Pausable_init();
        __Ownable_init();
        platformFee = 1 * 10 ** 8;
        makerFee = 5 * 10 ** 8;
    }

    function pause() public onlyOwner {
        _pause();
    }

    function unpause() public onlyOwner {
        _unpause();
    }

    function setCronjobAddress(address cronAddress) public {
        cronjobAddress = cronAddress;
    }

    function create(address _pair, uint256 _price, uint _time) public returns (Prediction memory) {
        require(msg.sender != address(0), "Invalid caller");
        require(_pair != address(0), "Invalid pair");
        uint _now = block.timestamp;
        require(_time - _now >= 6 hours, "Invalid time: time required greater than now at least 6 hours");
        require(_time % 60 == 0 && (_time / 60) % 60 == 0, "Invalid time: time only allow hours");
        uint _endTime = (_now + (_time - _now) / 2);
        Prediction memory p = Prediction({pair : _pair, price : _price, time : _time, endTime : _endTime, owner : msg.sender, volumes : 0});
        // Push prediction instance into list
        predictions.push(p);
        // Push prediction index into map
        predictionsByTime[_time].push(predictions.length - 1);
        emit Create(msg.sender, _pair, _price, _time);
        return p;
    }

    function predict(uint _predictionIndex, uint _pairIndex, bool _upper) external payable {
        require(_predictionIndex >= 0 && _predictionIndex < predictions.length, "Invalid prediction id");
        require(_pairIndex >= 0 && _pairIndex <= predictors[_predictionIndex].length, "Invalid pair index");
        require(msg.value == 0.01 ether || msg.value == 0.05 ether || msg.value == 0.1 ether, "Invalid value");
        Prediction memory prediction = predictions[_predictionIndex];
        require(block.timestamp < prediction.endTime, "Invalid request: cannot predict now");
        if (_pairIndex > 0) {
            PredictPair memory _pair = predictors[_predictionIndex][_pairIndex - 1];
            require(msg.value == _pair.value, "Invalid value");
            if (_upper) {
                require(_pair.upper == address(0), "Invalid predict: Upper has been predicted");
                _pair.upper = msg.sender;
            } else {
                require(_pair.lower == address(0), "Invalid predict: Lower has been predicted");
                _pair.lower = msg.sender;
            }
        } else {
            PredictPair memory _pair = PredictPair({
            upper : address(0),
            value : msg.value,
            lower : address(0)
            });
            if (_upper) _pair.upper = msg.sender;
            else _pair.lower = msg.sender;
            predictors[_predictionIndex].push(_pair);
        }
        prediction.volumes += msg.value;
        emit Predict(msg.sender, _predictionIndex, msg.value, _upper);
    }

    function getPredictions() external view returns (PredictionResponse[] memory) {
        uint _now = DateTime.addHours(block.timestamp, 1);
        uint year; uint month; uint day; uint hour; uint minute; uint second;
        (year, month, day, hour, minute, second) = DateTime.timestampToDateTime(_now);
        _now = DateTime.timestampFromDateTime(year, month, day, hour, 0, 0);
        PredictionResponse[] memory results = new PredictionResponse[](predictions.length);
        uint len = 0;
        for (uint i = 0; i <= 720; i++) {
            uint _time = DateTime.addHours(_now, i);
            uint[] memory predictionIndexes = predictionsByTime[_time];
            for (uint j = 0; j < predictionIndexes.length; j++) {
                uint index = predictionIndexes[j];
                results[len] = PredictionResponse(predictions[index], predictors[index]);
                len++;
            }
        }
        return results;
    }

    function getPredictPair(uint predictionId) external view returns (PredictPair[] memory) {
        return predictors[predictionId];
    }

    // For cronjob only
    function runCronJobGetPrice() public {
        require(msg.sender == cronjobAddress, "Invalid caller for cron job");
    }

    event Received(address, uint);
    event Predict(address, uint, uint, bool);
    event Create(address, address, uint256, uint);

    receive() external payable {
        emit Received(msg.sender, msg.value);
    }
}

