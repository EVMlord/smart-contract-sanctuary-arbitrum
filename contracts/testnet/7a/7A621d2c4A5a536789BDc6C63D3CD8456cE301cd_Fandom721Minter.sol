pragma solidity ^0.8.0;

import "@openzeppelin/contracts/access/AccessControl.sol";
import "@openzeppelin/contracts/utils/Context.sol";

import "../Base/Pickdol721.sol";
import "./Fandom721Codec.sol";

contract Fandom721Minter is 
    Context, 
    AccessControl
{
    bytes32 public constant MANAGER_ROLE = keccak256("MANAGER_ROLE");
    bytes32 public constant MINTER_ROLE = keccak256("MINTER_ROLE");
    bytes32 public constant DATA_ROLE = keccak256("DATA_ROLE");

    address public _fandom721;
    
    constructor(address fandom721) {
        _fandom721 = fandom721;

        _setupRole(DEFAULT_ADMIN_ROLE, _msgSender());

        _setupRole(MANAGER_ROLE, _msgSender());
        _setupRole(MINTER_ROLE, _msgSender());
        _setupRole(DATA_ROLE, _msgSender());
    }

    function mintFandom721(address toAddr, FandomFixedData memory data) external {
        require(hasRole(MINTER_ROLE, _msgSender()), "Fandom721Minter: FORBIDDEN");

        Fandom721Codec codec = Fandom721Codec(Pickdol721(_fandom721)._codec());
        bytes memory bydata = codec.encodeFandomFixedData(data);

        Pickdol721(_fandom721).mint(toAddr, bydata);
    }

    function getFandomFixedData(uint256 tokenId) external view returns(FandomFixedData memory data){
        bytes memory bydata = Pickdol721(_fandom721).getFixedData(tokenId);

        Fandom721Codec codec = Fandom721Codec(Pickdol721(_fandom721)._codec());
        return codec.decodeFandomFixedData(bydata);
    }

    function getFandomAttr(uint256 tokenId) external view returns(FandomAttr memory data) {
        bytes memory bydata = Pickdol721(_fandom721).getWriteabledData(tokenId, "attr");

        console.log("[sol]getFandomAttr buffer.length=",bydata.length);

        Fandom721Codec codec = Fandom721Codec(Pickdol721(_fandom721)._codec());
        return codec.decodeFandomAttr(bydata);
    }

    function modifyFandomAttr(uint256 tokenId, FandomAttr memory data) external {
        require(hasRole(DATA_ROLE, _msgSender()), "Fandom721Minter: FORBIDDEN");

        Fandom721Codec codec = Fandom721Codec(Pickdol721(_fandom721)._codec());
        bytes memory bydata = codec.encodeFandomAttr(data);

        console.log("[sol]modifyFandomAttr buffer.length=",bydata.length);

        Pickdol721(_fandom721).modifyWriteableData(tokenId, "attr", bydata);
    }
}

