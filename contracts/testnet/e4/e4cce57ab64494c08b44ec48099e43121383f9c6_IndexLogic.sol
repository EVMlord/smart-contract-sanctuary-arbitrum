        external
        view
        override
        returns (uint256 assets, uint256 fee)
    {
        fee = (indexShares * BURN_FEE) / (BP.DECIMAL_FACTOR + BURN_FEE);
        assets = convertToAssets(indexShares - fee, totalSupply);
    }

    /// @inheritdoc IIndexLogic
    function calculateAUMFee(uint256 totalSupplyWithoutFee, uint32 lastTransferTimestamp)
        external
        view
        override
        returns (uint256 fee)
    {
        uint256 timePassed = uint32(block.timestamp) - lastTransferTimestamp;
        if (timePassed != 0) {
            fee = totalSupplyWithoutFee.mulDivDown(
                FixedPointMathLib.rpow(AUM_SCALED_PER_SECOND_RATE, timePassed, AUMCalculationLibrary.RATE_SCALE_BASE)
                    - AUMCalculationLibrary.RATE_SCALE_BASE,
                AUMCalculationLibrary.RATE_SCALE_BASE
            );
        }
    }

    /// @inheritdoc IIndexLogic
    function convertToShares(uint256 reserveTokens, uint256 totalSupply) public view override returns (uint256) {
        return totalSupply == 0
            ? reserveTokens * PRICE_SCALING_FACTOR
            : reserveTokens.mulDivDown(totalSupply, totalAssets());
    }

    /// @inheritdoc IIndexLogic
    function convertToAssets(uint256 indexShares, uint256 totalSupply) public view override returns (uint256) {
        return totalSupply == 0 ? indexShares : indexShares.mulDivDown(totalAssets(), totalSupply);
    }

    /// @inheritdoc IIndexLogic
    function totalAssets() public view override returns (uint256 total) {
        total = IReserveVault(reserveVault).reserve();

        ConstituentLib.Constituent[] memory constituents_ = _constituents.values();
        UFixed128[] memory prices = priceSource.prices(constituents_);
        uint256 length = constituents_.length;
        for (uint256 i; i < length;) {
            total += constituents_[i].balance.convertToBase(prices[i]);
            unchecked {
                i = i + 1;
            }
        }
    }

