    //增加NFT道具配方,  约定0-采集器，1-载具
    function addMaterial(
            uint256 uruFee,
            string memory imageUri,
            address factoryContractAddress,
            uint256[] memory ids,
            uint256[] memory quantities
        ) public onlyOperator returns(uint256){
        require(ids.length == quantities.length, "ids and quantities length is not equal");
        uint256 length = ids.length;

        uint256 nftTypeIndex = _nftMaterialTypeIndexTracker.current();
        _nftMaterialTypeIndexTracker.increment();

        Material memory material;
        material.enabled = true;
        material.UruFee = uruFee;
        material.ImageUri = imageUri;
        material.FactoryContractAddress = factoryContractAddress;

        material.MaterialIds = new uint[](length);
        material.MaterialFees = new uint[](length);

        for (uint256 i = 0; i < ids.length; i++) {
            material.MaterialIds[i] = ids[i];
            material.MaterialFees[i] = quantities[i];
        }

        _materials[nftTypeIndex] = material; 

        emit OnAddMaterial(nftTypeIndex);
        return nftTypeIndex;
    }

