pragma solidity ^0.8.18;

import {ReentrancyGuard} from "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import {SafeERC20} from "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
import {IERC20} from "@openzeppelin/contracts/token/ERC20/IERC20.sol";

import {ITradingEngine, IncreasePositionParams, DecreasePositionParams} from "../../interfaces/ITradingEngine.sol";
import {IStandardPriceFeed, INoSpreadPriceFeed} from "../../interfaces/IPriceFeed.sol";
import {Order, OrderType, OrderExtra} from "../../interfaces/IExchange.sol";
import {IFeeManager} from "../../interfaces/IFeeManager.sol";
import {PriceFeedType} from "../../interfaces/IMarket.sol";
import {PositionUtils} from "../position/PositionUtils.sol";
import {SafeTransfer} from "../../common/SafeTransfer.sol";
import {Constants} from "../../common/Constants.sol";
import {IWETH} from "../../interfaces/IWETH.sol";
import {Errors} from "../../common/Errors.sol";
import {PriceValidator} from "./PriceValidator.sol";
import {console} from "forge-std/console.sol";

contract Exchange is PriceValidator, ReentrancyGuard {
    ITradingEngine public engine;
    IFeeManager public feeManager;
    IWETH public weth;

    address public gov;
    uint256 public nextOrderId = 1;
    uint64 public minExecutionFee;
    // every time an order associated with a key is closed
    mapping(uint256 => Order) public orders;

    event OpenMarketOrderPlaced(address indexed account);
    event CloseMarketOrderPlaced(address indexed account);
    event OrderPlaced(
        bytes32 indexed marketId,
        OrderType indexed orderType,
        address indexed account,
        uint256 orderId
    );
    // event OrderExecuted(address indexed account, uint256 indexed orderId, uint256 triggerPrice);
    event OrderExecuted(address indexed account, uint256 indexed orderId, address indexed executor);
    event OrderCancelled(address indexed account, uint256 indexed orderId);

    using SafeERC20 for IERC20;
    using SafeTransfer for IERC20;

    constructor(ITradingEngine _engine, IWETH _weth, uint64 _minExecutionFee) {
        gov = msg.sender;
        engine = _engine;
        weth = _weth;
        minExecutionFee = _minExecutionFee;
    }

    modifier onlyGov() {
        require(msg.sender == gov, "Exchange: onlyGov");
        _;
    }

    function lastOrderId() external view returns (uint256) {
        return nextOrderId - 1;
    }

    function setFeeManager(IFeeManager _feeManager) external onlyGov {
        feeManager = _feeManager;
    }

    function placeOrder(
        OrderType orderType,
        bool isLong,
        bytes32 marketId,
        address collateralToken,
        uint256 collateralAmount,
        uint256 sizeDelta,
        bytes32 /*referralCode*/,
        OrderExtra calldata extra
    ) external payable nonReentrant {
        require(orderType != OrderType.TP_SL, "TP/SL for close only");

        if (orderType == OrderType.MARKET) {
            _placeMarketOrder(
                isLong,
                marketId,
                collateralToken,
                collateralAmount,
                sizeDelta,
                bytes32(0),
                extra
            );
        } else {
            _placeOrder(
                orderType,
                isLong,
                marketId,
                collateralToken,
                collateralAmount,
                sizeDelta,
                bytes32(0),
                extra
            );
        }
    }

    function _placeMarketOrder(
        bool isLong,
        bytes32 marketId,
        address collateralToken,
        uint256 collateralAmount,
        uint256 sizeDelta,
        bytes32 /*referralCode*/,
        OrderExtra calldata extra
    ) internal {
        uint256 amountIn = _transferIn(collateralToken, collateralAmount);
        if (amountIn < collateralAmount) {
            revert Errors.InvalidCollateralAmount();
        }

        uint256 executionFee;
        if (collateralToken == Constants.ETH_ADDRESS) {
            executionFee = msg.value - collateralAmount;
        } else {
            executionFee = msg.value;
        }

        require(executionFee >= _getExecutionFee(extra), Errors.LOW_EXECUTION_FEE);

        (address vault, uint256 feeUsd, uint256 feeTokens) = engine.getVaultAndPositionFee(
            marketId,
            collateralToken,
            true,
            sizeDelta
        );

        require(feeTokens < amountIn, "Invalid collateral amount");
        IERC20(collateralToken).transferTo(address(feeManager), feeTokens);
        IERC20(collateralToken).transferTo(vault, amountIn - feeTokens);

        // TODO: emit fees collected event
        feeManager.updateFee(marketId, collateralToken, feeTokens);

        engine.increasePosition(
            IncreasePositionParams({
                marketId: marketId,
                sizeDelta: sizeDelta,
                initialCollateralAmount: collateralAmount,
                openFee: feeUsd,
                account: msg.sender,
                collateralToken: collateralToken,
                isLong: isLong
            })
        );

        emit OpenMarketOrderPlaced(msg.sender);

        // WARNING: Need to optimize this as it cost over 1 million gas
        if (extra.tpPrice > 0 || extra.slPrice > 0) {
            _createTpSlSubOrders(
                Order({
                    marketId: marketId,
                    account: msg.sender,
                    collateralToken: collateralToken,
                    collateralAmount: amountIn,
                    sizeDelta: sizeDelta,
                    orderType: OrderType.MARKET,
                    isLong: isLong,
                    open: true,
                    submissionBlock: 0,
                    submissionTimestamp: 0,
                    executionFee: 0,
                    minExecutionFee: minExecutionFee,
                    extra: extra
                })
            );
        }
    }

    function _closeMarketOrder(
        bool isLong,
        bytes32 marketId,
        address collateralToken,
        uint256 sizeDelta
    ) internal {
        DecreasePositionParams memory params = DecreasePositionParams({
            marketId: marketId,
            account: msg.sender,
            collateralToken: collateralToken,
            sizeDelta: sizeDelta,
            isLong: isLong
        });

        engine.decreasePosition(params);
        emit CloseMarketOrderPlaced(msg.sender);
    }

    function _createTpSlSubOrders(Order memory parentOrder) internal {
        _validateTpSlPrice(
            parentOrder.isLong,
            parentOrder.extra.tpPrice,
            parentOrder.extra.slPrice
        );

        uint256 orderId = nextOrderId;
        uint64 executionFee;

        if (parentOrder.extra.tpPrice > 0) {
            executionFee += minExecutionFee;
        }

        if (parentOrder.extra.slPrice > 0) {
            executionFee += minExecutionFee;
        }

        Order memory order = Order({
            marketId: parentOrder.marketId,
            account: msg.sender,
            collateralToken: parentOrder.collateralToken,
            collateralAmount: 0,
            sizeDelta: parentOrder.sizeDelta,
            orderType: OrderType.TP_SL,
            isLong: parentOrder.isLong,
            open: false,
            submissionBlock: uint64(block.number),
            submissionTimestamp: uint64(block.timestamp),
            executionFee: executionFee,
            minExecutionFee: minExecutionFee,
            extra: OrderExtra({
                triggerPrice: 0,
                tpPrice: parentOrder.extra.tpPrice,
                tpDelta: parentOrder.extra.tpDelta > 0
                    ? parentOrder.extra.tpDelta
                    : parentOrder.sizeDelta,
                slPrice: parentOrder.extra.slPrice,
                slDelta: parentOrder.extra.slDelta > 0
                    ? parentOrder.extra.slDelta
                    : parentOrder.sizeDelta
            })
        });

        orders[orderId] = order;
        nextOrderId = orderId + 1;
        emit OrderPlaced(order.marketId, order.orderType, msg.sender, orderId);
    }

    function _getExecutionFee(
        OrderExtra calldata extra
    ) internal view returns (uint256 totalExecutionFee) {
        if (extra.triggerPrice > 0) {
            totalExecutionFee += minExecutionFee;
        }

        // tp will create a suborder
        if (extra.tpPrice > 0) {
            totalExecutionFee += minExecutionFee;
        }

        // sl will create a suborder
        if (extra.slPrice > 0) {
            totalExecutionFee += minExecutionFee;
        }
    }

    function _placeOrder(
        OrderType orderType,
        bool isLong,
        bytes32 marketId,
        address collateralToken,
        uint256 collateralAmount,
        uint256 sizeDelta,
        bytes32 /*referralCode*/,
        OrderExtra calldata extra
    ) internal {
        require(sizeDelta > 0, "Exchange:S>0");
        engine.validatePositionSize(marketId, collateralToken, sizeDelta);

        uint256 executionFee;
        if (collateralToken == Constants.ETH_ADDRESS) {
            executionFee = msg.value - collateralAmount;
        } else {
            executionFee = msg.value;
        }

        require(extra.triggerPrice > 0, "Exchange:Invalid trigger price");
        require(executionFee >= _getExecutionFee(extra), Errors.LOW_EXECUTION_FEE);
        require(
            _transferIn(collateralToken, collateralAmount) >= collateralAmount,
            "Exchange: invalid collateral amount"
        );

        Order memory order = Order({
            marketId: marketId,
            account: msg.sender,
            collateralToken: collateralToken == Constants.ETH_ADDRESS
                ? address(weth)
                : collateralToken,
            collateralAmount: collateralAmount,
            sizeDelta: sizeDelta,
            orderType: orderType,
            isLong: isLong,
            open: true,
            submissionBlock: uint64(block.number),
            submissionTimestamp: uint64(block.timestamp),
            executionFee: minExecutionFee,
            minExecutionFee: minExecutionFee,
            extra: extra
        });

        uint256 orderId = nextOrderId;
        orders[orderId] = order;
        nextOrderId += 1;
        emit OrderPlaced(marketId, order.orderType, msg.sender, orderId);
    }

    function _closeOrder(
        OrderType orderType,
        bool isLong,
        bytes32 marketId,
        address collateralToken,
        uint256 sizeDelta,
        OrderExtra calldata extra
    ) internal {
        if (orderType != OrderType.TP_SL) {
            require(extra.triggerPrice > 0, "Exchange:Invalid trigger price");
        }

        uint256 executionFee = msg.value;
        require(executionFee >= _getExecutionFee(extra), "Exchange: low executionFee");

        Order memory order = Order({
            marketId: marketId,
            account: msg.sender,
            collateralToken: collateralToken,
            collateralAmount: 0,
            sizeDelta: sizeDelta,
            executionFee: minExecutionFee,
            minExecutionFee: minExecutionFee,
            submissionBlock: uint64(block.number),
            submissionTimestamp: uint64(block.timestamp),
            orderType: orderType,
            isLong: isLong,
            open: false,
            extra: extra
        });

        uint256 orderId = nextOrderId;
        orders[orderId] = order;
        nextOrderId += 1;
        emit OrderPlaced(marketId, order.orderType, msg.sender, orderId);
    }

    function closeOrder(
        OrderType orderType,
        bool isLong,
        bytes32 marketId,
        address collateralToken,
        uint256 sizeDelta,
        OrderExtra calldata extra
    ) external payable nonReentrant {
        if (orderType == OrderType.MARKET) {
            _closeMarketOrder(isLong, marketId, collateralToken, sizeDelta);
        } else {
            _closeOrder(orderType, isLong, marketId, collateralToken, sizeDelta, extra);
        }
    }

    function getOrder(uint256 orderId) external view returns (Order memory) {
        return orders[orderId];
    }

    function _transferIn(address _token, uint256 _amount) internal returns (uint256) {
        if (_token == Constants.ETH_ADDRESS) {
            weth.deposit{value: _amount}();
            return _amount;
        }

        IERC20 token = IERC20(_token);

        uint256 balance = token.balanceOf(address(this));
        token.safeTransferFrom(msg.sender, address(this), _amount);
        return token.balanceOf(address(this)) - balance;
    }

    function _isTpTrigger(
        bool isLong,
        uint256 indexPrice,
        uint96 tpPrice
    ) internal pure returns (bool isTpTrigger) {
        if (isLong) {
            isTpTrigger = indexPrice >= tpPrice;
        } else {
            isTpTrigger = indexPrice <= tpPrice;
        }
    }

    function _isSlTrigger(
        bool isLong,
        uint256 indexPrice,
        uint96 slPrice
    ) internal pure returns (bool isSlTrigger) {
        if (isLong) {
            isSlTrigger = indexPrice <= slPrice;
        } else {
            isSlTrigger = indexPrice >= slPrice;
        }
    }

    function _executeOrder(uint256 orderId, address payable feeTo, uint256 price) internal {
        Order memory order = orders[orderId];
        // require(block.number > order.submissionBlock, "OrderBook:block not pass");

        if (order.open) {
            (address vault, uint256 inUsd, uint256 inTokens) = engine.getVaultAndPositionFee(
                order.marketId,
                order.collateralToken,
                order.open,
                order.sizeDelta
            );

            IERC20(order.collateralToken).transferTo(address(feeManager), inTokens);
            IERC20(order.collateralToken).transferTo(vault, order.collateralAmount - inTokens);

            feeManager.updateFee(order.marketId, order.collateralToken, inTokens);

            engine.increasePosition(
                IncreasePositionParams({
                    marketId: order.marketId,
                    sizeDelta: order.sizeDelta,
                    initialCollateralAmount: order.collateralAmount,
                    openFee: inUsd,
                    account: order.account,
                    collateralToken: order.collateralToken,
                    isLong: order.isLong
                })
            );

            if (order.extra.tpPrice > 0 || order.extra.slPrice > 0) {
                _createTpSlSubOrders(order);
            }

            delete orders[orderId];
            emit OrderExecuted(order.account, orderId, msg.sender);
        } else {
            if (order.extra.tpPrice > 0 || order.extra.slPrice > 0) {
                _executeTpSlOrder(order, price, orderId);
            } else {
                // decrease psoition
                engine.decreasePosition(
                    DecreasePositionParams({
                        marketId: order.marketId,
                        account: order.account,
                        collateralToken: order.collateralToken,
                        sizeDelta: order.sizeDelta,
                        isLong: order.isLong
                    })
                );
                delete orders[orderId];
                emit OrderExecuted(order.account, orderId, msg.sender);
            }
        }
        // transfer execution fees to feeTo
        SafeTransfer.safeTransferETH(feeTo, order.minExecutionFee);
    }

    function _executeTpSlOrder(Order memory order, uint256 indexPrice, uint256 orderId) internal {
        uint256 sizeDelta;

        bool isTpTrigger = _isTpTrigger(order.isLong, indexPrice, order.extra.tpPrice);
        bool isSlTrigger = _isSlTrigger(order.isLong, indexPrice, order.extra.slPrice);

        if (isTpTrigger) {
            sizeDelta = order.extra.tpDelta;
        }

        if (isSlTrigger) {
            sizeDelta = order.extra.slDelta;
        }

        engine.decreasePosition(
            DecreasePositionParams({
                marketId: order.marketId,
                account: order.account,
                collateralToken: order.collateralToken,
                sizeDelta: sizeDelta,
                isLong: order.isLong
            })
        );
        emit OrderExecuted(order.account, orderId, msg.sender);
        order.executionFee -= order.minExecutionFee;

        bytes32 key = PositionUtils.getPositionKey(
            order.marketId,
            order.account,
            order.collateralToken,
            order.isLong
        );
        uint256 size = engine.getPositionSize(key);
        // if a position is till in the market and this order has both stop loss and tp
        // remove the side that was just executed

        if (size != 0 && order.extra.tpPrice > 0 && order.extra.slPrice > 0) {
            if (isTpTrigger) {
                // remove tp price after executed
                order.extra.tpPrice = 0;
                order.extra.tpDelta = 0;
            } else {
                order.extra.slPrice = 0;
                order.extra.slDelta = 0;
            }

            uint256 newOrderId = nextOrderId;
            orders[newOrderId] = order;
            nextOrderId += 1;
            // create a new order
            emit OrderPlaced(order.marketId, order.orderType, order.account, newOrderId);
        } else {
            if (order.executionFee > 0) {
                // refund user if all execution fees are not used up
                SafeTransfer.safeTransferETH(order.account, order.executionFee);
            }
        }
        delete orders[orderId];
    }

    function executeOrder(uint256 orderId, address payable feeTo) external nonReentrant {
        Order memory order = orders[orderId];
        require(order.sizeDelta > 0, Errors.ORDER_NOT_FOUND);

        (PriceFeedType feedType, address priceFeed) = engine.getPriceFeed(order.marketId);

        uint256 price = _getPrice(
            feedType,
            priceFeed,
            engine.getIndexToken(order.marketId),
            order.isLong
        );

        if (order.orderType == OrderType.LIMIT) {
            _validateLimitExecution(order.open, order.isLong, price, order.extra.triggerPrice);
        } else if (order.orderType == OrderType.STOP_MARKET) {
            _validateStopMarketExecution(order.open, order.isLong, price, order.extra.triggerPrice);
        } else if (order.orderType == OrderType.TP_SL) {
            _validateTpSlExecution(order.isLong, price, order.extra.tpPrice, order.extra.slPrice);
        }

        _executeOrder(orderId, feeTo, price);
    }

    function cancelOrder(uint256 orderId) external nonReentrant {
        require(orders[orderId].sizeDelta > 0, Errors.ORDER_NOT_FOUND);
        require(msg.sender == orders[orderId].account, Errors.NOT_ORDER_OWNER);
        Order memory order = orders[orderId];
        address collateralToken = order.collateralToken;

        // only refund if this is an open order
        if (order.open) {
            IERC20(collateralToken).transferTo(msg.sender, order.collateralAmount);
        }

        SafeTransfer.safeTransferETH(msg.sender, order.executionFee);

        delete orders[orderId];
        emit OrderCancelled(msg.sender, orderId);
    }

    function _getPrice(
        PriceFeedType feedType,
        address priceFeed,
        address token,
        bool isMax
    ) internal view returns (uint256) {
        uint256 price;
        if (feedType == PriceFeedType.Standard) {
            price = IStandardPriceFeed(priceFeed).getPrice(token, isMax);
        } else if (feedType == PriceFeedType.StandardNoSpread) {
            price = INoSpreadPriceFeed(priceFeed).getPrice(token);
        }

        require(price > 0, "TradingEngine::INVALID_PRICE");
        return price;
    }
}

