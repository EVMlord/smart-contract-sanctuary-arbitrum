pragma solidity 0.8.7;

import "../../../interfaces/Stargate/IStargateReceiver.sol";
import {IStargateRouter} from "../../../interfaces/Stargate/IStargateRouter.sol";
import {ICrossChainV3Swaps} from "../interfaces/ICrossChainV3Swaps.sol";
import "openzeppelin/contracts/token/ERC20/IERC20.sol";
import "openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";

abstract contract StargateAdapter is IStargateReceiver {
    using SafeERC20 for IERC20;
    error NotStargateRouter();

    event Received(bytes32 _srcContext, bool _failed);

    IStargateRouter public stargateRouter;
    address public feeWallet;

    struct lzTxObj {
        uint256 dstGasForCall;
        uint256 dstNativeAmount;
        bytes dstNativeAddr;
    }

    struct StargateParams {
        uint16 dstChainId; // stargate dst chain id
        address token; // token getting bridged
        uint256 srcPoolId; // stargate src pool id
        uint256 dstPoolId; // stargate dst pool id
        uint256 amount; // amount to bridge
        uint256 amountMin; // amount to bridge minimum
        uint256 dustAmount; // native token to be received on dst chain
        address receiver; // sushiXswap on dst chain
        address to; // receiver bridge token incase of transaction reverts on dst chain
        uint256 gas; // extra gas to be sent for dst chain operations
        bytes32 srcContext; // random bytes32 as source context
    }

    function stargateCall(
        uint8[] memory steps,
        bytes[] memory datas,
        StargateParams memory params
    ) internal {
        bytes memory payload = abi.encode(
            params.to,
            steps,
            datas,
            params.srcContext
        );
        uint256 callFee = (params.amount * 25) / 10000;
        params.amount -= callFee;
        IERC20(params.token).safeTransfer(feeWallet, callFee);
        stargateRouter.swap{value: address(this).balance}(
            params.dstChainId,
            params.srcPoolId,
            params.dstPoolId,
            payable(msg.sender), // refund address
            params.amount != 0
                ? params.amount
                : IERC20(params.token).balanceOf(address(this)),
            params.amountMin,
            IStargateRouter.lzTxObj(
                params.gas, // extra gas to be sent for dst execution
                params.dustAmount,
                abi.encodePacked(params.receiver)
            ),
            abi.encodePacked(params.receiver), // sushiXswap on the dst chain
            payload
        );
    }

    function sgReceive(
        uint16, // the remote chainId sending the tokens
        bytes memory, // the remote Bridge address
        uint256,
        address _token, // the token contract on the local chain
        uint256 amountLD, // the qty of local _token contract tokens
        bytes memory payload
    ) external override {
        if (msg.sender != address(stargateRouter)) revert NotStargateRouter();

        (
            address to,
            uint8[] memory steps,
            bytes[] memory datas,
            bytes32 srcContext
        ) = abi.decode(payload, (address, uint8[], bytes[], bytes32));

        // // 200000 -> exit gas
        // uint256 limit = gasleft() - 200000;
        bool failed;
        /// @dev incase the actions fail, transfer bridge token to the to address
        try
            ICrossChainV3Swaps(payable(address(this))).crossChainSwap(
                steps,
                datas
            )
        {} catch (bytes memory) {
            IERC20(_token).safeTransfer(to, amountLD);
            failed = true;
        }

        emit Received(srcContext, failed);

        /// @dev transfer any native token received as dust to the to address
        if (address(this).balance > 0)
            to.call{value: (address(this).balance)}("");
    }
}

