pragma solidity ^0.8.18;

enum OrderType {
    MARKET,
    LIMIT,
    STOP_MARKET,
    TP_SL // take profit, stop loss. This is only applicable for close order
}

struct OrderExtra {
    // triggerPrice is limit price for limit order and stop price for stop order
    uint96 triggerPrice;
    uint96 tpPrice; // take-profit price. decimals = 18. only valid when flags.POSITION_TPSL_STRATEGY.
    uint96 slPrice; // stop-loss price. decimals = 18. only valid when flags.POSITION_TPSL_STRATEGY.
    uint256 tpDelta; // delta of position size to close
    uint256 slDelta; // delta of position size to close
}

struct Order {
    bytes32 marketId;
    uint256 collateralAmount;
    /// @notice when increase, collateralAmount is desired amount of collateral used as margin.
    /// When decrease, collateralAmount is value in USD of collateral user want to reduce from
    uint256 sizeDelta;
    /// @notice To prevent front-running, order MUST be executed on next block
    uint64 submissionBlock;
    uint64 submissionTimestamp;
    uint64 executionFee;
    // execution fee of the protocol could change anytime so we need to keep track
    // of what min execution fee is used for this order
    uint64 minExecutionFee;
    address account;
    address collateralToken; // long or short
    OrderType orderType;
    bool isLong;
    bool open;
    OrderExtra extra;
    // extra data for each order type
}

interface IExchange {
    function placeOrder(
        OrderType orderType,
        bool isLong,
        bytes32 marketId,
        address collateralToken,
        uint256 collateralAmount,
        uint256 sizeDelta,
        bytes32 /*referralCode*/,
        OrderExtra calldata extra
    ) external payable;

    function closeOrder(
        OrderType orderType,
        bool isLong,
        bytes32 marketId,
        address collateralToken,
        uint256 sizeDelta,
        OrderExtra calldata extra
    ) external payable;

    function executeOrder(uint256 orderId, address payable feeTo) external;

    function lastOrderId() external view returns (uint256);
}
