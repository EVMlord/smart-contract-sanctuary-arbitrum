pragma solidity ^0.8.0;

import "hardhat/console.sol";
import "@openzeppelin/contracts/access/AccessControl.sol";
import "@openzeppelin/contracts/security/PullPayment.sol";

contract VerifiedSale is AccessControl, PullPayment {

    /**
     * @dev Roles for Access Control.
     */
    bytes32 public constant SELLER = keccak256("SELLER");
    bytes32 public constant BUYER = keccak256("BUYER");
    bytes32 public constant VERIFIER = keccak256("VERIFIER");

    struct Order {
        address seller;
        address buyer;
        Item[] items;
        uint256 totalPrice;
        uint256 timestampCreatedAt;
        Shipment shipment;
    }

    struct Item {
        uint256 id;
        uint256 unitPrice;
        uint256 quantity;
    }

    struct Shipment {
        address courier;
        uint256 expectedDeliveryDate;
        uint256 actualDeliveryDate;
    }

}

