import "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/security/ReentrancyGuardUpgradeable.sol";
import "./interfaces/IWETH.sol";
import "./interfaces/IPostTechProfile.sol";
import "hardhat/console.sol";
import "./interfaces/IPostTechMarginProfiles.sol";

contract PostTechMarginProfiles is
    OwnableUpgradeable,
    ReentrancyGuardUpgradeable,
    IPostTechMarginProfiles
{
    uint256 public maxLeverageProfile;
    IWETH public weth;
    IPostTechProfile public postTechProfile;

    IPostTechProfile public postTechProfileReader;

    /// Fee percentage on loan value
    uint256 public marginFeePercentage;

    /// Fee interval on loan value
    uint256 public marginFeeInterval;
    uint256 public basePercentage;
    uint128 public baseLeverage;
    uint256 public thresholdLiquidation;
    address public feeDestinationMarginProtocol;
    uint256 public liquidatorRewardPercentage;

    uint64 public indexId;

    uint256 public liquidity;

    uint256 public liquidityBorrowed;

    mapping(address => bool) public handlers;

    mapping(address => bool) public whiteListTransferEth;

    uint256 public thresholdLiquidityBorrowed;

    mapping(address => mapping(address => MarginProfile)) public marginProfiles;
    struct MarginProfile {
        uint256 marginValue;
        uint256 loanValue;
        uint128 amount;
        uint128 leverage;
        uint128 marginValueAdded;
        uint64 lastTimeTakeFee;
        uint64 id;
    }

    modifier onlyOperator() {
        require(handlers[msg.sender], "MarginProfiles: not hander");
        _;
    }

    function initialize(
        address _weth,
        address _postTechProfile
    ) public initializer {
        weth = IWETH(_weth);
        postTechProfile = IPostTechProfile(_postTechProfile);
        maxLeverageProfile = 200; // mean 2.00, with decimal 100. Ex: 110 mean leverage 1.1
        marginFeePercentage = 5; // 0.05%
        marginFeeInterval = 3_600 * 2; // 3600s * 2 = 2 hour
        thresholdLiquidation = 6_500; // 65%
        basePercentage = 10_000; // 100%
        liquidatorRewardPercentage = 5_000; // 50%
        handlers[msg.sender] = true;
        feeDestinationMarginProtocol = msg.sender;
        indexId = 1;
        baseLeverage = 100;

        whiteListTransferEth[_weth] = true;
        whiteListTransferEth[_postTechProfile] = true;

        thresholdLiquidityBorrowed = 7_000; // 70%

        __ReentrancyGuard_init();
        __Ownable_init();
    }

    receive() external payable {
        require(
            whiteListTransferEth[msg.sender],
            "MarginProfiles: not allow transfer eth"
        );
        liquidity += msg.value;
    }

    function createMarginProfiles(
        uint256 amountShares,
        address subjectAddress,
        uint128 leverage
    ) external payable nonReentrant {
        uint256 supply = postTechProfile.sharesSupply(subjectAddress);

        require(supply > 0, "MarginProfiles: supply must be greater than 0");
        require(amountShares > 0, "MarginProfiles: amount");
        require(
            leverage <= maxLeverageProfile && leverage > baseLeverage,
            "MarginProfiles: leverage invalid"
        );
        require(
            marginProfiles[msg.sender][subjectAddress].amount == 0,
            "MarginProfiles: margin profile already exists"
        );

        uint256 estimatedValue = postTechProfileReader.getBuyPriceAfterFee(
            subjectAddress,
            amountShares
        );
        uint256 amountMargin = (estimatedValue * baseLeverage) / leverage;

        if (msg.value > 0) {
            require(
                msg.value >= amountMargin,
                "MarginProfiles: incorrect margin amount"
            );
        } else {
            weth.transferFrom(msg.sender, address(this), amountMargin);
            weth.withdraw(amountMargin);
        }

        require(
            estimatedValue >= amountMargin,
            "MarginProfiles: no need to margin"
        );

        postTechProfile.buyShares{value: estimatedValue}(
            subjectAddress,
            amountShares
        );

        uint256 loanValue = estimatedValue - amountMargin;

        uint64 _id = indexId++;
        marginProfiles[msg.sender][subjectAddress] = MarginProfile({
            marginValue: amountMargin,
            loanValue: loanValue,
            leverage: leverage,
            amount: uint128(amountShares),
            lastTimeTakeFee: uint64(block.timestamp),
            id: _id,
            marginValueAdded: 0
        });

        emit MarginProfileCreated(
            msg.sender,
            subjectAddress,
            amountShares,
            amountMargin,
            loanValue,
            _id,
            supply + amountShares,
            leverage,
            uint64(block.timestamp)
        );
    }

    function closeMarginProfiles(
        bool isReceiveToken,
        address subjectProfileAddress
    ) external nonReentrant {
        address trader = msg.sender;
        (
            int256 pnl,
            uint256 fee,
            uint64 ratio,
            MarginProfile memory marginProfile
        ) = getMarginProfile(trader, subjectProfileAddress);

        require(
            ratio < thresholdLiquidation,
            "MarginProfiles: must not liquidation"
        );

        uint256 balanceBefore = address(this).balance;
        postTechProfile.sellShares(subjectProfileAddress, marginProfile.amount);
        uint256 balanceAfter = address(this).balance;

        uint256 amountReceive = (balanceAfter -
            balanceBefore +
            marginProfile.marginValueAdded) -
            fee -
            marginProfile.loanValue;

        if (isReceiveToken) {
            weth.deposit{value: amountReceive}();
            weth.transfer(trader, amountReceive);
        } else {
            payable(trader).transfer(amountReceive);
        }
        payable(feeDestinationMarginProtocol).transfer(fee);
        delete marginProfiles[trader][subjectProfileAddress];

        emit MarginProfileClosed(
            trader,
            subjectProfileAddress,
            marginProfile.amount,
            marginProfile.marginValue,
            marginProfile.loanValue,
            fee,
            pnl,
            marginProfile.id,
            marginProfile.leverage,
            uint64(block.timestamp)
        );
    }

    function addMarginProfile(
        address subjectProfileAddress,
        uint128 amountMarginAdded
    ) external payable nonReentrant {
        revert("Not support now");
        address trader = msg.sender;
        require(
            marginProfiles[trader][subjectProfileAddress].amount > 0,
            "MarginProfiles: margin profile not exists"
        );
        if (msg.value > 0) {
            require(
                msg.value == amountMarginAdded,
                "MarginProfiles: incorrect margin amount"
            );
        } else {
            weth.transferFrom(trader, address(this), amountMarginAdded);
            weth.withdraw(amountMarginAdded);
        }

        marginProfiles[trader][subjectProfileAddress]
            .marginValueAdded += amountMarginAdded;
        emit MarginProfileMarginAdded(
            trader,
            subjectProfileAddress,
            amountMarginAdded,
            marginProfiles[trader][subjectProfileAddress].id,
            postTechProfile.sharesSupply(subjectProfileAddress),
            uint64(block.timestamp)
        );
    }

    function removeMarginProfile(
        address subjectProfileAddress,
        uint256 amountMarginRemoved,
        bool isReceiveToken
    ) external payable nonReentrant {
        revert("Not support now");

        MarginProfile memory marginProfileSubject = marginProfiles[msg.sender][
            subjectProfileAddress
        ];
        address trader = msg.sender;
        require(
            marginProfiles[trader][subjectProfileAddress].amount > 0,
            "MarginProfiles: margin profile not exists"
        );
        require(
            marginProfileSubject.marginValueAdded >= amountMarginRemoved,
            "MarginProfiles: incorrect margin amount"
        );

        if (isReceiveToken) {
            weth.deposit{value: amountMarginRemoved}();
            weth.transfer(trader, amountMarginRemoved);
        } else {
            payable(trader).transfer(amountMarginRemoved);
        }

        emit MarginProfileMarginRemoved(
            trader,
            subjectProfileAddress,
            amountMarginRemoved,
            marginProfiles[trader][subjectProfileAddress].id,
            postTechProfile.sharesSupply(subjectProfileAddress),
            uint64(block.timestamp)
        );
    }

    function liquidateMarginProfiles(
        address trader,
        address subjectProfileAddress
    ) external nonReentrant {
        (
            int256 pnl,
            uint256 fee,
            uint64 ratio,
            MarginProfile memory marginProfile
        ) = getMarginProfile(trader, subjectProfileAddress);
        require(
            ratio >= thresholdLiquidation,
            "MarginProfiles: not liquidation"
        );

        uint256 balanceBefore = address(this).balance;
        postTechProfile.sellShares(subjectProfileAddress, marginProfile.amount);

        uint256 liquidatorReward;
        if (fee > 0) {
            liquidatorReward =
                (fee * liquidatorRewardPercentage) /
                basePercentage;
            payable(msg.sender).transfer(liquidatorReward);
            payable(feeDestinationMarginProtocol).transfer(
                fee - liquidatorReward
            );
        }

        uint256 balanceAfter = address(this).balance;

        if (
            balanceAfter > balanceAfter &&
            balanceAfter - balanceBefore > marginProfile.loanValue
        ) {
            liquidity += (balanceAfter - balanceBefore);
        }

        delete marginProfiles[trader][subjectProfileAddress];

        emit MarginProfileLiquidated(
            trader,
            subjectProfileAddress,
            msg.sender,
            marginProfile.amount,
            marginProfile.marginValue,
            marginProfile.loanValue,
            pnl,
            liquidatorReward,
            fee > 0 ? fee - liquidatorReward : 0,
            marginProfile.id,
            marginProfile.leverage,
            uint64(block.timestamp)
        );
    }

    function supplyLiquidity() external payable nonReentrant {
        liquidity += msg.value;
    }

    function isReachThresholdLiquidityBorrowed() public view returns (bool) {
        return
            ((liquidity - address(this).balance) * basePercentage) /
                liquidity >=
            thresholdLiquidityBorrowed;
    }

    // margin 0.1 + loan 0.9 eth => require pnl <= 0.1 and pnl < 0
    // margin 0.9 + loan 0.1 eth => require pnl <= 0.9 and pnl < 0
    // ratio = pnl / margin

    function getMarginProfile(
        address trader,
        address subjectProfileAddress
    )
        public
        view
        returns (
            int256 pnl,
            uint256 fee,
            uint64 ratio,
            MarginProfile memory marginProfile
        )
    {
        marginProfile = marginProfiles[trader][subjectProfileAddress];

        if (marginProfile.amount == 0) {
            return (0, 0, 0, marginProfile);
        }
        uint256 estimatedValue = postTechProfileReader.getSellPrice(
            subjectProfileAddress,
            marginProfile.amount
        );

        marginProfile.amount = getMaxAmountSell(
            marginProfile.amount,
            subjectProfileAddress
        );

        fee = getFee(marginProfile.lastTimeTakeFee, marginProfile.loanValue);

        pnl =
            int256(estimatedValue) -
            int256((marginProfile.loanValue + marginProfile.marginValue));

        if (pnl - int256(fee) < 0) {
            ratio = uint64(
                (uint256(
                    abs(
                        pnl -
                            int256(fee) +
                            int128(marginProfile.marginValueAdded)
                    )
                ) * basePercentage) / marginProfile.marginValue
            );
        }

        return (pnl, fee, ratio, marginProfile);
    }

    function estimateMaxRemoveMargin(
        address, //trader
        address //subjectProfileAddress/
    ) public view returns (uint256) {
        return 0;
    }

    function isLiquidation(
        address trader,
        address subjectAddress
    ) public view returns (bool) {
        uint64 ratio;
        (, , ratio, ) = getMarginProfile(trader, subjectAddress);
        return ratio >= thresholdLiquidation;
    }

    function getFee(
        uint64 lastTimeTakeFee,
        uint256 loanValue
    ) public view returns (uint256) {
        uint64 currentTime = uint64(block.timestamp);

        return
            ((currentTime - lastTimeTakeFee) / marginFeeInterval) *
            ((loanValue * marginFeePercentage) / basePercentage);
    }

    function getMaxAmountSell(
        uint128 amount,
        address subjectAddress
    ) public view returns (uint128) {
        uint128 supply = uint128(postTechProfile.sharesSupply(subjectAddress));
        return supply > amount ? amount : supply - 1;
    }

    ///
    /// ONLY OWNER FUNCTIONS
    ///

    function setMaxLeverage(uint256 _maxLeverageProfile) external onlyOwner {
        maxLeverageProfile = _maxLeverageProfile;
    }

    function setFeeDestinationMarginProtocol(
        address _feeDestinationMarginProtocol
    ) external onlyOwner {
        feeDestinationMarginProtocol = _feeDestinationMarginProtocol;
    }

    function setMarginFeePercentage(
        uint256 _marginFeePercentage
    ) external onlyOwner {
        marginFeePercentage = _marginFeePercentage;
        emit MarginFeeIntervalUpdated(
            _marginFeePercentage,
            uint64(block.timestamp)
        );
    }

    function setMarginInterval(uint256 _marginFeeInterval) external onlyOwner {
        marginFeeInterval = _marginFeeInterval;
        emit MarginFeeIntervalUpdated(
            _marginFeeInterval,
            uint64(block.timestamp)
        );
    }

    function setThresholdLiquidation(
        uint256 _thresholdLiquidation
    ) external onlyOwner {
        thresholdLiquidation = _thresholdLiquidation;
    }

    function setLiquidatorRewardPercentage(
        uint256 _liquidatorRewardPercentage
    ) external onlyOwner {
        liquidatorRewardPercentage = _liquidatorRewardPercentage;
    }

    function setHandler(address _handler, bool _isHandler) external onlyOwner {
        handlers[_handler] = _isHandler;
    }

    function setWhiteListTransferEth(
        address _whiteListTransferEth,
        bool _isWhiteList
    ) external onlyOwner {
        whiteListTransferEth[_whiteListTransferEth] = _isWhiteList;
    }

    function abs(int256 x) private pure returns (int256) {
        return x >= 0 ? x : -x;
    }
}

