pragma solidity ^0.8.0;

import './VRFCoordinatorV2Interface.sol';
import './AggregatorV3Interface.sol';
import 'openzeppelin/security/ReentrancyGuard.sol';
import 'openzeppelin/utils/Strings.sol';
import {SafeERC20, IERC20} from 'openzeppelin/token/ERC20/utils/SafeERC20.sol';
import {IFlush} from '../Flush/Flush.sol';

import './ChainSpecificUtil.sol';

interface IBankRoll {
    function getIsGame(address game) external view returns (bool);

    function getIsValidWager(address game, address tokenAddress) external view returns (bool);

    function transferPayout(address player, uint256 payout) external;

    function getOwner() external view returns (address);

    function getIsPlayerSuspended(address player) external view returns (bool);

    function getTeamWallet() external view returns (address);

    function getHolderWallet() external view returns (address);

    function increaseTotalBurned(uint256 amount) external;

    function getveFlush() external view returns (address);

    function getFlush() external view returns (address);
}

interface IVRFCoordinatorV2 is VRFCoordinatorV2Interface {
    function getFeeConfig()
        external
        view
        returns (uint32, uint32, uint32, uint32, uint32, uint24, uint24, uint24, uint24);
}

contract Common is ReentrancyGuard {
    using SafeERC20 for IERC20;

    uint256 public VRFFees;
    address public ChainLinkVRF;
    address public _trustedForwarder;

    AggregatorV3Interface public LINK_ETH_FEED;
    IVRFCoordinatorV2 public IChainLinkVRF;
    IBankRoll public Bankroll;

    /**
     * @dev function to transfer the player wager to bankroll, and charge for VRF fee
     * , reverts if bankroll doesn't approve game or token
     * @param tokenAddress address of the token the wager is made on
     * @param wager total amount wagered
     */

    function _transferWager(
        address tokenAddress,
        uint256 wager,
        uint256 gasAmount,
        uint256 l1Multiplier,
        address msgSender
    ) internal returns (uint256 VRFfee) {
        if (!Bankroll.getIsValidWager(address(this), tokenAddress)) {
            revert('Common : Bankroll does not approve game');
        }
        if (wager < 1 ether) {
            revert('Common : Wager must be greater than 1 token');
        }
        bool suspended = Bankroll.getIsPlayerSuspended(msgSender);
        if (suspended) {
            revert('Common : Player is suspended');
        }
        VRFfee = getVRFFee(gasAmount, l1Multiplier);

        if (tokenAddress == address(0)) {
            revert('Common : Token address cannot be 0');
        } else {
            if (msg.value < VRFfee) {
                revert('Common : Insufficient funds to pay for VRF');
            }

            IERC20(tokenAddress).safeTransferFrom(msgSender, address(this), wager);

            _refundExcessValue(msg.value - VRFfee);
        }
        VRFFees += VRFfee;
    }

    /**
     * @dev  Burn 90% of losing bet and transfer 9% to holder and 1% to team wallet.
     * The wager of the winning bets are 100% burned.
     * @param tokenAddress address of the token to transfer
     * @param wager wager per bet
     * @param numBet number of bets
     * @param winningBet number of winning bets
     */

    function _burnAndTransfer(address tokenAddress, uint256 wager, uint32 numBet, uint32 winningBet) internal {
        if (tokenAddress == address(0)) {
            revert('Common : Token address cannot be 0');
        } else {
            uint256 lossAmount = wager * (numBet - winningBet);
            uint256 winningBetAmount = wager * winningBet;
            address flushAddress = Bankroll.getFlush();
            uint256 lossAmount90 = ((lossAmount * 90) / 100);
            uint256 lossAmountLeft = lossAmount - lossAmount90;
            uint256 lossAmountToTeam = ((lossAmountLeft) / 100);

            // winning bet amount are totally burned bc payout is minted by bankroll
            // only loss amount are burned partially
            IFlush(tokenAddress).burn(winningBetAmount + lossAmount90);

            Bankroll.increaseTotalBurned(lossAmount90);

            // if vFlush are played transform 10% of the loss amount to Flush to rewards holder
            if (Bankroll.getveFlush() == tokenAddress) {
                _transferPayout(address(this), lossAmount - lossAmount90);
            }

            IERC20(flushAddress).safeTransfer(Bankroll.getHolderWallet(), lossAmountLeft - lossAmountToTeam);
            IERC20(flushAddress).safeTransfer(Bankroll.getTeamWallet(), lossAmountToTeam);
        }
    }

    /**
     * @dev calculates in form of native token the fee charged by chainlink VRF
     * @return fee amount of fee user has to pay
     */
    function getVRFFee(uint256 gasAmount, uint256 l1Multiplier) public view returns (uint256 fee) {
        (, int256 answer, , , ) = LINK_ETH_FEED.latestRoundData();
        (uint32 fulfillmentFlatFeeLinkPPMTier1, , , , , , , , ) = IChainLinkVRF.getFeeConfig();

        uint256 l1CostWei = (ChainSpecificUtil.getCurrentTxL1GasFees() * l1Multiplier) / 10;
        fee =
            tx.gasprice *
            (gasAmount) +
            l1CostWei +
            ((1e12 * uint256(fulfillmentFlatFeeLinkPPMTier1) * uint256(answer)) / 1e18);
    }

    /**
     * @dev returns to user the excess fee sent to pay for the VRF
     * @param refund amount to send back to user
     */
    function _refundExcessValue(uint256 refund) internal {
        if (refund == 0) {
            return;
        }
        (bool success, ) = payable(msg.sender).call{value: refund}('');
        if (!success) {
            revert('Common : Refund excess value failed');
        }
    }

    /**
     * @dev function to charge user for VRF
     */
    function _payVRFFee(uint256 gasAmount, uint256 l1Multiplier) internal returns (uint256 VRFfee) {
        VRFfee = getVRFFee(gasAmount, l1Multiplier);
        if (msg.value < VRFfee) {
            revert('Common : Insufficient funds to pay for VRF');
        }
        _refundExcessValue(msg.value - VRFfee);
        VRFFees += VRFfee;
    }

    /**
     * @dev function to transfer VRF fees acumulated in the contract to the Bankroll
     * Can only be called by owner
     */
    function transferFees(address to) external nonReentrant {
        if (msg.sender != Bankroll.getOwner()) {
            revert('Common : Only Bankroll owner can transfer fees');
        }
        uint256 fee = VRFFees;
        VRFFees = 0;
        (bool success, ) = payable(address(to)).call{value: fee}('');
        if (!success) {
            revert('Common : Transfer fees failed');
        }
    }

    /**
     * @dev function to transfer wager to game contract, without charging for VRF
     * @param tokenAddress tokenAddress the wager is made on
     * @param wager wager amount
     */
    function _transferWagerPvPNoVRF(address tokenAddress, uint256 wager) internal {
        if (!Bankroll.getIsValidWager(address(this), tokenAddress)) {
            revert('Common : Bankroll does not approve game');
        }
        if (tokenAddress == address(0)) {
            revert('Common : Token address cannot be 0');
        } else {
            IERC20(tokenAddress).safeTransferFrom(msg.sender, address(this), wager);
        }
    }

    /**
     * @dev function to request bankroll to give payout to player
     * @param player address of the player
     * @param payout amount of payout to give
     */
    function _transferPayout(address player, uint256 payout) internal {
        Bankroll.transferPayout(player, payout);
    }

    /**
     * @dev function to send the request for randomness to chainlink
     * @param numWords number of random numbers required
     */
    function _requestRandomWords(uint32 numWords) internal returns (uint256 s_requestId) {
        try
            IVRFCoordinatorV2(ChainLinkVRF).requestRandomWords(
                0x83d1b6e3388bed3d76426974512bb0d270e9542a765cd667242ea26c0cc0b730,
                112,
                1,
                2500000,
                numWords
            )
        returns (uint256 requestId) {
            s_requestId = requestId;
        } catch {
            revert('Common : Request random words failed');
        }
    }

    function isTrustedForwarder(address forwarder) public view returns (bool) {
        return forwarder == _trustedForwarder;
    }

    function _msgSender() internal view returns (address ret) {
        if (msg.data.length >= 20 && isTrustedForwarder(msg.sender)) {
            // At this point we know that the sender is a trusted forwarder,
            // so we trust that the last bytes of msg.data are the verified sender address.
            // extract sender address from the end of msg.data
            assembly {
                ret := shr(96, calldataload(sub(calldatasize(), 20)))
            }
        } else {
            ret = msg.sender;
        }
    }
}

