pragma solidity 0.8.21;

import "@openzeppelin/contracts/utils/structs/EnumerableSet.sol";
import "@openzeppelin/contracts/token/ERC1155/ERC1155.sol";
import "@openzeppelin/contracts/utils/Counters.sol";
import "@openzeppelin/contracts/utils/Address.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "./interfaces/IHighFive.sol";
import "./interfaces/IOracle.sol";
import {console} from "forge-std/console.sol";

contract HighFive is IHighFive, Ownable, ERC1155 {
    using EnumerableSet for EnumerableSet.AddressSet;
    using Counters for Counters.Counter;
    using Address for address payable;
    using Address for address;

    IOracle public constant ORACLE =
        IOracle(0x639Fe6ab55C921f74e7fac1ee960C0B6293ba612); // ETH-USD
    uint256 public constant FIVE = 5;
    uint256 public constant LAST = FIVE - 1;
    uint256 public constant MIN_REC_NUM = 5;
    uint256 public constant MAX_REC_NUM = 10;
    uint256 public constant MAX_ITER = 100;
    uint256 public constant NULL_POS = 100;
    uint256 public constant MAX_SEQ_CREATION_FEE_FACTOR = 10;
    uint256 public constant MAX_BALNACE_THRESHOLD = 1e17;
    address public immutable treasury;

    uint256 public seqCreationFeeFactor;
    uint256 public maxSeqNum;
    uint256 public numOfRecipients;
    uint256 public balanceThreshold;

    bool public modeLock1;
    bool public modeLock2;

    mapping(address => Mailbox) private _mailbox;
    mapping(uint256 => Letter) private _letterInfo; // letter Id => Letter

    EnumerableSet.AddressSet private _pool;
    Counters.Counter private _letterId; // letter Id
    Counters.Counter private _seqId; // sequence Id

    constructor(address _treasury) ERC1155("hi5.network") {
        require(_treasury != address(0), "HF: Invalid address");
        treasury = _treasury;
        _letterId.increment(); // skip 0

        seqCreationFeeFactor = FIVE - 1; // $20 to treasury
        maxSeqNum = 10; // max sequence number
        numOfRecipients = 5; // recipient number
        balanceThreshold = 1e18 / 100; // 0.01 ETH
    }

    function initNewSequence(
        address[FIVE] memory _beneficiaries,
        address[] memory _recipients
    ) external payable override {
        require(
            msg.value == _ethAmount() * seqCreationFeeFactor,
            "HF: Invalid amount"
        );
        require(
            _validateAccounts(_beneficiaries, _recipients),
            "HF: Invalid accounts"
        );
        require(_beneficiaries[LAST] == msg.sender, "HF: Invalid accounts");
        uint256 sid = _seqId.current();
        _seqId.increment();

        uint256 seqCreationFee = _ethAmount() * seqCreationFeeFactor;
        payable(treasury).sendValue(seqCreationFee);

        Letter memory letter = _createLetter(_beneficiaries);
        _sendLettersDeterministically(letter, _recipients);

        _addToPool(_beneficiaries);
        _addToPool(_recipients);

        emit NewSequenceInitiated(sid, msg.sender, block.timestamp);
    }

    function propagate() external payable override {
        require(
            msg.value == FIVE * _ethAmount() + _feeAmount(),
            "HF: Invalid amount"
        );
        require(!modeLock1, "HF: This mode locked");
        address caller = msg.sender;
        uint256 index = _mailbox[caller].index;
        require(index < FIVE, "HF: All letters have been sent");
        uint256 lid = _mailbox[caller].ids[index];
        require(balanceOf(caller, lid) == 1, "HF: Invalid balance");
        Letter memory letter = _letterInfo[lid];
        _mailbox[caller].index++;

        _sendETH(letter.beneficiaries);

        Letter memory newLetter = _updateLetter(letter, lid, caller);
        _sendLettersRandomly(newLetter);
        console.log(lid);
        console.log(caller);
        console.log(block.timestamp);
        emit LetterPropagated(lid, caller, block.timestamp);
    }

    function propagate(uint256 n) external payable override {
        require(
            msg.value == n * FIVE * _ethAmount() + n * _feeAmount(),
            "HF: Invalid amount"
        );
        require(!modeLock1, "HF: This mode locked");
        address caller = msg.sender;
        uint256 index = _mailbox[caller].index;
        require(n > 1, "HF: 0 is invalid and 1 is handled by another function");
        require(index + n <= FIVE, "HF: Propagating more than available");

        Letter memory letter;
        Letter memory newLetter;
        uint256 lid;
        for (uint256 i = 0; i < n; i++) {
            lid = _mailbox[caller].ids[index + i];
            require(balanceOf(caller, lid) == 1, "HF: Invalid balance");
            letter = _letterInfo[lid];
            _mailbox[caller].index++;
            _sendETH(letter.beneficiaries);
            newLetter = _updateLetter(letter, lid, caller);
            _sendLettersRandomly(newLetter);
            emit LetterPropagated(lid, caller, block.timestamp);
        }
    }

    function propagate(address[] memory recipients) external payable override {
        require(recipients.length == numOfRecipients, "HF: Invalid number");
        require(
            msg.value == FIVE * _ethAmount() + _feeAmount(),
            "HF: Invalid amount"
        );
        require(!modeLock2, "HF: This mode locked");
        address caller = msg.sender;
        uint256 index = _mailbox[caller].index;
        require(index < FIVE, "HF: All letters have been sent");
        uint256 lid = _mailbox[caller].ids[index];
        require(balanceOf(caller, lid) == 1, "HF: Invalid balance");
        Letter memory letter = _letterInfo[lid];
        _mailbox[caller].index++;

        require(
            _validateAccounts(letter.beneficiaries, recipients),
            "HF: Invalid accounts"
        );

        _sendETH(letter.beneficiaries);

        Letter memory newLetter = _updateLetter(letter, lid, caller);

        _sendLettersDeterministically(newLetter, recipients);

        _addToPool(recipients);

        emit LetterPropagated(lid, caller, block.timestamp);
    }

    function addToPool(address[] calldata _accounts) external override {
        _addToPool(_accounts);
    }

    function _addToPool(address[FIVE] memory _accounts) private {
        address account;
        for (uint256 i = 0; i < FIVE; i++) {
            account = _accounts[i];
            if (_isWalletValid(account)) _pool.add(account);
        }
    }

    function _addToPool(address[] memory _accounts) private {
        uint256 n = _accounts.length;
        address account;
        for (uint256 i = 0; i < n; i++) {
            account = _accounts[i];
            if (_isWalletValid(account)) _pool.add(account);
        }
    }

    function setNumOfRecipients(uint256 _num) external override onlyOwner {
        require(_num <= MAX_REC_NUM && _num >= MIN_REC_NUM, "HF: Invalid number");
        numOfRecipients = _num;

        emit NumOfRecipientsUpdated(_num);
    }

    function setSeqCreationFeeFactor(
        uint256 _factor
    ) external override onlyOwner {
        require(_factor <= MAX_SEQ_CREATION_FEE_FACTOR, "HF: Invalid number");
        seqCreationFeeFactor = _factor;

        emit SeqCreationFeeFactorUpdated(_factor);
    }

    function setBalanceThreshold(
        uint256 _balanceThreshold
    ) external override onlyOwner {
        require(_balanceThreshold <= MAX_BALNACE_THRESHOLD, "HF: Invalid number");
        balanceThreshold = _balanceThreshold;

        emit BalanceThresholdSet(_balanceThreshold);
    }

    function toggleModelock1() external override onlyOwner {
        modeLock1 = !modeLock1;

        emit Modelock1Toggled(modeLock1);
    }

    function toggleModelock2() external override onlyOwner {
        modeLock2 = !modeLock2;

        emit Modelock2Toggled(modeLock2);
    }

    function getSeqId() external view override returns (uint256) {
        return _seqId.current();
    }

    function getPoolSize() external view override returns (uint256) {
        return _pool.length();
    }

    function getIndex(
        address account
    ) external view override returns (uint256) {
        return _mailbox[account].index;
    }

    function getLetterInfo(
        address account,
        uint256 index
    ) external view override returns (Letter memory) {
        uint256 letterId = _mailbox[account].ids[index];
        return _letterInfo[letterId];
    }

    function getLetterDepth(
        uint256 lid
    ) external view override returns (uint256) {
        Letter memory letter = _letterInfo[lid];
        return letter.depth;
    }

    function getNextLetterId() external view override returns (uint256) {
        return _letterId.current();
    }

    function getMailboxInfo(
        address account
    ) external view returns (Mailbox memory) {
        return _mailbox[account];
    }

    function getETHPrice() external view override returns (uint256) {
        return _getPrice();
    }

    function getETHAmount() external view override returns (uint256) {
        return _ethAmount();
    }

    function _getRecipientsFromPool(
        address[5] memory arr
    ) private view returns (address[] memory recipients) {
        recipients = new address[](numOfRecipients);
        uint256 index = uint256(
            keccak256(abi.encodePacked(blockhash(block.number - 1)))
        );
        uint256 n = _pool.length();
        require(n > numOfRecipients, "HF: Insufficient pool size");

        uint256 i;
        uint256 counter;
        address account;
        while (i < numOfRecipients && counter < MAX_ITER) {
            account = _pool.at(index % n);
            if (_isInArray(recipients, NULL_POS, account)) {
                continue;
            }
            if (
                _isWalletValid(account) && !_isInArray(arr, NULL_POS, account)
            ) {
                recipients[i] = account;
                ++i;
            }
            index = uint256(keccak256(abi.encodePacked(index)));
            ++counter;
        }
        if (counter == MAX_ITER) {
            revert("HF: Exceeded max iteration");
        }
        if (i != numOfRecipients) {
            revert("HF: Insufficient pool size");
        }
    }

    function _sendETH(address[FIVE] memory _recipients) private {
        uint256 ethAmount = _ethAmount();
        for (uint256 i = 0; i < FIVE; i++) {
            payable(_recipients[i]).sendValue(ethAmount);
        }
        payable(treasury).sendValue(ethAmount / FIVE); // $1 to treasury
    }

    function _createLetter(
        address[FIVE] memory _beneficiaries
    ) private returns (Letter memory letter) {
        letter = Letter({depth: 1, beneficiaries: _beneficiaries});

        emit LetterCreated(_letterId.current(), letter.depth);
    }

    function _updateLetter(
        Letter memory _letter,
        uint256 _lid,
        address account
    ) private returns (Letter memory newLetter) {
        _burn(account, _lid, 1);
        require(balanceOf(account, _lid) == 0, "HF: Invalid balance");
        uint256 level = (_letter.depth - 1) % FIVE; // 0 to 4
        address[FIVE] memory _beneficiaries = _letter.beneficiaries;
        _beneficiaries[level] = account;
        newLetter = Letter({
            depth: _letter.depth + 1,
            beneficiaries: _beneficiaries
        });

        emit LetterUpdated(_lid, _letter.depth, newLetter.depth);
    }

    function _sendLettersRandomly(Letter memory _letter) private {
        address[] memory recipients = _getRecipientsFromPool(
            _letter.beneficiaries
        );
        uint256 _lid = _letterId.current();
        address recipient;
        uint256 pos;
        for (uint256 i = 0; i < recipients.length; i++) {
            recipient = recipients[i];
            pos = _findPos(_mailbox[recipient].ids);
            require(pos != NULL_POS, "HF: No slot available for recipient");
            _mailbox[recipient].ids[pos] = _lid;
            _mint(recipient, _lid, 1, "");
            require(balanceOf(recipient, _lid) == 1, "HF: Invalid balance");
        }
        _letterInfo[_lid] = _letter;
        _letterId.increment();
    }

    function _sendLettersDeterministically(
        Letter memory _letter,
        address[] memory _recipients
    ) private {
        uint256 _lid = _letterId.current();
        address recipient;
        uint256 pos;
        for (uint256 i = 0; i < _recipients.length; i++) {
            recipient = _recipients[i];
            pos = _findPos(_mailbox[recipient].ids);
            require(pos != NULL_POS, "HF: No slot available for recipient");
            _mailbox[recipient].ids[pos] = _lid;
            _mint(recipient, _lid, 1, "");
            require(balanceOf(recipient, _lid) == 1, "HF: Invalid balance");
        }
        _letterInfo[_lid] = _letter;
        _letterId.increment();
    }

    function _validateAccounts(
        address[FIVE] memory _beneficiaries,
        address[] memory _recipients
    ) internal view returns (bool) {
        if (_recipients.length != numOfRecipients) return false;
        console.log("========tx1=========");
        for (uint256 i = 0; i < FIVE; i++) {
            if (!_isWalletValid(_beneficiaries[i])) {
                return false;
            }
            console.log("========tx2=========");
            if (_isInArray(_beneficiaries, i, _beneficiaries[i])) {
                return false;
            }
            console.log("========tx3=========");
        }
        for (uint256 i = 0; i < _recipients.length; i++) {
            if (!_isWalletValid(_recipients[i])) {
                return false;
            }
            console.log("========tx4=========");
            if (_isInArray(_beneficiaries, NULL_POS, _recipients[i])) {
                return false;
            }
            if (_isInArray(_recipients, i, _recipients[i])) {
                return false;
            }
            console.log("========tx5=========");
        }
        return true;
    }

    function _isInArray(
        address[] memory _arr,
        uint256 _excludedIndex,
        address _account
    ) internal pure returns (bool) {
        for (uint256 i = 0; i < _arr.length; i++) {
            if (i == _excludedIndex) continue;
            if (_arr[i] == _account) return true;
        }
        return false;
    }

    function _isInArray(
        address[FIVE] memory _arr,
        uint256 _excludedIndex,
        address _account
    ) internal pure returns (bool) {
        for (uint256 i = 0; i < FIVE; i++) {
            if (i == _excludedIndex) continue;
            if (_arr[i] == _account) return true;
        }
        return false;
    }

    function _findPos(
        uint256[FIVE] memory _arr
    ) private pure returns (uint256) {
        for (uint256 i = 0; i < FIVE; i++) {
            if (_arr[i] == 0) return i;
        }
        return NULL_POS;
    }

    function _feeAmount() private view returns (uint256) {
        return _ethAmount() / FIVE;
    }

    function _ethAmount() private view returns (uint256) {
        uint256 price = _getPrice();
        if (price > 0) {
            return (FIVE * 1e18 * 1e8) / price;
        } else {
            return 1e18 / 200; // 0.005 ETH
        }
    }

    function _getPrice() private view returns (uint256) {
        (, int256 price, , , ) = ORACLE.latestRoundData();
        if (price > 0) {
            return uint256(price);
        } else {
            return 0;
        }
    }

    function _isWalletValid(address _account) private view returns (bool) {
        bool cond1 = _findPos(_mailbox[_account].ids) != NULL_POS; // slot available
        bool cond2 = !_account.isContract();
        bool cond3 = _hasEther(_account);
        return cond1 && cond2 && cond3;
    }

    function _hasEther(address _account) private view returns (bool) {
        return _account.balance > balanceThreshold;
    }

    function _beforeTokenTransfer(
        address /*operator*/,
        address from,
        address to,
        uint256[] memory /*ids*/,
        uint256[] memory /*amounts*/,
        bytes memory /*data*/
    ) internal pure override {
        if (from == address(0) || to == address(0)) {
            return;
        }
        revert("HF: Transfer not allowed");
    }
}

