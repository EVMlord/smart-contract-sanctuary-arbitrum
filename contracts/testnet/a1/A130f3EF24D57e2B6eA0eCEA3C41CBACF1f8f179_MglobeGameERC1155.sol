        require(material.enabled);

        address factoryContractAddress = material.FactoryContractAddress;

        require(keccak256(abi.encodePacked(factoryContractAddress)) != keccak256(abi.encodePacked("")), "MglobeGameNFTs#mintErc1155: Factory contract address not set");
        
         uint[] memory charactors;

        //自增tokenId
        uint tokenId = _tokenIdTracker.current();
		_tokenIdTracker.increment();

       
       if (type_id == -1) {
         //调用工厂合约生成NFTmetadata数据
        charactors= IFactory(factoryContractAddress).geneMetaData(tokenId); 

       } else {

        //调用工厂合约生成NFTmetadata数据
        charactors= IFactory(factoryContractAddress).geneMetaData2(tokenId, uint256(type_id));
           
       }

        //注意此语句必须在geneMetaData之前
        _mint(to, tokenId, 1, '0');

        _idNftTypeindexes[tokenId] = nftTypeIndex; //用于回收

        creators[tokenId] = to;
        tokenSupply[tokenId] = 1;
        
        emit OnMinted(to, nftTypeIndex, tokenId, charactors);

	}
    
    /**
      * @dev  Upgrade a ERC1155 NFT
      * NOTE: NFT强化升级， 原来的id保留
      * NOTE: emits OnUpgraded event
      * @param to 给谁铸造，如果填写调用者地址，则为自己铸造
      * @param currId 当前NFT编号
      * @param nftTypeIndex 升级配方
      * @param orderid 订单id，由中心化接口生成
      * @param signature 服务端侧私钥签名
      * @return true
      */     
    function upgradeErc1155(
        address to,
        uint256 currId,
        uint256 potential,     //强化次数 
        uint256 nftTypeIndex,  //升级配方
        uint256 orderid,
        bytes calldata signature
    ) 
        external
        whenNotPaused
        onlyNonContract
        // nonReentrant 
        returns(bool)
    {
        require(to != address(0x0), "to can't be empty");
        require(potential<=5, "potential exceed");
        //强化需要的原材料
        Material memory material = _materials[nftTypeIndex];
        require(material.enabled);

        //判断是否需要扣除Uru
        if (material.UruFee >0) {
            //玩家先提前操作Uru发币合约授权本合约地址，由合约操作将玩家的Uru转到平台运营钱包
            URUTokenContract.safeTransferFrom(to, address(this), material.UruFee);
        }

        //销毁原材料
        for (uint256 i = 0; i < material.MaterialIds.length; i++) {

            uint256 _id = material.MaterialIds[i];
            uint256 _quantity = material.MaterialFees[i];

            _burn(to, _id, _quantity);
            tokenSupply[_id] = tokenSupply[_id].sub(_quantity);

            //非系统token
            if (_id >= 10000 && tokenSupply[_id] == 0) {
                creators[_id] = address(0x0);
            }
        }

        //metadata数据
        uint[] memory charactors = IFactory(material.FactoryContractAddress).upgradeMetaData(currId, potential,orderid, signature);

        emit OnUpgraded(to, currId, orderid, charactors);

        return true;
    }

    /**
      * @dev  Revert a ERC1155 NFT from server
      * NOTE: NFT强化升级并销毁原来的tokenId
      * NOTE: emits OnMinted event
      * @param to 给谁铸造，如果填写调用者地址，则为自己铸造
      * @param currId 当前NFT编号
      * @param nftTypeIndex 合成配方
      * @param orderid 订单id，由中心化接口生成
      * @param signature 服务端侧私钥签名
      * @return true
      */     
    function revertErc1155(
        uint256 uruFee,
        address to,
        uint256 currId,
        uint256 nftTypeIndex,  //合成配方
        uint256 orderid,
        bytes calldata signature
    ) 
        external
        whenNotPaused
        onlyNonContract
        // nonReentrant 
        returns(bool)
    {
        require(to != address(0x0), "to can't be empty");

        //根据配方找出工厂合约
        Material memory material = _materials[nftTypeIndex];
        require(material.enabled);

        //判断是否需要扣除Uru
        //TODO 是否需要一个系数
        if (uruFee >0) {
            //玩家先提前操作Uru发币合约授权本合约地址，由合约操作将玩家的Uru转到平台运营钱包
            URUTokenContract.safeTransferFrom(to, address(this), uruFee);
        }

        //恢复metadata数据
        IFactory(material.FactoryContractAddress).revertMetaData(to, currId, orderid, signature);

        emit OnReverted(to, currId, orderid);

        return true;
    }

    /**
     * @dev Creates `amount` new tokens for `to`, of token type `id`.
     *
