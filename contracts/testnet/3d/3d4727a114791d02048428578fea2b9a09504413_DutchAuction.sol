import "@openzeppelin/contracts-upgradeable/security/PausableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/access/AccessControlUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "@openzeppelin/contracts-upgradeable/utils/StringsUpgradeable.sol";

import "./base/Versioned.sol";

contract Validator is Initializable, PausableUpgradeable, AccessControlUpgradeable, Versioned {

    using StringsUpgradeable for uint256;
    bytes32 public constant PAUSER_ROLE = keccak256("PAUSER_ROLE");

    mapping(address => bool) private _validators;
    uint256 private _numValidators;
    uint256 private _validationThreshold;
    
    event ValidatorAdded(address validator);
    event ValidatorRemoved(address validator);
    event ThresholdUpdated(uint256 oldThreshold, uint256 newThreshold);

    /// @custom:oz-upgrades-unsafe-allow constructor
    constructor() initializer {}
    
    function initialize() initializer public {
        __Pausable_init();
        __AccessControl_init();

        _grantRole(DEFAULT_ADMIN_ROLE, msg.sender);
        _grantRole(PAUSER_ROLE, msg.sender);
    }

    function pause() public onlyRole(PAUSER_ROLE) {
        _pause();
    }

    function unpause() public onlyRole(PAUSER_ROLE) {
        _unpause();
    }

    function isValidator(address account) public view returns (bool) {
        return _validators[account];
    }

    function numValidators() public view returns (uint256) {
        return _numValidators;
    }

    function threshold() public view returns (uint256) {
        return _validationThreshold;
    }

    function adminSetThreshold(uint256 newThreshold) public onlyRole(DEFAULT_ADMIN_ROLE) whenNotPaused {
        require(newThreshold <= _numValidators, "adminSetThreshold: not enough validators to meet threshold");
        uint256 old = _validationThreshold;
        _validationThreshold = newThreshold;
        emit ThresholdUpdated(old, newThreshold);
    }

    function adminAddValidator(address account) public onlyRole(DEFAULT_ADMIN_ROLE) whenNotPaused {
        require(account != address(0x0), "adminAddValidator: invalid address");
        require(!isValidator(account), "adminAddValidator: account is already a validator");
        _validators[account] = true;
        _numValidators += 1;
        emit ValidatorAdded(account);
    }

    function adminRemoveValidator(address account) public onlyRole(DEFAULT_ADMIN_ROLE) whenNotPaused {
        require(isValidator(account), "adminRemoveValidator: account is not a validator");
        require(_numValidators - 1 >= _validationThreshold, "adminRemoveValidator: not enough validators to meet threshold");
        _validators[account] = false;
        _numValidators -= 1;
        emit ValidatorRemoved(account);
    }

    function splitSignature(bytes memory signature, uint256 offset) internal pure returns (bytes32 r, bytes32 s, uint8 v) {
        assembly {
            r := mload(add(signature, offset))
            s := mload(add(add(signature, offset), 32))
            v := byte(0, mload(add(add(signature, offset), 64)))
        }
    }

    function verifySignature(bytes memory message, bytes memory signature) public view whenNotPaused returns (bool) {
        require(_numValidators > 0, "verifySignature: no validator");

        bytes32 messageHash = keccak256(abi.encodePacked("\x19Ethereum Signed Message:\n", message.length.toString(), message));
        uint256 numVerifications = 0;
        uint256 numSignatures = signature.length / 65;
        address[] memory usedValidators = new address[](numSignatures);
        for (uint256 index = 0; index < numSignatures; index++) {
            (bytes32 r, bytes32 s, uint8 v) = splitSignature(signature, 32 + index * 65);
            address recovered = ecrecover(messageHash, v, r, s);

            if (_validators[recovered]) {
                // check for duplicated validators
                bool duplicated = false;
                for (uint256 index2 = 0; index2 < usedValidators.length; index2++) {
                    if (usedValidators[index2] == recovered) {
                        duplicated = true;
                        break;
                    }
                }

                if (!duplicated) {
                    numVerifications += 1;
                    usedValidators[index] = recovered;
                }
            }
        }

        return numVerifications >= _validationThreshold;
    }
}

