pragma solidity ^0.8.9;

import "@openzeppelin/contracts/token/ERC20/ERC20.sol";
import "@openzeppelin/contracts/access/AccessControl.sol";
import "@uniswap/v3-periphery/contracts/interfaces/ISwapRouter.sol";
import "@uniswap/v3-periphery/contracts/libraries/TransferHelper.sol";
import "@uniswap/v3-core/contracts/interfaces/IUniswapV3Pool.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@uniswap/v3-core/contracts/libraries/TickMath.sol";
import "@openzeppelin/contracts/token/ERC721/IERC721Receiver.sol";
import "./interfaces/INonFungiblePositionManager.sol";
import "@uniswap/v3-periphery/contracts/interfaces/IQuoterV2.sol";
import "@uniswap/v3-core/contracts/interfaces/IUniswapV3Factory.sol";

import {MerkleProof} from "@openzeppelin/contracts/utils/cryptography/MerkleProof.sol";

import "./T4LDividendTracker.sol";

contract T4LToken is ERC20, AccessControl {
    bytes32 public constant ADMIN_ROLE = keccak256("ADMIN_ROLE");
    bytes32 public constant UPDATE_ROOT_ROLE = keccak256("UPDATE_ROOT_ROLE");
    bytes32 root;

    address public nonfungiblePositionManagerAddress = 0xC36442b4a4522E871399CD717aBDD847Ab11FE88;
    address quoterAddress = 0x61fFE014bA17989E743c5F6cB21bF9697530B21e;
    //address public constant WETH = 0x82aF49447D8a07e3bd95BD0d56f35241523fBab1;
    address public opsMarketingAddress;
    address public constant WETH = 0xe39Ab88f8A4777030A534146A9Ca3B52bd5D43A3; //testnet

    INonfungiblePositionManager public immutable nonfungiblePositionManager =
        INonfungiblePositionManager(nonfungiblePositionManagerAddress);
    ISwapRouter public immutable swapRouter =
        ISwapRouter(0xE592427A0AEce92De3Edee1F18E0157C05861564);
    T4LDividendTracker public dividendTracker;
    
    uint256 public liquidityPositionID;
    uint16 liquidityPoolFee = 200;
    uint16 ethRewardsFee = 200;
    uint16 opsMarketingFee = 200;
    uint256 public gasForProcessing = 300000;
    uint16 public totalFees = ethRewardsFee + liquidityPoolFee + opsMarketingFee;
    
    

    bool private swapping;

    mapping(address => bool) private _isExcludedFromFees; // exlcude from fees and max transaction amount
    mapping(address => uint256) public taken; // How much tokens taken by address
    mapping(string => address) public withdrawingAddress;

    event UserCalledClaimRewards(address _userAddress);
    event UpdateDividendTracker(
        address indexed newAddress,
        address indexed oldAddress
    );

    event UpdateUniswapV2Router(
        address indexed newAddress,
        address indexed oldAddress
    );

    event ExcludeFromFees(address indexed account, bool isExcluded);
    event ExcludeMultipleAccountsFromFees(address[] accounts, bool isExcluded);

    event LiquidityWalletUpdated(
        address indexed newLiquidityWallet,
        address indexed oldLiquidityWallet
    );

    event GasForProcessingUpdated(
        uint256 indexed newValue,
        uint256 indexed oldValue
    );

    event SwapAndLiquify(
        uint256 Liquidity,
        uint256 AmountT4L,
        uint256 AmountWETH
    );

    event SendDividends(uint256 tokensSwapped, uint256 amount);

    event ProcessedDividendTracker(
        uint256 iterations,
        uint256 claims,
        uint256 lastProcessedIndex,
        bool indexed automatic,
        uint256 gas,
        address indexed processor
    );
    event UserClaimedRewards(address indexed _address, uint amount);

    constructor(string memory name, string memory symbol) ERC20(name, symbol) {
        dividendTracker = new T4LDividendTracker();

        _grantRole(AccessControl.DEFAULT_ADMIN_ROLE, msg.sender);

        //exclude from receiving dividends
        dividendTracker.excludeFromDividends(address(dividendTracker));
        dividendTracker.excludeFromDividends(address(this));
        dividendTracker.excludeFromDividends(msg.sender); //OWNER//
        dividendTracker.excludeFromDividends(address(swapRouter));

        // exclude from paying fees or having max transaction amount
        excludeFromFees(msg.sender, true);
        excludeFromFees(address(this), true);

    }   
    function mint(uint256 _amount) public onlyRole(DEFAULT_ADMIN_ROLE){
        require(totalSupply() < (1500000000 * 10 ** decimals()), "Only 1.5 billion can be minted" );
        _mint(msg.sender, _amount);
    }
    function setMininmumBalanceForDividends(uint _minimumTokenBalanceForDividends) public onlyRole(DEFAULT_ADMIN_ROLE){
        dividendTracker.setMininmumBalanceForDividends(_minimumTokenBalanceForDividends);
    }
    function updateGasForProcessing(uint256 newValue) public onlyRole(DEFAULT_ADMIN_ROLE) {
        require(newValue >= 200000 && newValue <= 500000, "EverETH: gasForProcessing must be between 200,000 and 500,000");
        require(newValue != gasForProcessing, "EverETH: Cannot update gasForProcessing to same value");
        emit GasForProcessingUpdated(newValue, gasForProcessing);
        gasForProcessing = newValue;
    }

    function updateClaimWait(uint256 claimWait) external onlyRole(DEFAULT_ADMIN_ROLE) {
        dividendTracker.updateClaimWait(claimWait);
    }


    function getClaimWait() external view returns (uint256) {
        return dividendTracker.claimWait();
    }

    function getTotalDividendsDistributed() external view returns (uint256) {
        return dividendTracker.totalDividendsDistributed();
    }

    function isExcludedFromFees(address account) public view returns (bool) {
        return _isExcludedFromFees[account];
    }

    function withdrawableDividendOf(
        address account
    ) public view returns (uint256) {
        return dividendTracker.withdrawableDividendOf(account);
    }

    function dividendTokenBalanceOf(
        address account
    ) public view returns (uint256) {
        return dividendTracker.balanceOf(account);
    }

	function excludeFromDividends(address account) external onlyRole(DEFAULT_ADMIN_ROLE){
	    dividendTracker.excludeFromDividends(account);
	}

    function getAccountDividendsInfo(
        address account
    )
        external
        view
        returns (
            address,
            int256,
            int256,
            uint256,
            uint256,
            uint256,
            uint256,
            uint256
        )
    {
        return dividendTracker.getAccount(account);
    }

    function getAccountDividendsInfoAtIndex(
        uint256 index
    )
        external
        view
        returns (
            address,
            int256,
            int256,
            uint256,
            uint256,
            uint256,
            uint256,
            uint256
        )
    {
        return dividendTracker.getAccountAtIndex(index);
    }

    function processDividendTracker(uint256 gas) external {
        (
            uint256 iterations,
            uint256 claims,
            uint256 lastProcessedIndex
        ) = dividendTracker.process(gas);
        emit ProcessedDividendTracker(
            iterations,
            claims,
            lastProcessedIndex,
            false,
            gas,
            tx.origin
        );
    }

    function getLastProcessedIndex() external view returns (uint256) {
        return dividendTracker.getLastProcessedIndex();
    }

    function getNumberOfDividendTokenHolders() external view returns (uint256) {
        return dividendTracker.getNumberOfTokenHolders();
    }

    function setOpsMarketingWallet(address _opsMarketingAddress) public onlyRole(DEFAULT_ADMIN_ROLE){
        opsMarketingAddress = _opsMarketingAddress;
    }

    function setWallet(string memory _name, address _wallet) public onlyRole(DEFAULT_ADMIN_ROLE) {
    withdrawingAddress[_name] = _wallet;
    }

/// @notice Set the merkle tree root 
/// @param _root Hash of the root
    
    function setRoot(bytes32 _root) external onlyRole(UPDATE_ROOT_ROLE) {
        require(root != _root, "This root is already in use");
        root = _root;
    }

/// @notice Checks if the user is allowed to receive their rewards
/// @param wallet wallet of the user
/// @param amount amount that user wants to receive
/// @param proof a hash from the Merkle tree that proves that the user is allowed to receive rewards
/// @return true / false
    function isEligible(
        address wallet,
        uint256 amount,
        string memory nameOfApp,
        bytes32[] memory proof
    ) public view returns (bool) {
        return
            MerkleProof.verify(
                proof,
                root,
                keccak256(abi.encodePacked(wallet, nameOfApp, taken[wallet] + amount )) 
            );
    }

/// @notice Checks if the user is allowed to receive their rewards
/// @param amount amount that user wants to receive
/// @param proof a hash from the Merkle tree that proves that the user is allowed to receive rewards

    function mintRewards(uint256 amount, string memory nameOfApp, bytes32[] memory proof) public {
        require(isEligible(msg.sender, amount, nameOfApp, proof), "Not eligible");
        _transfer(withdrawingAddress[nameOfApp], msg.sender, amount); 
        emit UserClaimedRewards(msg.sender, amount);
    }

/// @notice Exclude the account from paying any fees
/// @param account account that has to be excluded
/// @param excluded true/false 

    function excludeFromFees(address account, bool excluded) public onlyRole(DEFAULT_ADMIN_ROLE) {
        require(
            _isExcludedFromFees[account] != excluded,
            "T4L: Account is already the value of 'excluded'"
        );
        _isExcludedFromFees[account] = excluded;

        emit ExcludeFromFees(account, excluded);
    }

/// @notice If the caller owns at least half a million tokens, he can receive dividends

    function claimRewards() external {
        dividendTracker.processAccount(payable(msg.sender), false);

        emit UserCalledClaimRewards(msg.sender);
    }

    function _transfer(
        address from,
        address to,
        uint amount
    ) internal override {
        if (amount == 0) {
            return;
        }
        bool takeFee = true;
        // if any account belongs to _isExcludedFromFee account then remove the fee
        if(_isExcludedFromFees[from] == true ||_isExcludedFromFees[to] == true) {
             takeFee = false;
        }
        if(takeFee){
            uint256 fees = percentOf(amount, totalFees) ;
            amount = amount - fees;

            super._transfer(from, address(this), fees);
            require(to != address(0), "ERC20: transfer to the zero address");
            if (!swapping &&  !hasRole(DEFAULT_ADMIN_ROLE, from) &&!hasRole(DEFAULT_ADMIN_ROLE, to) && takeFee) {
                swapping = true;

                uint256 swapTokens = percentOf(amount, liquidityPoolFee);
                swapAndLiquify(swapTokens);

                uint256 sellTokens =  percentOf(amount, ethRewardsFee);
                swapAndSendDividends(sellTokens);

                uint256 _opsMarketingFee =  percentOf(amount, opsMarketingFee);
                swapTokensForWETH(opsMarketingAddress, _opsMarketingFee); /////////////////////////////////
                swapping = false;
            }
        }
            
            super._transfer(from, to, amount);

        try
            dividendTracker.setBalance(payable(from), balanceOf(from))
        {} catch {}
        try dividendTracker.setBalance(payable(to), balanceOf(to)) {} catch {}
        if (!swapping) {
            uint256 gas = gasForProcessing;

            try dividendTracker.process(gas) returns (
                uint256 iterations,
                uint256 claims,
                uint256 lastProcessedIndex
            ) {
                emit ProcessedDividendTracker(
                    iterations,
                    claims,
                    lastProcessedIndex,
                    true,
                    gas,
                    tx.origin
                );
            } catch {}
        }
    }

/// @notice Swaps half of tokens to WETH and mints new position with it, if its already minted - increases liquidity
/// @notice of that position 
/// @param tokens the amount of T4L tokens

    function swapAndLiquify(uint256 tokens) private {
        // split the fee for lp into halves
        uint256 half = tokens / (2);
        uint256 otherHalf = tokens - (half);
        uint24 poolFee = 500;
        uint256 amount0ToMint = half;
        uint256 amount1ToMint = swapTokensForWETH(address(this), otherHalf);

        if (liquidityPositionID == 0) {
            TransferHelper.safeApprove(
                address(this),
                address(nonfungiblePositionManager),
                amount0ToMint
            );
            TransferHelper.safeApprove(
                WETH,
                address(nonfungiblePositionManager),
                amount1ToMint
            );
            INonfungiblePositionManager.MintParams
                memory mintingParams = INonfungiblePositionManager.MintParams({
                    token0: address(this),
                    token1: WETH,
                    fee: poolFee,
                    tickLower: -887270,
                    tickUpper: 887270,
                    amount0Desired: amount0ToMint,
                    amount1Desired: amount1ToMint,
                    amount0Min: amount0ToMint - percentOf(amount0ToMint,500),
                    amount1Min: amount1ToMint - percentOf(amount1ToMint,500),
                    recipient: address(this),
                    deadline: block.timestamp
                });

            (
                uint256 tokenId,
                uint256 liquidity, uint amount0 , uint amount1
            ) = nonfungiblePositionManager.mint(mintingParams);
            liquidityPositionID = tokenId;
            emit SwapAndLiquify(liquidity, amount0, amount1);

        }
        else{
        // swap tokens for WETH
        swapTokensForWETH(address(this), half); 

       
        uint256 _amount0ToMint = half;
        uint256 _amount1ToMint = otherHalf;
        TransferHelper.safeApprove(
            address(this),
            address(nonfungiblePositionManager),
            _amount0ToMint
        );
        TransferHelper.safeApprove(
            WETH,
            address(nonfungiblePositionManager),
            _amount1ToMint
        );
        INonfungiblePositionManager.IncreaseLiquidityParams
            memory params = INonfungiblePositionManager
                .IncreaseLiquidityParams({
                    tokenId: liquidityPositionID,
                    amount0Desired: _amount0ToMint,
                    amount1Desired: _amount1ToMint,
                    amount0Min: 0,
                    amount1Min: 0, /////////
                    deadline: block.timestamp
                });
        (uint256 liquidity, uint amount0 , uint amount1) = nonfungiblePositionManager.increaseLiquidity(params); //(otherHalf, newBalance);
             emit SwapAndLiquify(liquidity, amount0, amount1);

        }
        

    }

/// @notice Swaps tokens to WETH and sends them to holders
/// @param tokens the amount of T4L tokens

    function swapAndSendDividends(uint256 tokens) private {
        uint256 balanceWETH = IERC20(WETH).balanceOf(address(this));
        swapTokensForWETH(address(this), tokens);
        uint256 dividends = IERC20(WETH).balanceOf(address(this)) - balanceWETH;
        bool success = IERC20(WETH).transfer(
            address(dividendTracker),
            dividends
        );

        if (success) {
            dividendTracker.distributeEthereumDividends(dividends);
            emit SendDividends(tokens, dividends);
        }
    }
/// @notice Swaps tokens to WETH
/// @param _recepient the address that has to receive WETH
/// @param _tokenAmount the amount of T4L tokens
/// @return amountOut amount of WETH tokens
    function swapTokensForWETH(
        address _recepient,
        uint256 _tokenAmount
    ) private returns (uint256 amountOut) {
        require(_tokenAmount > 0, "Token amount must be greater than zero");

        // Approve the router to spend T4L.
        TransferHelper.safeApprove(
            address(this),
            address(swapRouter),
            _tokenAmount
        );
        // Call the swap
        amountOut = swapRouter.exactInputSingle(
            ISwapRouter.ExactInputSingleParams({
                tokenIn: address(this),
                tokenOut: WETH,
                fee: 500,
                recipient: _recepient,
                deadline: block.timestamp,
                amountIn: _tokenAmount,
                amountOutMinimum: 0,
                sqrtPriceLimitX96: 0
            })
        );
    }

/// @notice Calculates the percentage of a number
/// @param amount the amount of tokens
/// @param bips percent times 100
/// @return the calculated percentage of amount


    function percentOf(
        uint256 amount,
        uint16 bips
    ) internal pure returns (uint256) {
        return (amount * bips) / 10_000;
    }

/// @notice Calculates cost of T4L
/// @param amountT4L the amount of tokens
/// @return amountOut cost of tokens in WETH

    function priceOfT4L(uint256 amountT4L) public returns (uint256 amountOut) {
        IQuoterV2 quoter = IQuoterV2(quoterAddress);
        IQuoterV2.QuoteExactInputSingleParams memory params = IQuoterV2
            .QuoteExactInputSingleParams({
                tokenIn: address(this),
                tokenOut: WETH,
                amountIn: amountT4L,
                fee: 500,
                sqrtPriceLimitX96: 0
            });
        (amountOut, , , ) = quoter.quoteExactInputSingle(params);
    }

}

