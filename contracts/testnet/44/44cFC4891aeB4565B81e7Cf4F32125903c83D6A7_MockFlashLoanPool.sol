contract MockFlashLoanPool is IFlashLoanPool, Ownable {
  using SafeERC20 for IERC20;
  address public WETH;

  IWETHVault public WETHVault;

  constructor(IWETHVault _vault) {
    WETHVault = _vault;
    WETH = address(_vault.WETH());
  }

  function setWETHVault(IWETHVault _vault) public onlyOwner {
    WETHVault = _vault;
  }

  function flashLoan(
    address receiverAddress,
    address[] calldata assets,
    uint256[] calldata amounts,
    uint256[] calldata modes,
    address onBehalfOf,
    bytes calldata params,
    uint16 referralCode
  ) external {
    referralCode;
    modes;
    onBehalfOf;

    IMintableERC20 assetToken = IMintableERC20(assets[0]);
    uint256 balance = assetToken.balanceOf(address(this));

    if (balance < amounts[0]) {
      WETHVault.withdrawToken(address(assetToken), address(this), amounts[0] - balance);
    }
    assetToken.transfer(receiverAddress, amounts[0]);

    uint256[] memory premiums = new uint256[](assets.length);
    premiums[0] = (amounts[0] * 9) / 10000;

    IFlashLoanReceiver(receiverAddress).executeOperation(assets, amounts, premiums, msg.sender, params);

    assetToken.transferFrom(receiverAddress, address(this), amounts[0] + premiums[0]);
  }

  function flashLoanSimple(address receiverAddress, address asset, uint256 amount, bytes calldata params, uint16 referralCode) external {
    referralCode;

    IMintableERC20 assetToken = IMintableERC20(asset);
    uint256 balance = assetToken.balanceOf(address(this));

    if (balance < amount) {
      WETHVault.withdrawToken(address(assetToken), address(this), amount - balance);
    }
    assetToken.transfer(receiverAddress, amount);

    uint256 premium = (amount * 9) / 10000;

    IFlashLoanSimpleReceiver(receiverAddress).executeOperation(asset, amount, premium, msg.sender, params);

    assetToken.transferFrom(receiverAddress, address(this), amount + premium);
  }

  function flash(address recipient, uint256 amount0, uint256 amount1, bytes calldata data) external {
    // user recipient as flashloan asset while test in testnets.
    address asset = recipient;
    address receiverAddress = msg.sender;

    IMintableERC20 assetToken = IMintableERC20(asset);
    uint256 balance = assetToken.balanceOf(address(this));
    uint256 amount = amount0 > 0 ? amount0 : amount1;

    if (balance < amount) {
      WETHVault.withdrawToken(address(assetToken), address(this), amount - balance);
    }
    uint256 beforeBalance = assetToken.balanceOf(address(this));
    assetToken.transfer(receiverAddress, amount);

    uint256 fee0 = (amount0 * 9) / 10000;
    uint256 fee1 = (amount1 * 9) / 10000;

    uint256 fee = fee0 > 0 ? fee0 : fee1;

    IAlgebraFlashCallback(msg.sender).algebraFlashCallback(fee0, fee1, data);

    require(assetToken.balanceOf(address(this)) >= beforeBalance + fee, "flashloan failed in the last step...");
  }

