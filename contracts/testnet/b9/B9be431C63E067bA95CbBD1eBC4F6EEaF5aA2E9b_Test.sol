contract List{
    using SafeMath for uint256;
    using Math for uint256;

    mapping(address => Record[]) public records;
    struct Record{
        uint256 amount;
        string typeString;
        bool increase;
    }

    function push(uint256 amount) public{
        records[msg.sender].push(Record({
            amount: amount,
            typeString: "TEST",
            increase: true
        }));
    }

    function recordLen(address account) public view  returns (uint256) {
        return records[account].length;
    }

    function getRecordList(address account, uint256 start, uint256 length) public view 
    returns(uint256[] memory amounts, string[] memory typeStrings, bool[] memory increases){
        uint256 recordLength = recordLen(account);
        uint256 end = recordLen(account).min(start+length);
        (,length) = end.trySub(start);
        amounts = new uint256[](length);
        typeStrings = new string[](length);
        increases = new bool[](length);
        Record storage record;
        for(uint i=start; i<end; i++){
            record = records[account][recordLength-1-i];
            amounts[i-start] = record.amount;
            typeStrings[i-start] = record.typeString;
            increases[i-start] = record.increase;
        }
    }
}

