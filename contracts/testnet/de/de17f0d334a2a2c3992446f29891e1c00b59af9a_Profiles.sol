    }

    struct UpdatableByUserData {
        // Struct to facilitate ProfileData modifications by users
        string pseudo;
        string profilePictureURL;
        string coverPictureURL;
    }

    struct ThirdParty {
        uint256 thirdPartyId;
        uint256 userId;
    }

    error ProfileAlreadyRegistered(address _player);
    error ProfileNotRegistered(address _player);
    error WrongSignature();
    error ThirdPartyNameCannotBeEmpty();
    error ProfileAlreadyLinked(address _player, uint256 _thirdPartyId);
    error PseudoCannotBeEmpty();
    error PseudoNotAvailable();
    error TokenIdNotFound();

    event Mint(address _userAddress, string _pseudo);
    event Burn(address _userAddress);
    event Update(address _userAddress, string _pseudo);
    event IncreaseReputation(address _userAddress, uint256 _amount);
    event DecreaseReputation(address _userAddress, uint256 _amount);
    event AddSupportedThirdParty(string _name);
    event LinkThirdPartyToProfile(address _userAddress, uint256 _thirdPartyId, uint256 _thirdPartyUserId);
    event UnlinkThirdPartyToProfile(address _userAddress, uint256 _thirdPartyId);

    function mint(UpdatableByUserData calldata _userData, bytes calldata _signature) external;

    function update(UpdatableByUserData calldata _userData) external;

    function increaseReputation(address _userAddress, uint256 _amount) external;

    function decreaseReputation(address _userAddress, uint256 _amount) external;

    function addThirdParty(string calldata _thirdPartyName) external;

    function linkThirdPartyToProfile(
        address _profileAddress,
        uint256 _thirdPartyId,
        uint256 _thirdPartyUserID
    ) external;

    function unlinkThirdPartyFromProfile(address _profileAddress, uint256 _thirdPartyId) external;

    function setContractURI(string calldata _contractURI) external;

    function getRegisteredAddresses() external view returns (address[] memory);

    function getThirdParties() external view returns (string[] memory);

    function getProfileData(address _userAddress) external view returns (ProfileData memory);

    function getProfilePictures(address _user) external view returns (string memory, string memory);

    function getReputation(address _userAddress) external view returns (uint256, uint256);

    function hasProfileData(address _userAddress) external view returns (bool);

    function isAvailable(string calldata _pseudo) external view returns (bool);

    function generateHash(UpdatableByUserData calldata _userData) external pure returns (bytes32);
}

