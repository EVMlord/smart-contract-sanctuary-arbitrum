     *
     * mint
     *
     */

    function mintAndBurnTokensManyToOne(uint256[] memory _tokenIds)
        public
        payable
        whenMintPaid(1)
        whenNotPaused
    {
        require(_tokenIds.length > 1, "there must be multiple token IDs");

        string memory unicodes;

        for (uint256 i = 0; i < _tokenIds.length; i++) {
            if (i > 0) {
                unicodes = string(abi.encodePacked(unicodes, ","));
            }

            unicodes = string(
                abi.encodePacked(unicodes, globalTokenCodes[_tokenIds[i]])
            );

            burnToken(_tokenIds[i]);
        }

        privateMintTokens(1, unicodes);
    }

    function mintAndBurnTokensOneToMany(uint256 _tokenId)
        public
        payable
        whenMintPaid(1)
        whenNotPaused
    {
        Slice memory s = newSlice(globalTokenCodes[_tokenId]);
        Slice memory d = newSlice(",");
        uint256 c = sliceCount(s, d) + 1;

        require(c > 1, "there must be multiple unicode codepoints");

        for (uint256 i = 0; i < c; i++) {
            privateMintTokens(1, toString(split(s, d)));
        }

        burnToken(_tokenId);
    }

