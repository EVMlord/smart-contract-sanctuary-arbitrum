contract CoinGame is Owned, ERC20 {
    event RequestedRandomNumbers(uint indexed requestId, uint32 size);
    event ReceivedRandomNumbers(uint indexed requestId, uint response);

    event Divisor(uint value);

    event Winner(address winner, uint prize);

    event Debug(string msg, uint value);

    event NewTickets(address user, uint numTickets);

    event Tickets(address user, uint[] tickets);

    event SellTaxes();

    uint private constant TOTAL_SUPPLY = 777_777_777_777 * 10**18;

    IUniswapV2Router02 public router;
    IUniswapV2Factory public factory;
    IUniswapV2Pair public pair;

    //
    // The tax to deduct, in basis points
    //
    uint public constant BUY_TAX_BPS = 50;
    uint public constant SELL_TAX_BPS = 450;

    //
    // I only get paid when someone wins the prize.
    //
    uint public constant MY_CUT_BPS = 1500;

    //
    // The addresses for each user that bought tickets, in
    // chronological order.
    //
    using DoubleEndedQueue for DoubleEndedQueue.Bytes32Deque;
    DoubleEndedQueue.Bytes32Deque public ticketBuyers;
    uint public totalTicketsAtTimeOfVRFRequest; // TODO: accumulate as buyers are added
    //
    // Since multiple tickets can be bought at once, this parallel
    // dequeue shows tickets were bought for each element in the
    // ticketBuyers dequeue. This prevents having to insert addresses
    // repeatedly into ticketBuyers per ticket.
    //
    DoubleEndedQueue.Bytes32Deque public ticketsPerBuyer;

    //
    // Query in the frontend
    //
    address [] public revealedAddresses;
    uint [] public revealedValues;
    uint public numRevealed;

    //
    // This number must be on the winning ticket.
    //
    uint24 public constant MAGIC_NUMBER = 1;

    address payable[] public winners;
    uint [] public earnings;

    uint public immutable MAX_WINNERS;

    uint public startTime;

    //
    // Time at which the probability of a ticket having the magic
    // number is 100%.
    //
    uint public endTime;

    uint public constant START_ODDS = 1_000_000;

    uint public constant END_ODDS = 1;

    bool isSellingCollectedTaxes;

    bool public isLaunched;

    //
    // The maximum number of tickets that can be processed from a
    // single VRF callback.
    //
    uint public maxRandomIterations;

    //
    // How much to spend to get one ticket. This is based on the
    // nominal amount of tokens received after slippage and other
    // fees.
    //
    uint public bnbPerTicket;

    IRandomizer public randomizer;
    uint32 callbackGasLimit = 2_500_000;
    uint16 requestConfirmations = 3;
    bool public isPendingRequest;
    uint public pendingRequestId;

    modifier onlyTestnet() {
        require(isTestnet(), "testnet only");
        _;
    }

    modifier lockTheSwap() {
        isSellingCollectedTaxes = true;
        _;
        isSellingCollectedTaxes = false;
    }

    constructor(uint maxWinners)
        Owned(msg.sender)
        ERC20("test", "TEST", 18)
    {
        MAX_WINNERS = maxWinners;

        _mint(owner, TOTAL_SUPPLY);
        if (isTestnet()) {
            randomizer = IRandomizer(0x923096Da90a3b60eb7E12723fA2E1547BA9236Bc);
            router = IUniswapV2Router02(0x413B7f9FB17f27C5327e3b06B31477BD91c33FF9);
        } else  {
            randomizer = IRandomizer(0x5b8bB80f2d72D0C85caB8fB169e8170A05C94bAF);
            router = IUniswapV2Router02(0x1b02dA8Cb0d097eB8D57A175b88c7D8b47997506);
        }
        factory = IUniswapV2Factory(router.factory());

        // Approve infinite spending by DEX, to sell tokens collected via tax.
        allowance[address(this)][address(router)] = type(uint).max;
        emit Approval(address(this), address(router), type(uint).max);

        maxRandomIterations = 50;
        bnbPerTicket = 100_000_000_000_000_000;

        startTime = block.timestamp;
        endTime = block.timestamp + 7 days;
    }

    function getNumberOfTicketBuyers() external view returns (uint) {
        return ticketBuyers.length();
    }

    function getNumberOfTicketsForBuyerIndex(uint ticketBuyerIndex) external view returns (uint) {
        return uint(ticketsPerBuyer.at(ticketBuyerIndex));
    }

    function getNumberOfWinners() public view returns (uint) {
        return winners.length;
    }

    function getTotalNumberOfTickets() public view returns (uint) {
        uint x;
        for (uint i = 0; i < ticketsPerBuyer.length(); i++) {
            x += uint(ticketsPerBuyer.at(i));
        }
        return x;
    }

    function getTotalNumberOfTicketsForAddress(address addr) public view returns (uint) {
        uint x;
        for (uint i = 0; i < ticketBuyers.length(); i++) {
            if (address(uint160(uint(ticketBuyers.at(i)))) == addr) {
                x += uint(ticketsPerBuyer.at(i));
            }
        }
        return x;
    }

    function getRevealedAddresses(uint start, uint end) external view returns (address[] memory) {
        require(start < end);
        require(end <= revealedAddresses.length);
        address[] memory addys = new address[](end - start);

        for (uint i = 0; i < end - start; i++) {
            addys[i] = revealedAddresses[i + start];
        }
        return addys;
    }

    function getRevealedValues(uint start, uint end) external view returns (uint[] memory) {
        require(start < end);
        require(end <= revealedValues.length);
        uint[] memory values = new uint[](end - start);

        for (uint i = 0; i < end - start; i++) {
            values[i] = revealedValues[i + start];
        }
        return values;
    }

    function isTestnet() public view returns (bool) {
        return block.chainid == 421613;
    }

    function setMaxRandomIterations(uint newSize) external onlyOwner {
        require(newSize > 0, "size must be nonzero");
        maxRandomIterations = newSize;
    }

    function setBnbPerTicket(uint x) external onlyOwner {
        bnbPerTicket = x;
    }

    function setCallbackGasLimit(uint32 x) external onlyOwner {
        callbackGasLimit = x;
    }

    function setRequestConfirmations(uint16 x) external onlyOwner {
        requestConfirmations = x;
    }

    /**
     * @dev Open the floodgates.
     * @param amount Amount of tokens to pair with msg.value
     */
    function launch(uint amount) external payable onlyOwner {
        require(!isLaunched, "already launched");
        isLaunched = true;

        bool sent = transferFrom(msg.sender, address(this), amount);
        require(sent, "couldn't transfer");

        /*(uint amountToken, uint amountETH, uint liquidity) =*/ router.addLiquidityETH{ value: msg.value }(
            address(this),
            amount,
            0,
            0,
            owner,
            block.timestamp);
        //emit Debug("launch addLiquidity amountToken", amountToken);
        //emit Debug("launch addLiquidity amountETH", amountETH);
        //emit Debug("launch addLiquidity liquidity", liquidity);

        pair = IUniswapV2Pair(factory.getPair(address(this), router.WETH()));

        // And for removing liquidity at the end.
        pair.approve(address(router), type(uint).max);
    }

    /**
     * @dev Calculate the amount of tax to apply to a transaction.
     * @param from the sender
     * @param to the receiver
     * @param amount the quantity of tokens being sent
     * @return the amount of tokens to withhold for taxes
     */
    function calcTax(address from, address to, uint amount) internal view returns (uint) { // TODO Commented out view because of the debug logging
        if (winners.length == MAX_WINNERS) {
            // No tax after the game ends
            //emit Debug("calcTax is 0 (no tax after game ends)", 1337);
            return 0;
        } else if (from == owner || to == owner || from == address(this)) {
            // For adding liquidity at the beginning and removing at
            // the end.
            //
            // Also for this contract selling the collected tax.
            //emit Debug("calcTax is 0 (from/to owner or from token address)", 1337);
            return 0;
        } else if (from == address(pair)) {
            // Buy from DEX, or adding liquidity.
            //emit Debug("calcTax on buy", 1337);
            return amount * BUY_TAX_BPS / 10_000;
        } else if (to == address(pair)) {
            // Sell from DEX, or removing liquidity.
            //emit Debug("calcTax on sell", 1337);
            return amount * SELL_TAX_BPS / 10_000;
        } else {
            // Sending to other wallets (e.g. OTC) is tax-free, but no
            // tax means no dice roll.
            //emit Debug("calcTax is 0 (transfer)", 1337);
            return 0;
        }
    }

    /**
     * @dev Calculate the BNB price for a given token quantity,
     *      including slippage.
     * @param amount the quantity of tokens to quote a price for
     * @return the equivalent amount in BNB
     */
    function calcBNBAmount(uint amount) public view returns (uint) {
        address[] memory path = new address[](2);
        path[0] = address(this);
        path[1] = router.WETH();
        uint[] memory amounts = router.getAmountsOut(amount, path);
        return amounts[1];
    }

    /**
     * @dev Calculate the number of tickets earned for a given order
     *      size.
     * @param from the sender
     * @param to the receiver
     * @param amount the number of tokens sent
     * @return the number of tickets earned
     */
    function numberOfTicketsEarned(address from, address to, uint amount) public view returns (uint) {
        if (from == address(pair) && to != address(this)) {
            if (isTestnet()) {
                return 5;
            } else {
                return calcBNBAmount(amount) / bnbPerTicket;
            }
        }
        return 0;
    }

    /**
     * @dev Sell the balance accumulated from taxes.
     */
    function sellCollectedTaxes() internal lockTheSwap {
        //emit Debug("sellCollectedTaxes", balanceOf[address(this)]);
        address[] memory path = new address[](2);
        path[0] = address(this);
        path[1] = router.WETH();

        router.swapExactTokensForETHSupportingFeeOnTransferTokens(
            balanceOf[address(this)],
            0,
            path,
            address(this),
            block.timestamp
        );
        emit SellTaxes();
    }

    /**
     * @dev Transfer tokens from the caller to another address.
     * @param to the receiver
     * @param amount the quantity to send
     * @return true if the transfer succeeded, otherwise false
     */
    function transfer(address to, uint amount) public override returns (bool) {
        //emit Debug("in transfer", 0);
        return transferFrom(msg.sender, to, amount);
    }

    /**
     * @dev Transfer tokens from one address to another. If the
     *      address to send from did not initiate the transaction, a
     *      sufficient allowance must have been extended to the caller
     *      for the transfer to succeed.
     * @param from the sender
     * @param to the receiver
     * @param amount the quantity to send
     * @return true if the transfer succeeded, otherwise false
     */
    function transferFrom(
        address from,
        address to,
        uint amount
    ) public override returns (bool) {
        if (from != msg.sender) {
            // This is a typical transferFrom
            //emit Debug("This is a typical transferFrom", 0);

            uint allowed = allowance[from][msg.sender]; // Saves gas for limited approvals.

            if (allowed != type(uint).max) allowance[from][msg.sender] = allowed - amount;
        }

        // Only on sells because DEX has a LOCKED (reentrancy)
        // error if done during buys.
        //
        // isSellingCollectedTaxes prevents an infinite loop.
        if (balanceOf[address(this)] > 0 && !isSellingCollectedTaxes && from != address(pair) && from != address(this)) {
            //emit Debug("selling collected taxes", balanceOf[address(this)]);
            sellCollectedTaxes();
        } else {
            //emit Debug("not selling collected taxes", balanceOf[address(this)]);
        }

        // Needs to be calculated before any swaps occur
        uint numTickets = numberOfTicketsEarned(from, to, amount);

        uint tax = calcTax(from, to, amount);
        uint afterTaxAmount = amount - tax;

        //emit Debug("amount", amount);
        /*
        //temp
        if (from != address(this)) {
        //emit Debug("bnb amount", calcBNBAmount(amount));
        }
        */
        //emit Debug("numTickets earned", numTickets);
        //emit Debug("tax", tax);
        //emit Debug("afterTaxAmount", afterTaxAmount);

        //emit Debug("balanceOf[from] before", balanceOf[from]);
        balanceOf[from] -= amount;
        //emit Debug("balanceOf[from] after", balanceOf[from]);

        // Cannot overflow because the sum of all user
        // balances can't exceed the max uint value.
        //emit Debug("balanceOf[to] before", balanceOf[to]);
        unchecked {
            balanceOf[to] += afterTaxAmount;
        }
        //emit Debug("balanceOf[to] after", balanceOf[to]);

        emit Transfer(from, to, afterTaxAmount);

        if (tax > 0) {
            //emit Debug("balanceOf[address(this)] before", balanceOf[address(this)]);
            unchecked {
                balanceOf[address(this)] += tax;
            }
            //emit Debug("balanceOf[address(this)] after", balanceOf[address(this)]);

            // Any transfer to the contract can be viewed as tax
            emit Transfer(from, address(this), tax);

            if (numTickets > 0) {
                ticketBuyers.pushBack(bytes32(uint(uint160(to))));
                ticketsPerBuyer.pushBack(bytes32(numTickets));
                emit NewTickets(to, numTickets);
            }
        }

        //emit Debug("end transferFrom", 0);
        return true;
    }

    /**
     * @dev Convenience function to get the current divisor, easy to
     *      read externally.
     * @return the current divisor
     */
    function getDivisor() public view returns (uint) {
        return mapIntervalLinear(startTime, endTime, START_ODDS, END_ODDS, block.timestamp);
    }

    /**
     * @dev Map a number from one interval to another, linearly
     * @param a1 the start of the first range
     * @param a2 the end of the first range
     * @param b1 the start of the second range
     * @param b2 the end of the second range
     * @param x a value in [a1..a2]
     * @return the value of x mapped into [b1..b2]
     */
    function mapIntervalLinear(uint a1, uint a2, uint b1, uint b2, uint x) internal pure returns (uint) {
        if (x < a1) {
            return b1;
        } else if (x > a2) {
            return b2;
        }

        uint WAD = 10 ** 18;
        uint t = (x - a1) * WAD / (a2 - a1);
        uint result = b2 * t / WAD + b1 - b1 * t / WAD;

        return result;
    }

    //
    // Cancel the pending VRF request, e.g. if it gets stuck.
    //
    function cancelRandomNumbers() external onlyOwner {
        isPendingRequest = false;
    }

    function requestRandomNumbers() external onlyOwner {
        require(ticketBuyers.length() > 0, "No buyers to get random numbers for");
        require(!isPendingRequest, "request already pending");
        require(winners.length < MAX_WINNERS, "max number of winners already reached");

        pendingRequestId = randomizer.request(callbackGasLimit);

        isPendingRequest = true;
        totalTicketsAtTimeOfVRFRequest = getTotalNumberOfTickets();
        emit RequestedRandomNumbers(pendingRequestId, 1);
    }

    function randomizerCallback(uint256 _id, bytes32 _value) public {
        if (!isTestnet()) {
            require(msg.sender == address(randomizer), "Caller not Randomizer");
        }

        require(isPendingRequest, "no request pending");
        require(pendingRequestId == _id, "request ID mismatch");
        require(winners.length < MAX_WINNERS, "max number of winners already reached");
        isPendingRequest = false;
        uint randomWord = uint(_value);
        emit ReceivedRandomNumbers(_id, randomWord);

        uint divisor = getDivisor();
        emit Divisor(divisor);

        require(ticketBuyers.length() > 0, "no buyers");

        uint totalIterations = Math.min(totalTicketsAtTimeOfVRFRequest, maxRandomIterations);
        uint ticketsLeft = totalIterations;
        while (ticketsLeft > 0) {
            uint buyerNumTickets = uint(ticketsPerBuyer.popFront());

            uint n = Math.min(ticketsLeft, buyerNumTickets);
            uint[] memory tickets = new uint[](n);

            for (uint i = 0; i < n; i++) {
                uint pseudo = uint(keccak256(abi.encodePacked(block.number, blockhash(block.number), randomWord, ticketsLeft - i)));
                uint x = (pseudo % divisor) + 1;
                tickets[i] = x;
                revealedAddresses.push(address(uint160(uint(ticketBuyers.front()))));
                revealedValues.push(x);
                numRevealed++;

                if (x == MAGIC_NUMBER) {
                    address payable w = payable(address(uint160(uint(ticketBuyers.front()))));
                    emit Tickets(w, tickets);
                    endGame(w);
                    ticketBuyers.popFront();
                    return;
                } else {
                    // rip bozo
                }
            }

            if (buyerNumTickets <= ticketsLeft) {
                //
                // All of this buyer's tickets were consumed.
                //
                emit Tickets(address(uint160(uint(ticketBuyers.popFront()))), tickets);
                ticketsLeft -= buyerNumTickets;
            } else {
                //
                // Some of this buyer's tickets remain.
                //
                emit Tickets(address(uint160(uint(ticketBuyers.front()))), tickets);
                ticketsPerBuyer.pushFront(bytes32(buyerNumTickets - ticketsLeft));
                ticketsLeft = 0;
            }
        }
    }

    /**
     * @dev Handle the end of game payout. This should only be called
     *      after a winner has been determined.
     * @param winner the new winner
     */
    function endGame(address payable winner) internal {
        require(winner != address(0), "invalid address");
        require(winners.length < MAX_WINNERS, "max number of winners already reached");

        sellCollectedTaxes();

        uint mine = address(this).balance * MY_CUT_BPS / 10_000;
        (bool sent,) = owner.call{ value: mine }("");
        require(sent, "failed to send to myself");

        emit Winner(winner, address(this).balance);
        winners.push(winner);
        earnings.push(address(this).balance);
        (sent,) = winner.call{value: address(this).balance}(abi.encode("Coin Game was a 7 day psychological experiment conducted by Degenerate Enterprises. This concludes the program. Thank you for your cooperation."));
        require(sent, "failed to send to the winner");

        //
        // New Game+
        //
        startTime = block.timestamp;
        endTime = block.timestamp + 7 days;
    }

