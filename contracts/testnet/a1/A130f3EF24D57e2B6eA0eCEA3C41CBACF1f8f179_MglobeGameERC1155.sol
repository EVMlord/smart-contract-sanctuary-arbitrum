	function mintErc1155(        
        address to,
        uint256 nftTypeIndex   //0-采集器 1-载具, 2-...
    ) 
        external
        whenNotPaused
        //nonReentrant
    {
		// require(to != address(0x0), "MglobeGameNFTs#mintErc1155: to can't be empty");
        
        Material memory material = _materials[nftTypeIndex];
        require(material.enabled);

        address factoryContractAddress = material.FactoryContractAddress;

        require(keccak256(abi.encodePacked(factoryContractAddress)) != keccak256(abi.encodePacked("")), "MglobeGameNFTs#mintErc1155: Factory contract address not set");
        
         uint[] memory charactors;

        //自增tokenId
        uint tokenId = _tokenIdTracker.current();
		_tokenIdTracker.increment();

        //判断是否需要扣除Uru
        if (material.UruFee >0) {
            //玩家先提前操作Uru发币合约授权本合约地址，由合约操作将玩家的Uru转到平台运营钱包
            URUTokenContract.safeTransferFrom(_msgSender(), address(this), material.UruFee);
        }
                
        //销毁原材料
        for (uint256 i = 0; i < material.MaterialIds.length; i++) {
            uint256 _id = material.MaterialIds[i];
            uint256 _quantity = material.MaterialFees[i];

            _burn(_msgSender(), _id, _quantity);
            tokenSupply[_id] = tokenSupply[_id].sub(_quantity);

            //非系统token
            if (_id >= 10000 && tokenSupply[_id] == 0) {
                creators[_id] = address(0x0);
            }
        }

        //调用工厂合约生成NFTmetadata数据
        charactors= IFactory(factoryContractAddress).geneMetaData(tokenId); 

        //注意此语句必须在geneMetaData之前
        _mint(to, tokenId, 1, '0');

        _idNftTypeindexes[tokenId] = nftTypeIndex; //用于回收

        creators[tokenId] = to;
        tokenSupply[tokenId] = 1;

        emit OnMinted(to, nftTypeIndex, tokenId, charactors);

	}
    

	function mintByShop(        
        address to,
        uint256 nftTypeIndex,   //0-采集器 1-载具, 2-...
        int256 type_id         //指定的道具id
    ) 
