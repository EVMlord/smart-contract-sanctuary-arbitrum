 */
contract L2UnicornHatcher is L2UnicornHatcherPermit, BaseL2, BaseL2Unicorn, BaseL2UnicornRule, BaseRecipient, BaseGame {

    struct HatchResult {
        uint256 requestId;
        address user;
        bytes32 seed;
        uint8 eSeries;
        uint8 number;
        uint256 randomNumber;
    }

    event HatchCallback (
        address indexed user,
        uint256 requestId,
        bytes32 seed,
        uint8 eSeries,
        uint8 hatchNumber,
        uint8 level,
        uint256 randomNumber,
        uint256 tokenId
    );

    mapping(uint8 => uint256) private _costAmount;

    mapping(uint256 => uint8) private _requestIdHatchCount;

    mapping(uint256 => uint8) private _requestIdESeries;

    constructor(address randomizer_, address l2Unicorn_, address l2UnicornRule_, address l2_, address recipient_, address authorizer_)
        BaseGame(randomizer_, 80)
        BaseL2Unicorn(l2Unicorn_)
        BaseL2UnicornRule(l2UnicornRule_)
        BaseL2(l2_)
        BaseRecipient(recipient_)
        L2UnicornHatcherPermit(authorizer_) {
        _costAmount[1] = 100e18;
        _costAmount[2] = 1000e18;
    }

    /**
     * @notice 孵化 EO
     */
    function hatchE0(uint256 gasPrice_, uint256 callbackGasLimit_, uint256 deadline, uint8 v, bytes32 r, bytes32 s) payable external {
        uint8 eSeries = 0;
        permit(_msgSender(), eSeries, deadline, v, r, s);
        require(IAccessControl(l2Unicorn).hasRole(0xaeaef46186eb59f884e36929b6d682a6ae35e1e43d8f05f058dcefb92b601461, address(this)), "L2UnicornHatcher: access denied");
        require(!IPausable(l2Unicorn).paused(), "L2UnicornHatcher: contract already paused");
        //
        uint256 randomizerFee_ = estimateFeeUsingConfirmationsAndGasPrice(callbackGasLimit_, 2, gasPrice_);
        require(msg.value >= randomizerFee_, "L2UnicornHatcher: randomizer fee insufficient");
        depositFund();
        //
        uint256 requestId_ = afterPlay(_msgSender(), callbackGasLimit_);
        _requestIdHatchCount[requestId_] = 1;
        _requestIdESeries[requestId_] = eSeries;
    }

    /**
     * @notice 孵化 E1 E2
     */
    function hatchE1E2(uint256 gasPrice_, uint256 callbackGasLimit_, uint8 hatchCount_, uint8 eSeries_) payable external {
        require(hatchCount_ > 0 && hatchCount_ <= maximum, "L2UnicornHatcher: exceed maximum");
        require(eSeries_ == 1 || eSeries_ == 2, "L2UnicornHatcher: e series invalid");
        require(IAccessControl(l2Unicorn).hasRole(0xaeaef46186eb59f884e36929b6d682a6ae35e1e43d8f05f058dcefb92b601461, address(this)), "L2UnicornHatcher: access denied");
        require(!IPausable(l2Unicorn).paused(), "L2UnicornHatcher: contract already paused");
