pragma solidity ^0.8.9;
import "./IFeeManager.sol";
import "./DCAStrategyManagerV2.sol";
import "./DCATypes.sol";

contract DefaultFeeManagerV2 is IFeeManager {
    uint256 constant DENOMINATOR = 1000000;
    DCAStrategyManagerV2 dcaStrategyManager;

    constructor(address dcaStrategyManager_) {
        dcaStrategyManager = DCAStrategyManagerV2(dcaStrategyManager_);
    }

    function getFeePercentage(
        uint256 strategyId,
        address /*user*/
    ) public view returns (uint256) {
        DCATypes.StrategyDataV2 memory strategyData = dcaStrategyManager
            .getStrategy(strategyId);
        return strategyData.strategyFee;
    }

    function calculateFee(
        uint256 strategyId,
        address user,
        uint256 amount
    ) external view returns (uint256) {
        uint256 strategyFee = getFeePercentage(strategyId, user);
        uint256 fee = (amount * strategyFee) / DENOMINATOR;

        return fee;
    }
}

