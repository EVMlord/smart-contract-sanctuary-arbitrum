    /*
     * @dev Splits the slice, setting `self` to everything after the first
     *      occurrence of `needle`, and `token` to everything before it. If
     *      `needle` does not occur in `self`, `self` is set to the empty slice,
     *      and `token` is set to the entirety of `self`.
     * @param self The slice to split.
     * @param needle The text to search for in `self`.
     * @param token An output parameter to which the first token is written.
     * @return `token`.
     */
    function splitToken(
        Slice memory _stack,
        Slice memory _delim,
        Slice memory _token
    ) private pure returns (Slice memory) {
        uint256 ptr = findPtr(
            _stack._len,
            _stack._ptr,
            _delim._len,
            _delim._ptr
        );

        _token._ptr = _stack._ptr;
        _token._len = ptr - _stack._ptr;

        if (ptr == _stack._ptr + _stack._len) {
            _stack._len = 0; // not found
        } else {
            _stack._len -= _token._len + _delim._len;
            _stack._ptr = ptr + _delim._len;
        }

        return _token;
    }

    /**
     * Count the number of non-overlapping occurrences of needle in stack.
     */
    function sliceCount(Slice memory _stack, Slice memory _delim)
        private
        pure
        returns (uint256)
    {
        uint256 c;

        uint256 ptr = findPtr(
            _stack._len,
            _stack._ptr,
            _delim._len,
            _delim._ptr
        ) + _delim._len;

        while (ptr <= _stack._ptr + _stack._len) {
            ptr =
                findPtr(
                    _stack._len - (ptr - _stack._ptr),
                    ptr,
                    _delim._len,
                    _delim._ptr
                ) +
                _delim._len;

            c++;
        }

        return c;
    }

    /*
     * @dev Copies a slice to a new string.
     * @param self The slice to copy.
     * @return A newly allocated string containing the slice's text.
     */
    function toString(Slice memory _stack)
        private
        pure
        returns (string memory)
    {
        string memory ret = new string(_stack._len);
        uint256 retptr;

        assembly {
            retptr := add(ret, 32)
        }

        memcpy(retptr, _stack._ptr, _stack._len);

        return ret;
    }

    /**
     *
     * style
     *
     */

    /**
     * Add a globally available token style.
     */
    function addTokenStyle(string memory _style, uint256 _price)
        public
        onlyOwner
        whenNotEmpty(_style)
        whenNotFrozen
        whenStyleNotExists(_style)
    {
        Style storage style = globalAvailableStyles[_style];

        style.name = _style;
        style.price = _price;

        globalStyleNames.push(_style);
    }

    /**
     * Modify a globally available token style.
     */
    function modTokenStyle(string memory _style, uint256 _price)
        public
        onlyOwner
        whenStyleExists(_style)
        whenNotFrozen
    {
        Style storage style = globalAvailableStyles[_style];

        style.price = _price;
    }

    function getAllTokenStyles() public view returns (Style[] memory) {
        Style[] memory styles = new Style[](globalStyleNames.length);
        uint256 c;

        for (uint256 i = 0; i < globalStyleNames.length; i++) {
            string memory name = globalStyleNames[i];

            styles[c].name = globalAvailableStyles[name].name;
            styles[c].price = globalAvailableStyles[name].price;

            c++;
        }

        return styles;
    }

    function getTokenStylePrice(string memory _style)
        public
        view
        whenStyleExists(_style)
        returns (uint256)
    {
        return globalAvailableStyles[_style].price;
    }

    function remTokenStyle(string memory _style)
        public
        onlyOwner
        whenNotFrozen
        whenStyleExists(_style)
    {
        delete globalAvailableStyles[_style];

        string[] memory styles = new string[](globalStyleNames.length.sub(1));
        uint256 c;

        for (uint256 i = 0; i < globalStyleNames.length; i++) {
            bytes32 a = keccak256(abi.encodePacked(_style));
            bytes32 b = keccak256(abi.encodePacked(globalStyleNames[i]));

            if (a == b) {
                continue;
            }

            styles[c] = globalStyleNames[i];

            c++;
        }

        globalStyleNames = styles;
    }

    function setTokenStyle(uint256 _tokenId, string memory _style)
        public
        payable
        whenNotPaused
        whenStyleExists(_style)
        whenStylePaid(_style)
        whenTokenExists(_tokenId)
        whenTokenOwner(_tokenId)
    {
        globalTokenStyles[_tokenId] = _style;
    }

    /**
     * Get the style of a token. May return empty string in case the given token
     * has no configured style.
     */
    function tokenStyle(uint256 _tokenId) public view returns (string memory) {
        return globalTokenStyles[_tokenId];
    }

    /**
     *
     * supply
     *
     */

    function maxSupply() public view returns (uint256) {
        return globalMaxSupply;
    }

    function setMaxSupply(uint256 _newMaxSupply)
        public
        onlyOwner
        whenNotFrozen
    {
        require(_newMaxSupply > globalRebateCount, "max supply must be greater than rebate count"); // prettier-ignore

        globalMaxSupply = _newMaxSupply;
    }

    /**
     *
     * token
     *
     */

    function tokenCodes(uint256 _tokenId)
        public
        view
        virtual
        returns (string memory)
    {
        return globalTokenCodes[_tokenId];
    }

    function tokenURI(uint256 _tokenId)
        public
        view
        virtual
        override
        returns (string memory)
    {
        if (_exists(_tokenId) == false) {
            return "";
        }

        string memory u = baseURI();
        string memory c = tokenCodes(_tokenId);
        string memory s = tokenStyle(_tokenId);

        if (bytes(u).length > 0) {
            u = string(abi.encodePacked(u, "?text=", c));

            if (bytes(s).length > 0) {
                u = string(abi.encodePacked(u, "&style=", s));
            }

            return u;
        }

        return "";
    }

    /**
     *
     * uri
     *
     */

    function _baseURI() internal view override returns (string memory) {
        return globalBaseURI;
    }

    function baseURI() public view returns (string memory) {
        return globalBaseURI;
    }

    /**
     * Configure the base URI applied to every token. Should not contain any URL
     * query parameters.
     */
    function setBaseURI(string memory _newBaseURI)
        public
        onlyOwner
        whenNotFrozen
    {
        globalBaseURI = _newBaseURI;
    }

    /**
     *
     * withdrawal
     *
     */

    function withdrawContract() public onlyOwner {
        uint256 amount = address(this).balance;

        amount = amount.sub(globalMintRebate.mul(globalRebateCount));

        payable(msg.sender).transfer(amount);
    }
}