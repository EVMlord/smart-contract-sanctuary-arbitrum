     */

    function lockPrice() public view returns (uint256) {
        return globalLockPrice;
    }

    function lockToken(uint256 _tokenId)
        public
        payable
        whenLockPaid
        whenTokenExists(_tokenId)
        whenTokenNotLocked(_tokenId)
        whenTokenOwner(_tokenId)
    {
        globalTokenLocks[_tokenId] = true;
    }

    function tokenLock(uint256 _tokenId) public view returns (bool) {
        return globalTokenLocks[_tokenId];
    }

    function unlockToken(uint256 _tokenId)
        public
        payable
        whenLockPaid
        whenTokenExists(_tokenId)
        whenTokenLocked(_tokenId)
        whenTokenOwner(_tokenId)
    {
        delete globalTokenLocks[_tokenId];
    }

    function transferFromAndLock(address from, address to, uint256 _tokenId)
        public
        payable
        whenLockPaid
        whenTokenExists(_tokenId)
        whenTokenNotLocked(_tokenId)
        whenTokenOwner(_tokenId)
    {
        transferFrom(from, to, _tokenId);
        globalTokenLocks[_tokenId] = true;
    }

