  function withdrawERC20(address _token, address _account, uint256 _amount) public onlyOwner {
    IERC20 token = IERC20(_token);
    if (_amount > token.balanceOf(address(this))) {
      _amount = token.balanceOf(address(this));
    }
    token.safeTransfer(_account, _amount);
  }

  function _mintERC20(address _token, uint256 _amount) internal {
    Address.functionCall(_token, abi.encodeWithSignature("mint(address,uint256)", address(this), _amount));
