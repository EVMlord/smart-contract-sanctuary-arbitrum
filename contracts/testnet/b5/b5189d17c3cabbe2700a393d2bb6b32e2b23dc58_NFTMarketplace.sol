pragma solidity ^0.8.9;

import "@openzeppelin/contracts/utils/Counters.sol";
import "@openzeppelin/contracts/utils/Strings.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC721/ERC721Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC721/utils/ERC721HolderUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC721/IERC721ReceiverUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC721/extensions/ERC721URIStorageUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/UUPSUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/security/PausableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/security/ReentrancyGuardUpgradeable.sol";

import "hardhat/console.sol";


struct BuyerHistory {
  address payable seller;
  address payable buyer;
  AllowlistStatus alStatus;
  uint256 eventId;
  uint256 eventDate;
  uint256 expiresAt;
  uint256 price;
  bool claimed;
}

struct SaleEventData {
  address payable seller;
  uint256 eventId;
}

enum AllowlistStatus {
  Raffle,
  Confirmed,
  Unsuccessfull
}

enum DisputeStatus {
  NotDisputed,
  InProgress,
  BuyerError,
  SellerError,
  NoFault,
  Void
}

enum MarketItemStatus{ 
  Available,
  Sold,
  Redeemed,
  Minted,
  Cancelled,
  Disputed
}

struct ProjectMintDate {
  string projectId;
  uint256 mintDate;
}

struct VerifiedData {
  uint8 v;
  bytes32 r;
  bytes32 s;
  string[] data;
}

struct AppFeeParam {
  uint8 v;
  bytes32 r;
  bytes32 s;
  address a;
  uint fee;
}

struct MarketItem {
  uint256 tokenId;
  address payable seller;
  address payable buyer;
  string safeId;
  string projectId;
  uint256 price;
  uint256 expiresAt;
  AllowlistStatus alStatus;
  MarketItemStatus status;
  DisputeStatus disputeStatus;
  uint256 mintDate;
  bool claimed;
}

library NFTMarketplaceData {
  string constant signedPrefix = "\x19Ethereum Signed Message:\n";
  
  string constant LE = "LE"; //"Listing has been expired" : 
  string constant MF = "MF"; //"Mint Date must be in the future": 
  string constant DNR = "DNR"; // "Dispute not resolved yet": 
  string constant GP = "GP"; // "Grace period is not over": 
  string constant UC = "UC"; // "Cannot be claimed": 
  string constant LT = "<0"; // "Must be greater then 0": 
  string constant EXP = "EXP"; // "Expiration date must be in the future": 
  string constant MP = "MP"; // "Missing Parameter":
  string constant INV = "INV"; // "Invalid request":
  string constant EXISTS = "EXISTS"; // "NFT already exists in marketplace":

}


contract NFTMarketplaceDefaults is Initializable, ERC721Upgradeable, ERC721URIStorageUpgradeable, PausableUpgradeable, OwnableUpgradeable, UUPSUpgradeable, IERC721ReceiverUpgradeable, ERC721HolderUpgradeable, ReentrancyGuardUpgradeable {

    function _baseURI() internal pure override returns (string memory) {
        return "test";
    }

    function _burn(uint256 tokenId)
        internal
        override(ERC721Upgradeable, ERC721URIStorageUpgradeable)
    {
        super._burn(tokenId);
    }

    function tokenURI(uint256 tokenId)
        public
        view
        override(ERC721Upgradeable, ERC721URIStorageUpgradeable)
        returns (string memory)
    {
        return super.tokenURI(tokenId);
    }

    function _authorizeUpgrade(address newImplementation)
        internal
        onlyOwner
        override
    {}

    function _beforeTokenTransfer(address from, address to, uint256 tokenId)
        internal
        whenNotPaused
        override
    {
        super._beforeTokenTransfer(from, to, tokenId);
    }

    function pause() public onlyOwner {
        _pause();
    }

    function unpause() public onlyOwner {
        _unpause();
    }
}

contract NFTMarketplace is  NFTMarketplaceDefaults {
    
    using NFTMarketplaceData for SaleEventData;
    using NFTMarketplaceData for AllowlistStatus;
    using NFTMarketplaceData for DisputeStatus;
    using NFTMarketplaceData for MarketItemStatus;
    using NFTMarketplaceData for ProjectMintDate;
    using NFTMarketplaceData for AppFeeParam;
    using NFTMarketplaceData for MarketItem;
    using NFTMarketplaceData for BuyerHistory;

    
    using Counters for Counters.Counter;
    Counters.Counter private _tokenIds;
    Counters.Counter private _itemsSold;
    Counters.Counter private _resaleCounter;
    
    uint256 private gracePeriodInSeconds;
    uint256 private appFee; // Percentage

    address private contractAddress;

    address private verifiedAddr;

    mapping(uint256 => BuyerHistory[]) private idToBuyerHistory;
    mapping(uint256 => MarketItem) private idToMarketItem;
    // mapping(uint256 => bool) private lockedNFTs;
    mapping(string => uint256) private safeToTokenId;

    mapping(string => uint256) private projectIdToMintDate;

    event MarketItemUpdate (
      uint256 indexed tokenId,
      MarketItem item,
      bool created,
      BuyerHistory[] history,
      uint256 timestamp
    );

    /// @custom:oz-upgrades-unsafe-allow constructor
    constructor() {
      _disableInitializers();
      contractAddress = address(this);
      _resaleCounter.increment();
    }

    function initialize() initializer public {
          gracePeriodInSeconds = 72 * 60 * 60;
          appFee = 5;
          verifiedAddr = 0x4663465D853471087158295e3d38A117EE3b80d7;
        __ERC721_init("UNGRIND-TEST", "UNG-T");
        __ERC721URIStorage_init();
        __Pausable_init();
        __Ownable_init();
        __UUPSUpgradeable_init();
    }

    function isExpired(uint256 tokenId) private view returns (bool) {
      return block.timestamp > idToMarketItem[tokenId].expiresAt;
    }

    function isGracePeriodOver(uint256 tokenId) private view returns (bool) {
      if (idToMarketItem[tokenId].mintDate == 0) {
        return false;
      }
      return block.timestamp > idToMarketItem[tokenId].mintDate + gracePeriodInSeconds;
    }

    function getMintDate(uint256 tokenId) private view returns (uint256) {
      uint256 mintDate = idToMarketItem[tokenId].mintDate;
      if (mintDate == 0 && projectIdToMintDate[idToMarketItem[tokenId].projectId] > 0) {
        mintDate = projectIdToMintDate[idToMarketItem[tokenId].projectId];
      }
      return mintDate;
    }

    function isNotExpiredRequired(uint256 tokenId) private view {
      require(isExpired(tokenId) == false, NFTMarketplaceData.LE);
      uint256 mintDate = getMintDate(tokenId);
      if (mintDate > 0) {
        require(mintDate > block.timestamp, NFTMarketplaceData.MF);
      }
    }

    function deductAppFee(uint256 price, uint _appFee) private pure returns (uint256) {
      return price - (price * _appFee / 100);
    }

    function isDataVerified(VerifiedData memory verifiedData) private view returns (bool) {
      bytes memory encMsg = abi.encode(verifiedData.data);
      bytes32 hash = keccak256(abi.encodePacked(NFTMarketplaceData.signedPrefix,Strings.toString(encMsg.length),encMsg));
      return verifiedAddr == ecrecover(hash, verifiedData.v, verifiedData.r, verifiedData.s);
    }

    function isAppFeeParamVerified(AppFeeParam memory appFeeParam) private view returns (bool) {
      bytes memory encMsg = abi.encodePacked(appFeeParam.a, appFeeParam.fee);
      bytes32 hash = keccak256(abi.encodePacked(NFTMarketplaceData.signedPrefix,Strings.toString(encMsg.length),encMsg));
      return verifiedAddr == ecrecover(hash, appFeeParam.v, appFeeParam.r, appFeeParam.s);
    }

    function isValidId(string memory _id) private pure returns (bool) {
      return bytes(_id).length == 24;
    }

    function totalSaleCount(uint256 tokenId) private view returns (uint256) {
      uint256 saleCount = idToBuyerHistory[tokenId].length;
      if (hasCurrentBuyer(tokenId) == true) {
        saleCount += 1;
      }
      return saleCount;
    }

    function continueAllowList(uint256 tokenId) private returns (bool) {
      if (idToMarketItem[tokenId].alStatus == AllowlistStatus.Raffle || idToMarketItem[tokenId].alStatus == AllowlistStatus.Unsuccessfull) {
        idToMarketItem[tokenId].status = MarketItemStatus.Disputed;
        idToMarketItem[tokenId].disputeStatus = DisputeStatus.SellerError;
        return false;
      }
      return true;
    }

    function isBuyerIncludingHistory(uint256 tokenId, address buyer)  private view returns (bool)  {
      bool isBuyer = idToMarketItem[tokenId].buyer == buyer;
      if (isBuyer == false) {
        uint arrayLength = idToBuyerHistory[tokenId].length;
        for (uint i=0; i<arrayLength; i++) {
            isBuyer = idToBuyerHistory[tokenId][i].buyer == buyer;
            if (isBuyer == true) break;
        }
      }
      return isBuyer;
    }

    function currentBuyer(uint256 tokenId) private view returns (address)  {
      if (hasCurrentBuyer(tokenId)) {
        return idToMarketItem[tokenId].buyer;
      }
      if (totalSaleCount(tokenId) > 0) {
        return idToMarketItem[tokenId].seller;
      }
      return address(0);
    }

    function hasCurrentBuyer(uint256 tokenId) private view returns (bool)  {
      return idToMarketItem[tokenId].buyer != address(0);
    }

    function isClaimedByAll(uint256 tokenId) private view returns (bool) {
      bool claimedByAll = true;
      if (hasCurrentBuyer(tokenId)) {
        claimedByAll = idToMarketItem[tokenId].claimed;
      }
      if (claimedByAll) {
        uint arrayLength = idToBuyerHistory[tokenId].length;
        for (uint i=0; i<arrayLength; i++) {
            claimedByAll = idToBuyerHistory[tokenId][i].claimed;
            if (claimedByAll == false) break;
        }
      }
      return claimedByAll;
    }

    function burnIfConditionsAreMet(uint256 tokenId) private returns (bool) {
      if (isClaimedByAll(tokenId) == true) {
        _burn(tokenId);
        return true;
      }
      return false;
    }

    function _claimFunds(uint256 tokenId, uint256 eventId, uint _appFee) private {
      // TODO: Check if Minted
      if (idToMarketItem[tokenId].status ==  MarketItemStatus.Disputed) {
        require(
          idToMarketItem[tokenId].disputeStatus == DisputeStatus.BuyerError 
          || idToMarketItem[tokenId].disputeStatus == DisputeStatus.NoFault
          , NFTMarketplaceData.DNR);
      }
      require(isGracePeriodOver(tokenId) == true, NFTMarketplaceData.GP);

      if (msg.sender == idToMarketItem[tokenId].seller && eventId <= 0) {
        require(
          (
            idToMarketItem[tokenId].status == MarketItemStatus.Sold 
            || idToMarketItem[tokenId].status == MarketItemStatus.Redeemed
            || idToMarketItem[tokenId].status == MarketItemStatus.Minted
          ), NFTMarketplaceData.UC);
        require(idToMarketItem[tokenId].claimed == false, NFTMarketplaceData.UC);
        idToMarketItem[tokenId].claimed = true;
        payable(idToMarketItem[tokenId].seller).transfer(deductAppFee(idToMarketItem[tokenId].price, _appFee));
        // Transfer to seller
      } else {
        
        uint arrayLength = idToBuyerHistory[tokenId].length;
        for (uint i=0; i<arrayLength; i++) {
          if (idToBuyerHistory[tokenId][i].seller == msg.sender && idToBuyerHistory[tokenId][i].claimed == false) {
            
            if (eventId > 0 && idToBuyerHistory[tokenId][i].eventId != eventId) {
              continue;
            }
            // Transfer to Seller
            idToBuyerHistory[tokenId][i].claimed = true;
            idToBuyerHistory[tokenId][i].eventDate = block.timestamp;
            payable(idToBuyerHistory[tokenId][i].seller).transfer(deductAppFee(idToBuyerHistory[tokenId][i].price, _appFee));
            break;
          }
        }
      }
      burnIfConditionsAreMet(tokenId);
      emitMarketEent(tokenId, false);
    }

    function emitMarketEent(uint256 tokenId, bool created) private {
      emit MarketItemUpdate(
        tokenId,
        idToMarketItem[tokenId],
        created,
        idToBuyerHistory[tokenId],
        block.timestamp
      );
    }

    function createListing(string memory _tokenURI, string memory safeId, uint256 price, uint256 expiresAt, AllowlistStatus alStatus, VerifiedData memory verifiedData) public whenNotPaused nonReentrant payable returns (uint) {
      return _createToken(_tokenURI, safeId, price, expiresAt, alStatus, verifiedData);
    }

    function _createToken(string memory _tokenURI, string memory safeId, uint256 price, uint256 expiresAt, AllowlistStatus alStatus, VerifiedData memory verifiedData) private returns (uint) {
      require(price >= 0, NFTMarketplaceData.LT);
      require(expiresAt > block.timestamp, NFTMarketplaceData.EXP);
      require(bytes(safeId).length > 0, NFTMarketplaceData.MP);
      require(safeToTokenId[safeId] == 0, NFTMarketplaceData.EXISTS);
      require(isValidId(safeId), NFTMarketplaceData.INV);
      // TODO: check if safeId is valid

      string memory verifiedProjectId;
      bool _isDataVerified = isDataVerified(verifiedData);
      if (_isDataVerified) {
        verifiedProjectId = verifiedData.data[0];
        require(isValidId(verifiedProjectId), NFTMarketplaceData.INV);
      }

      _tokenIds.increment();
      uint256 newItemId = _tokenIds.current();

      safeToTokenId[safeId] = newItemId;
      _mint(contractAddress, newItemId);
      setApprovalForAll(address(this), true);
      _setTokenURI(newItemId, _tokenURI);
      createMarketItem(newItemId, safeId, verifiedProjectId, price, expiresAt, alStatus);
      return newItemId;
    }

    function createMarketItem(
      uint256 tokenId,
      string memory safeId,
      string memory verifiedProjectId,
      uint256 price,
      uint256 expiresAt,
      AllowlistStatus alStatus
    ) private {
      require(price >= 0, NFTMarketplaceData.LT);
      idToMarketItem[tokenId] =  MarketItem(
        tokenId,
        payable(msg.sender),
        payable(address(0)),
        safeId,
        verifiedProjectId,
        price,
        expiresAt,
        alStatus,
        MarketItemStatus.Available,
        DisputeStatus.NotDisputed,
        0,
        false
      );
      emitMarketEent(tokenId, true);
    }

    // function _transfer(
    //     address from,
    //     address to,
    //     uint256 tokenId
    // ) internal override(ERC721) {
    //     require(lockedNFTs[tokenId] != true, "NFT is locked");
    //     super._transfer(from, to, tokenId);
    // }

    function deActivate(uint256 tokenId) public whenNotPaused nonReentrant {
      require(msg.sender == idToMarketItem[tokenId].seller, NFTMarketplaceData.INV);
      require(idToMarketItem[tokenId].status == MarketItemStatus.Available, NFTMarketplaceData.INV);
      idToMarketItem[tokenId].status = MarketItemStatus.Cancelled;
      emitMarketEent(tokenId, false);
    }

    function activate(uint256 tokenId, uint256 price, uint256 expiresAt, AllowlistStatus alStatus) public whenNotPaused nonReentrant {
      require(msg.sender == idToMarketItem[tokenId].seller, NFTMarketplaceData.INV);
      require(idToMarketItem[tokenId].status == MarketItemStatus.Cancelled, NFTMarketplaceData.INV);
      require(expiresAt > block.timestamp, NFTMarketplaceData.EXP);
      // isNotExpiredRequired(tokenId);
      idToMarketItem[tokenId].status = MarketItemStatus.Available;
      if (price > 0) {
        idToMarketItem[tokenId].price = price;
      }
      if (expiresAt > 0) {
        idToMarketItem[tokenId].expiresAt = expiresAt;
      }
      idToMarketItem[tokenId].alStatus = alStatus;
      emitMarketEent(tokenId, false);
    }

    function buyListing(uint256 tokenId) public whenNotPaused nonReentrant payable {
      require(idToMarketItem[tokenId].price == msg.value, NFTMarketplaceData.INV);
      require(idToMarketItem[tokenId].status == MarketItemStatus.Available, NFTMarketplaceData.INV);
      require(idToMarketItem[tokenId].seller != msg.sender, NFTMarketplaceData.INV);
      isNotExpiredRequired(tokenId);

      idToMarketItem[tokenId].buyer = payable(msg.sender);
      idToMarketItem[tokenId].status = MarketItemStatus.Sold;
      _itemsSold.increment();
      _transfer(ownerOf(tokenId), payable(msg.sender), tokenId);
      // lockedNFTs[tokenId] = true;
      emitMarketEent(tokenId, false);
    }

    function listForResale(uint256 tokenId, uint256 price, uint256 expiresAt, AllowlistStatus alStatus) public whenNotPaused nonReentrant {
      address owner = ownerOf(tokenId);
      require(price >= 0, NFTMarketplaceData.LT);
      require(owner == msg.sender, NFTMarketplaceData.INV);
      require(idToMarketItem[tokenId].status == MarketItemStatus.Sold, NFTMarketplaceData.INV);
      isNotExpiredRequired(tokenId);
      
      _transfer(ownerOf(tokenId), payable(contractAddress), tokenId);
      
      _resaleCounter.increment();
      idToBuyerHistory[tokenId].push(BuyerHistory(
        idToMarketItem[tokenId].seller,
        idToMarketItem[tokenId].buyer,
        idToMarketItem[tokenId].alStatus,
        _resaleCounter.current(),
        block.timestamp,
        idToMarketItem[tokenId].expiresAt,
        idToMarketItem[tokenId].price,
        false
      ));
      idToMarketItem[tokenId].alStatus = alStatus;
      idToMarketItem[tokenId].status = MarketItemStatus.Available;
      idToMarketItem[tokenId].price = price;
      idToMarketItem[tokenId].expiresAt = expiresAt;
      idToMarketItem[tokenId].seller = payable(msg.sender);
      idToMarketItem[tokenId].buyer = payable(address(0));

      emitMarketEent(tokenId, false);
    }

    function redeem(uint256 tokenId) public whenNotPaused nonReentrant {
      bool isValid = false;
      if (idToMarketItem[tokenId].status == MarketItemStatus.Cancelled) {
        isValid = true;
      }
      if (idToMarketItem[tokenId].status == MarketItemStatus.Sold) {
        isValid = true;
      }
      require(currentBuyer(tokenId) == msg.sender,NFTMarketplaceData.INV);
      require(isValid == true , NFTMarketplaceData.INV);
      _transfer(ownerOf(tokenId), payable(contractAddress), tokenId);
      idToMarketItem[tokenId].status = MarketItemStatus.Redeemed;
      emitMarketEent(tokenId, false);
    }

    function updatePrice(uint256 tokenId, uint256 price) public whenNotPaused nonReentrant {
      require(msg.sender == idToMarketItem[tokenId].seller, NFTMarketplaceData.INV);
      require(idToMarketItem[tokenId].status == MarketItemStatus.Available, NFTMarketplaceData.INV);
      require(price >= 0, NFTMarketplaceData.LT);
      require(idToMarketItem[tokenId].price != price, NFTMarketplaceData.INV);
      isNotExpiredRequired(tokenId);
      idToMarketItem[tokenId].price = price;
      emitMarketEent(tokenId, false);
    }

    function updateExpiry(uint256 tokenId, uint256 expiresAt) public whenNotPaused nonReentrant {
      require(msg.sender == idToMarketItem[tokenId].seller, NFTMarketplaceData.INV);
      require(idToMarketItem[tokenId].status == MarketItemStatus.Available, NFTMarketplaceData.INV);
      require(idToMarketItem[tokenId].expiresAt != expiresAt, NFTMarketplaceData.EXP);
      // TODO: check if mint date is in the past
      
      idToMarketItem[tokenId].expiresAt = expiresAt;
      emitMarketEent(tokenId, false);
    }

    function updateAlStatus(uint256 tokenId, AllowlistStatus alStatus) public whenNotPaused nonReentrant {
      require(msg.sender == idToMarketItem[tokenId].seller, NFTMarketplaceData.INV);
      require(idToMarketItem[tokenId].status == MarketItemStatus.Available, NFTMarketplaceData.INV);
      require(idToMarketItem[tokenId].alStatus != alStatus, NFTMarketplaceData.INV);
      // TODO: check if mint date is in the past

      require(idToMarketItem[tokenId].alStatus == AllowlistStatus.Raffle, NFTMarketplaceData.INV);

      require(alStatus == AllowlistStatus.Unsuccessfull || alStatus == AllowlistStatus.Confirmed, NFTMarketplaceData.INV);

      idToMarketItem[tokenId].alStatus = alStatus;
      continueAllowList(tokenId);
      emitMarketEent(tokenId, false);
    }

    function confirmMintSucceeded(uint256 tokenId) public whenNotPaused nonReentrant {
      address owner = ownerOf(tokenId);
      bool isValid = false;
      if (idToMarketItem[tokenId].status == MarketItemStatus.Sold || idToMarketItem[tokenId].status == MarketItemStatus.Redeemed) {
        require(msg.sender == idToMarketItem[tokenId].buyer, NFTMarketplaceData.INV);
        isValid = true;
      }

      if (idToMarketItem[tokenId].status == MarketItemStatus.Available) {
        require(msg.sender == idToMarketItem[tokenId].seller, NFTMarketplaceData.INV);
        isValid = true;
      }

      require(isValid == true , NFTMarketplaceData.INV);

      if (idToMarketItem[tokenId].mintDate == 0) {
        idToMarketItem[tokenId].mintDate = block.timestamp;
      }

      if (owner != contractAddress) {
        _transfer(owner, payable(contractAddress), tokenId);
      }
      
      idToMarketItem[tokenId].status = MarketItemStatus.Minted;
      emitMarketEent(tokenId, false);
      burnIfConditionsAreMet(tokenId);
      
    }

    function claimFunds(uint256 tokenId, uint256 eventId) public whenNotPaused nonReentrant {
      _claimFunds(tokenId, eventId, appFee);
    }

    function claimFundsSpecial(uint256 tokenId, uint256 eventId, AppFeeParam memory appFeeParam) public whenNotPaused nonReentrant {
      uint _appFee = appFee;
      if (isAppFeeParamVerified(appFeeParam) && msg.sender == appFeeParam.a) {
        _appFee = appFeeParam.fee;
      }
      _claimFunds(tokenId, eventId, _appFee);
    }

    function raiseDispute(uint256 tokenId) public whenNotPaused nonReentrant {
      address raisedBy = msg.sender;
      require(currentBuyer(tokenId)  == raisedBy, NFTMarketplaceData.INV);
      require(getMintDate(tokenId) > 0, NFTMarketplaceData.INV);
      require(isGracePeriodOver(tokenId) == false, NFTMarketplaceData.INV);

      idToMarketItem[tokenId].status = MarketItemStatus.Disputed;

      bool shouldContinue = continueAllowList(tokenId);
      if (shouldContinue) {
        idToMarketItem[tokenId].disputeStatus = DisputeStatus.InProgress;
      }
      emitMarketEent(tokenId, false);
    }    

    function removeDispute(uint256 tokenId, DisputeStatus disputeStatus) public onlyOwner nonReentrant {
      require(idToMarketItem[tokenId].status == MarketItemStatus.Disputed, NFTMarketplaceData.INV);
      require(idToMarketItem[tokenId].disputeStatus == DisputeStatus.InProgress, NFTMarketplaceData.INV);

      idToMarketItem[tokenId].disputeStatus = disputeStatus;
      emitMarketEent(tokenId, false);
    }

    function claimRefund(uint256 tokenId, uint256 eventId) public whenNotPaused nonReentrant {
      require(isBuyerIncludingHistory(tokenId, msg.sender), NFTMarketplaceData.INV);
      require(
        idToMarketItem[tokenId].disputeStatus == DisputeStatus.SellerError || idToMarketItem[tokenId].disputeStatus == DisputeStatus.NoFault
      , NFTMarketplaceData.DNR);

      if (idToMarketItem[tokenId].buyer == msg.sender && eventId <= 0) {
        require(idToMarketItem[tokenId].claimed == false, NFTMarketplaceData.UC);
        idToMarketItem[tokenId].claimed = true;
        payable(idToMarketItem[tokenId].buyer).transfer(idToMarketItem[tokenId].price);
      } else {
        uint arrayLength = idToBuyerHistory[tokenId].length;
        for (uint i=0; i<arrayLength; i++) {
            if (idToBuyerHistory[tokenId][i].buyer == msg.sender) {
              if (eventId > 0 && idToBuyerHistory[tokenId][i].eventId != eventId) {
                continue;
              }
              require(idToBuyerHistory[tokenId][i].claimed == false, NFTMarketplaceData.UC);
              idToBuyerHistory[tokenId][i].claimed = true;
              idToBuyerHistory[tokenId][i].eventDate = block.timestamp;
              payable(idToBuyerHistory[tokenId][i].buyer).transfer(idToBuyerHistory[tokenId][i].price);
              break;
            }
        }
      }
      burnIfConditionsAreMet(tokenId);
      emitMarketEent(tokenId, false);
    }

    function setMintDate(ProjectMintDate[] memory mintDates) public onlyOwner {
      for (uint i=0; i<mintDates.length; i++) {
        projectIdToMintDate[mintDates[i].projectId] = mintDates[i].mintDate;
      }
    }

    function setAppFee(uint256 appFeePercentage) public onlyOwner {
      appFee = appFeePercentage;
    }

    function setGracePeriod(uint256 newGracePeriodInSeconds) public onlyOwner {
      gracePeriodInSeconds = newGracePeriodInSeconds;
    }

    function updateVerifiedAddress(address _newVerifiedAddress) public onlyOwner {
      verifiedAddr = _newVerifiedAddress;
    }

    // ------------------- //
    function getSales(uint256 tokenId) public view returns (SaleEventData[] memory sales) {
      uint256 totalSales = totalSaleCount(tokenId);
      SaleEventData[] memory salesData = new SaleEventData[](totalSales);
      uint iterCount = 0;
      if (hasCurrentBuyer(tokenId) == true) {
        salesData[iterCount] = SaleEventData(
          idToMarketItem[tokenId].seller,
          0
        );
        iterCount++;
      }

      uint arrayLength = idToBuyerHistory[tokenId].length;
