// SPDX-License-Identifier: MIT
pragma solidity 0.8.13;

import "@openzeppelin/contracts-upgradeable/token/ERC721/ERC721Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC721/extensions/ERC721EnumerableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC721/extensions/ERC721URIStorageUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/security/PausableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/access/AccessControlUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC721/extensions/ERC721BurnableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "@openzeppelin/contracts-upgradeable/utils/CountersUpgradeable.sol";
import "./BaseToken.sol";
import "./Versioned.sol";
import "../Validator.sol";

import "hardhat/console.sol";

import "./Utils.sol";


contract BaseCitiNFT is Initializable, ERC721Upgradeable, ERC721EnumerableUpgradeable, ERC721URIStorageUpgradeable, PausableUpgradeable, AccessControlUpgradeable, ERC721BurnableUpgradeable, Versioned {
    using CountersUpgradeable for CountersUpgradeable.Counter;

    bytes32 public constant PAUSER_ROLE = keccak256("PAUSER_ROLE");
    bytes32 public constant MINTER_ROLE = keccak256("MINTER_ROLE");
    CountersUpgradeable.Counter private _tokenIdCounter;

    bool private isDevContract;
    BaseToken private _token;

    address private _verifierAddress;

    mapping(uint256 => uint256) private _tokenNonces;
    mapping(bytes32 => uint256) private _hashNonces;

    // the dummy owner is not used for anything meaningful in the contract
    // it's only stored so that the contract ownership can be claimed at opensea
    address private _dummyOwner;
    
    // see https://docs.opensea.io/docs/contract-level-metadata
    string private _contractURI;

    string private _baseTokenURI;

    Validator private _validator;

    event TokenURIUpdated(uint256 indexed tokenId, string oldURI, string newURI);
    event TokenContractUpdated(BaseToken indexed oldContract, BaseToken indexed newContract);
    event ValidatorContractUpdated(Validator indexed oldContract, Validator indexed newContract);
    event TokenNonceUpdated(uint256 indexed tokenId, uint256 oldNonce, uint256 newNonce);
    event TokenUpgradedWithDetails(uint256 indexed tokenId, uint256 tokenNonce, string details, Validator validatorContract);
    event TokenMintedWithDetails(uint256 indexed tokenId, address indexed to, address indexed from, string details, Validator validatorContract);
    event TokenIdCounterOverriden(uint256 oldCounter, uint256 newCounter);
    event DummyOwnerUpdated(address oldOwner, address newOwner);
    event BaseTokenURIUpdated(string oldURI, string newURI);

    function __BaseCitiNFT_init(string memory _name, string memory _symbol, bool _isDevContract) internal onlyInitializing {
        
        // Do not allow deploying the dev contract to mainnets
        require(!_isDevContract || !Utils.isMainnet());
        isDevContract = _isDevContract;

        __ERC721_init(_name, _symbol);
        __ERC721Enumerable_init();
        __ERC721URIStorage_init();
        __Pausable_init();
        __AccessControl_init();
        __ERC721Burnable_init();

        _grantRole(DEFAULT_ADMIN_ROLE, msg.sender);
        _grantRole(PAUSER_ROLE, msg.sender);
        _grantRole(MINTER_ROLE, msg.sender);
    }

    function _baseURI() internal view override returns (string memory) {
        return _baseTokenURI;
    }

    function pause() public onlyRole(PAUSER_ROLE) {
        _pause();
    }

    function unpause() public onlyRole(PAUSER_ROLE) {
        _unpause();
    }

    function safeMint(address to, string calldata uri) public onlyRole(MINTER_ROLE) returns (uint256) {
        return _doSafeMint(to, uri);
    }

    function _beforeTokenTransfer(address from, address to, uint256 tokenId)
        internal
        whenNotPaused
        override(ERC721Upgradeable, ERC721EnumerableUpgradeable)
    {
        super._beforeTokenTransfer(from, to, tokenId);
    }

    // The following functions are overrides required by Solidity.

    function _burn(uint256 tokenId)
        internal
        override(ERC721Upgradeable, ERC721URIStorageUpgradeable)
    {
        super._burn(tokenId);
    }

    function tokenURI(uint256 tokenId)
        public
        view
        override(ERC721Upgradeable, ERC721URIStorageUpgradeable)
        returns (string memory)
    {
        if (bytes(_baseTokenURI).length > 0) {
            return ERC721Upgradeable.tokenURI(tokenId);
        } else {
            return ERC721URIStorageUpgradeable.tokenURI(tokenId);
        }
    }

    function supportsInterface(bytes4 interfaceId)
        public
        view
        override(ERC721Upgradeable, ERC721EnumerableUpgradeable, AccessControlUpgradeable)
        returns (bool)
    {
        return super.supportsInterface(interfaceId);
    }

    function _doSafeMint(address to, string calldata uri) private returns (uint256) {
        uint256 tokenId = _tokenIdCounter.current();
        _tokenIdCounter.increment();
        _safeMint(to, tokenId);
        _setTokenURI(tokenId, uri);
        return tokenId;
    }

    //====================== DG contract extensions ======================
    function tokenURIOfOwnerByIndex(address account, uint256 index) public view returns (uint256, string memory) {
        uint256 tokenID = tokenOfOwnerByIndex(account, index);
        string memory uri = tokenURI(tokenID);
        return (tokenID, uri);
    }

    function tokenContract() public view returns (BaseToken) {
        return _token;
    }

    function adminSetTokenContract(BaseToken token) public onlyRole(DEFAULT_ADMIN_ROLE) whenNotPaused {
        require(token != BaseToken(address(0)), "adminSetTokenContract: invalid token contract");
        BaseToken oldTokenContract = _token;
        _token = token;
        emit TokenContractUpdated(oldTokenContract, token);
    }

    function adminSetValidatorContract(Validator validator) public onlyRole(DEFAULT_ADMIN_ROLE) whenNotPaused {
        require(validator != Validator(address(0)), "adminSetValidatorContract: invalid validator contract");
        Validator oldContract = _validator;
        _validator = validator;
        emit ValidatorContractUpdated(oldContract, validator);
    }

    function validatorContract() public view returns (Validator) {
        return _validator;
    }

    function getTokenNonce(uint256 tokenId) public view returns (uint256) {
        return _tokenNonces[tokenId];
    }

    function upgradeWithDetails(uint256 tokenId, string calldata details, 
                                uint256 tokenNonce, uint256 numTokensToBurn, 
                                bytes memory signature) public whenNotPaused whenHasValidator {
        require(tokenNonce == _tokenNonces[tokenId], "upgradeWithDetails: invalid nonce");
        require(_isApprovedOrOwner(msg.sender, tokenId), "upgradeWithDetails: caller is not owner nor approved");

        bytes32 messageHash = keccak256(abi.encodePacked(tokenId, details, msg.sender, tokenNonce, numTokensToBurn));
        bool verified = _validator.verifySignature(abi.encodePacked(messageHash), signature);
        require(verified, "upgradeWithDetails: invalid signature");

        if (numTokensToBurn > 0) {
            require(_token != BaseToken(address(0)), "upgradeWithDetails: token contract is null");
            _token.burnFrom(msg.sender, numTokensToBurn);
        }
        
        _tokenNonces[tokenId] = tokenNonce + 1;
        emit TokenNonceUpdated(tokenId, tokenNonce, tokenNonce + 1);
        emit TokenUpgradedWithDetails(tokenId, tokenNonce, details, _validator);
    }    
    
    function mintWithDetails(address to, string calldata details, 
                             uint256 numTokensToBurn, string calldata uri, 
                             bytes memory signature) public whenNotPaused whenHasValidator returns (uint256) {
        bytes32 messageHash = keccak256(abi.encodePacked(to, details, msg.sender, numTokensToBurn));
        require(_hashNonces[messageHash] == 0, "mintWithDetails: hash already used");
        
        bool verified = _validator.verifySignature(abi.encodePacked(messageHash), signature);
        require(verified, "mintWithDetails: invalid signature");

        if (numTokensToBurn > 0) {
            require(_token != BaseToken(address(0)), "mintWithDetails: token contract is null");
            _token.burnFrom(msg.sender, numTokensToBurn);
        }

        uint256 tokenId = _doSafeMint(to, uri);
        _hashNonces[messageHash] = 1;
        emit TokenMintedWithDetails(tokenId, to, msg.sender, details, _validator);
        return tokenId;
    }

    function adminMigrateOwnership(uint256[] calldata tokenIds, address[] calldata owners) public onlyRole(DEFAULT_ADMIN_ROLE) whenNotPaused {
        require(tokenIds.length == owners.length, "adminMigrateOwnership: tokenId/owner mismatch");
        for (uint index = 0; index < tokenIds.length; ++index) {
            _mint(owners[index], tokenIds[index]);            
        }
    }

    function adminMigrateOwnershipWithUri(uint256[] calldata tokenIds, address[] calldata owners, string[] calldata uris) public onlyRole(DEFAULT_ADMIN_ROLE) whenNotPaused {
        require(tokenIds.length == owners.length, "adminMigrateOwnershipWithUri: tokenId/owner mismatch");
        require(tokenIds.length == uris.length, "adminMigrateOwnershipWithUri: tokenId/uri mismatch");
        for (uint index = 0; index < tokenIds.length; ++index) {
            _mint(owners[index], tokenIds[index]);
            _setTokenURI(tokenIds[index], uris[index]);
        }
    }

    function currentTokenIdCounter() public view returns (uint256) {
        return _tokenIdCounter.current();
    }

    function adminOverrideTokenIdCounter(uint256 newCounter) public onlyRole(DEFAULT_ADMIN_ROLE) whenNotPaused {
        require(newCounter >= _tokenIdCounter.current(), "adminOverrideTokenIdCounter: new counter is too small");
        uint256 oldCounter = _tokenIdCounter.current();
        _tokenIdCounter._value = newCounter;
        emit TokenIdCounterOverriden(oldCounter, newCounter);
    }

    function owner() public view returns (address) {
        return _dummyOwner;
    }

    function adminSetDummyOwner(address dummyOwner) public onlyRole(DEFAULT_ADMIN_ROLE) whenNotPaused {
        address oldOwner = _dummyOwner;
        _dummyOwner = dummyOwner;
        emit DummyOwnerUpdated(oldOwner, dummyOwner);
    }

    function contractURI() public view returns (string memory) {
        return _contractURI;
    }

    function adminSetContractURI(string calldata newContractURI) public onlyRole(DEFAULT_ADMIN_ROLE) whenNotPaused {
        _contractURI = newContractURI;
    }
    
    function baseTokenURI() public view returns (string memory) {
        return _baseTokenURI;
    }

    function adminSetBaseTokenURI(string calldata newBaseTokenURI) public onlyRole(DEFAULT_ADMIN_ROLE) whenNotPaused {
        string memory oldURI = _baseTokenURI;
        _baseTokenURI = newBaseTokenURI;
        emit BaseTokenURIUpdated(oldURI, newBaseTokenURI);
    }

    modifier whenHasValidator() {
        require(_validator != Validator(address(0)), "invalid validator contract");
        _;
    }

    //====================== Dev Features ======================
    modifier devMode() {
        require(isDevContract, "The contract is not in dev mode!");
        _;
    }

    function devMint(address to, string calldata uri) public devMode returns (uint256) {
        return _doSafeMint(to, uri);
    }

    function devMint2(address to, string calldata uri, uint256 numTokensToBurn) public devMode returns (uint256) {
        if (numTokensToBurn > 0) {
            require(_token != BaseToken(address(0)), "devMint2: token contract is null");
            _token.burnFrom(msg.sender, numTokensToBurn);
        }
        return _doSafeMint(to, uri);
    }

    function devMintWithTokenId(address to, string calldata uri, uint256 tokenId) public devMode returns (uint256) {
        _safeMint(to, tokenId);
        _setTokenURI(tokenId, uri);
        return tokenId;
    }
    
    function devUpdateTokenURI(uint256 tokenId, string calldata newURI) public devMode {
        require(_exists(tokenId), "devUpdateTokenURI: token doesn't exist");
        string memory oldURI = tokenURI(tokenId);
        _setTokenURI(tokenId, newURI);
        emit TokenURIUpdated(tokenId, oldURI, newURI);
    }
}

