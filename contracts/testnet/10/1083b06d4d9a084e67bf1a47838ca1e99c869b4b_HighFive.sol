pragma solidity 0.8.21;

interface IHighFive {
    event LetterSent(
        address sender,
        address recipient,
        uint256 lid,
        uint256 timestamp
    );

    event NewSequenceInitiated(
        uint256 seqId,
        address initiator,
        uint256 timestamp
    );
    event MaxSeqNumUpdated(uint256 maxSeqNum);
    event NumOfRecipientsUpdated(uint256 numOfRecipients);
    event SeqCreationFeeFactorUpdated(uint256 factor);
    event LetterCreated(uint256 lid, uint256 depth);
    event LetterUpdated(uint256 lid, uint256 depth, uint256 newDepth);
    event Modelock1Toggled(bool enabled);
    event Modelock2Toggled(bool enabled);
    event LetterPropagated(uint256 lid, address caller, uint256 timestamp);
    event BalanceThresholdSet(uint256 balanceThreshold);

    struct Mailbox {
        uint256 index; // a pointer to the first unactivated letter
        uint256[5] ids; // an array of letters by their ids
    }

    struct Letter {
        uint256 depth; // depth in the letter squence
        address[5] beneficiaries; // the beneficiaries in the letter
    }

    /**
     * @notice Initializes a sequence of letters.
     */
    function initNewSequence(
        address[5] memory _beneficiaries,
        address[] memory _recipients
    ) external payable;

    /**
     * @notice Sends the first letter of the calling account to
     * five randomly chosen wallets.
     */
    function propagate() external payable;

    /**
     * @notice Sends the first letter of the calling account to
     * an array of provided wallets.
     */
    function propagate(address[] memory) external payable;

    /**
     * @notice Sends the first n letter of the calling account to
     * five randomly chosen wallets.
     */
    function propagate(uint256 n) external payable;

    /**
     * @notice Adds accounts to the pool of receivers.
     */
    function addToPool(address[] memory _accounts) external;

    /**
     * @notice Sets the number of recipients to send a letter to in one shot.
     */
    function setNumOfRecipients(uint256 num) external;

    /**
     * @notice Sets the sequence creation factor `_factor`.
     */
    function setSeqCreationFeeFactor(uint256 _factor) external;

    /**
     * @notice Sets the balance threshold.
     */
    function setBalanceThreshold(uint256 _balanceThreshold) external;

    /**
     * @notice Toggles the lock status for Mode 1.
     */
    function toggleModelock1() external;

    /**
     * @notice Toggles the lock status for Mode 2.
     */
    function toggleModelock2() external;

    /**
     * @notice Returns the size of pool of receivers.
     */
    function getPoolSize() external view returns (uint256);

    /**
     * @notice Returns the index of an account.
     */
    function getIndex(address account) external view returns (uint256);

    /**
     * @notice Returns the next letterId.
     */
    function getNextLetterId() external view returns (uint256);

    /**
     * @notice Returns the letter info for an account at a given index.
     */
    function getLetterInfo(
        address account,
        uint256 index
    ) external view returns (Letter memory);

    /**
     * @notice Returns the current ETH price returned from Chainlink oracle.
     */
    function getETHPrice() external view returns (uint256);

    /**
     * @notice Returns the unit ETH amount for each recipient in the letter.
     */
    function getETHAmount() external view returns (uint256);

    /**
     * @notice Returns the sequence id.
     */
    function getSeqId() external view returns (uint256);

    /**
     * @notice Returns the depth of a given letter.
     */
    function getLetterDepth(uint256 tokenId) external view returns (uint256);
}

