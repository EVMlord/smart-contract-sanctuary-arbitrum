pragma solidity ^0.8.9;

import "@openzeppelin/contracts/token/ERC20/ERC20.sol";
import "@openzeppelin/contracts/access/Ownable.sol";

contract A3SToken is ERC20, Ownable {
    uint256 public initial_supply;

    mapping(address => bool) public governors;

    event Mint(address to, uint256 amount);
    event Burn(address account, uint256 amount);
    event UpdateGovernors(address newGovernor, bool status);

    modifier ONLY_GOV() {
        require(governors[msg.sender], "A3S: caller is not governor");
        _;
    }

    constructor(
        uint256 _initial_supply,
        string memory _name,
        string memory _symbol
    ) ERC20(_name, _symbol) {
        initial_supply = _initial_supply;
        _mint(msg.sender, initial_supply);
        emit Mint(msg.sender, initial_supply);
    }

    //ONLY Governors addresss could mint
    function mint(address to, uint256 amount) public ONLY_GOV {
        _mint(to, amount);
        emit Mint(to, amount);
    }

    //ONLY Owner Could Burn
    function burn(address account, uint256 amount) public onlyOwner {
        _burn(account, amount);
        emit Burn(account, amount);
    }

    //Update governors
    function updateGovernors(address newGovernor, bool status) public onlyOwner {
        governors[newGovernor] = status;
        emit UpdateGovernors(newGovernor, status);
    }
}

