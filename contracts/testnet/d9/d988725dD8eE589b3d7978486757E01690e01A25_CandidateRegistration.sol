pragma solidity ^0.8.18;

import {IVotingStorage} from "./IVotingStorage.sol";

interface ICandidateRegistration {
    /**
     * @dev Nomination state enumeration.
     */
    enum NominationState {
        SUBMITED,
        REVIEWING,
        REVOKED,
        APPROVE
    }
    /**
     * @dev Candidate information structure.
     */
    struct CandidateInfo {
        uint age;
        string name;
        string citizenship;
        string educationalQualifications;
        string partyNomination;
        string partyMembership;
        string nominationForm;
        string courtReport;
        uint lastCourtCase; // in year
    }

    /**
     * @dev Party nomination information structure.
     */
    struct PartyNomination {
        uint partyCount;
        CandidateInfo candidate1;
        CandidateInfo candidate2;
        string partyName;
        string partyCard;
        NominationState nominationState;
    }

    /**
     * @dev Approved Candidates information structure.
     */
    struct ApproveCanditates {
        uint id;
        string candidate1;
        string candidate2;
        string partyName;
        string partyCard;
        uint voteCount;
    }

    // mapping(uint => mapping(uint => GovernorshipNomination))
    //     private governorshipNominationPerState;

    function getPartyNomination(
        uint index
    ) external view returns (PartyNomination memory);

    function getApprovedNomination(
        uint index
    ) external view returns (ApproveCanditates memory);

    function getNominationCount() external view returns (uint);

    function getNominationState(
        uint nominationId
    ) external view returns (NominationState);

    function getCandidatePerState(
        uint stateId,
        uint candidateId
    ) external view returns (ApproveCanditates memory);

    function incrementVoteCount(uint candidateId, uint stateId) external;

    function getApproveCandidateCount() external view returns (uint);
}

