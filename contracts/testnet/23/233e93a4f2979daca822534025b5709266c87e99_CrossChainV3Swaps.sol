pragma solidity 0.8.7;

import {TransferHelper} from "uniswap/v3-periphery/contracts/libraries/TransferHelper.sol";
import {ISwapRouter} from "uniswap/v3-periphery/contracts/interfaces/ISwapRouter.sol";
import {PoolAddress} from "uniswap/v3-periphery/contracts/libraries/PoolAddress.sol";
import {IWETH9} from "../../../interfaces/IWETH9.sol";
import {IERC20} from "openzeppelin/contracts/token/ERC20/IERC20.sol";
import {SafeERC20} from "openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";

abstract contract UniswapAdapter {
    using SafeERC20 for IERC20;
    ISwapRouter public swapRouter;

    struct SingleHopInfo {
        address to;
        address tokenIn;
        uint256 qty;
        address tokenOut;
        uint24 poolFee;
        uint256 amountOut;
        uint160 sqrt;
    }

    struct MultiHopInfo {
        address to;
        address tokenIn;
        address token1;
        address tokenOut;
        uint24 poolFee0;
        uint24 poolFee1;
        uint256 qty;
        uint256 amountOutMin;
    }

    function swapExactInputSingle(SingleHopInfo memory singleHop)
        internal
        returns (uint256 amountOut)
    {
        TransferHelper.safeTransferFrom(
            singleHop.tokenIn,
            msg.sender,
            address(this),
            singleHop.qty
        );

        TransferHelper.safeApprove(
            singleHop.tokenIn,
            address(swapRouter),
            singleHop.qty
        );

        ISwapRouter.ExactInputSingleParams memory params = ISwapRouter
            .ExactInputSingleParams({
                tokenIn: singleHop.tokenIn,
                tokenOut: singleHop.tokenOut,
                fee: singleHop.poolFee,
                recipient: singleHop.to,
                deadline: block.timestamp,
                amountIn: singleHop.qty,
                amountOutMinimum: singleHop.amountOut,
                sqrtPriceLimitX96: singleHop.sqrt
            });

        // The call to `exactInputSingle` executes the swap.
        amountOut = swapRouter.exactInputSingle(params);
    }

    function swapExactInputMultihop(MultiHopInfo memory multiParams)
        internal
        returns (uint256 amountOut)
    {
        // Transfer `amountIn` of DAI to this contract.
        TransferHelper.safeTransferFrom(
            multiParams.tokenIn,
            msg.sender,
            address(this),
            multiParams.qty
        );

        // Approve the router to spend DAI.
        TransferHelper.safeApprove(
            multiParams.tokenIn,
            address(swapRouter),
            multiParams.qty
        );

        // Multiple pool swaps are encoded through bytes called a `path`. A path is a sequence of token addresses and poolFees that define the pools used in the swaps.
        // The format for pool encoding is (tokenIn, fee, tokenOut/tokenIn, fee, tokenOut) where tokenIn/tokenOut parameter is the shared token across the pools.
        // Since we are swapping DAI to USDC and then USDC to WETH9 the path encoding is (DAI, 0.3%, USDC, 0.3%, WETH9).
        ISwapRouter.ExactInputParams memory params = ISwapRouter
            .ExactInputParams({
                path: abi.encodePacked(
                    multiParams.tokenIn,
                    multiParams.poolFee0,
                    multiParams.token1,
                    multiParams.poolFee1,
                    multiParams.tokenOut
                ),
                recipient: multiParams.to,
                deadline: block.timestamp,
                amountIn: multiParams.qty,
                amountOutMinimum: multiParams.amountOutMin
            });

        // Executes the swap.
        amountOut = swapRouter.exactInput(params);
    }
}

