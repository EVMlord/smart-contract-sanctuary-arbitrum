contract TreasureZone is Initializable, EIP712Upgradeable, ZoneInterface, TreasureZoneAdmin {
    function initialize(address _signingAuthority) external initializer {
        __TreasureZoneAdmin_init();
        __EIP712_init("TreasureZone", "1.0");
        signingAuthority = _signingAuthority;
    }

    function validateOrder(ZoneParameters calldata _zoneParameters)
        external
        pure
        returns (bytes4 validOrderMagicValue)
    {
        // TODO: This needs to be fleshed out after some further work on the backend endpoints.
        validOrderMagicValue = bytes4(_zoneParameters.orderHash);
    }

    function getSeaportMetadata() external pure returns (string memory name, Schema[] memory schemas) {
        name = "TreasureZone";

        Schema[] memory emptySchemas;
        schemas = emptySchemas;
    }

    function supportsInterface(bytes4 _interfaceId)
        public
        view
        override(ZoneInterface, BaseUpgradeable)
        returns (bool)
    {
        return _interfaceId == type(ZoneInterface).interfaceId || super.supportsInterface(_interfaceId);
    }
}

